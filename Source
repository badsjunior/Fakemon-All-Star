//------------------------------------------SECTION:INCLUDES-------------------------------------------------------//
//									Keep it shorter than 1k lines!												   //
//-----------------------------------------------------------------------------------------------------------------//

Includes
*include move %id %key %name %type %category %class %shape %priority %crit %power %accuracy %moveCondID %moveCondNum %effect %fxPower %fxChance %fxCondID %fxCondNum %effect2 %fxPower2 %fxChance2 %fxCondID2 %fxCondNum2 %effect3 %fxPower3 %fxChance3 %fxCondID3 %fxCondNum3 %desc
	*aMOV[%key]
		name:[%name]
		desc:[?([%category]=-1)||<t>Poder: [%power]</t>]<t>[?([%category]=-1)|Estado|][?([%category]=0)|Físico|][?([%category]=1)|Especial|]</t><t>Precisão: [?([%accuracy]>=0)|[%accuracy]%|Garantida]</t></>[%desc]
		tags:activeMove type[%type]
		on select:
			do getname with tag:active
			log se prepara para usar <#ffcc00><b>[n:this]</b></#>...
			$activeMoveID = [%id]
			$activeMoveType = [%type]
			$activeMoveCategory = [%category]
			$activeMoveClass = [%class]
			$activeMoveShape = [%shape]
			$activeMovePriority = [%priority]
			$activeMoveCrit = [%crit]
			$activeMovePower = [%power]
			$activeMoveFxChance0 = [%accuracy]
			$activeMoveFxCondID0 = [%moveCondID]
			$activeMoveFxCondNum0 = [%moveCondNum]
			$activeMoveEffect1 = [%effect]
			$activeMoveFxPower1 = [%fxPower]
			$activeMoveFxChance1 = [%fxChance]
			$activeMoveFxCondID1 = [%fxCondID]
			$activeMoveFxCondNum1 = [%fxCondNum]
			$activeMoveEffect2 = [%effect2]
			$activeMoveFxPower2 = [%fxPower2]
			$activeMoveFxChance2 = [%fxChance2]
			$activeMoveFxCondID2 = [%fxCondID2]
			$activeMoveFxCondNum2 = [%fxCondNum2]
			$activeMoveEffect3 = [%effect3]
			$activeMoveFxPower3 = [%fxPower3]
			$activeMoveFxChance3 = [%fxChance3]
			$activeMoveFxCondID3 = [%fxCondID3]
			$activeMoveFxCondNum3 = [%fxCondNum3]
		end
	*eMOV[%key]
		name:[%name]
		tags:enemyMove hidden
		on select:
			do getname with tag:enemy
			log se prepara para usar <#ffcc00><b>[n:this]</b></#>...
			$enemyMoveID = [%id]
			$enemyMoveType = [%type]
			$enemyMoveCategory = [%category]
			$enemyMoveClass = [%class]
			$enemyMoveShape = [%shape]
			$enemyMovePriority = [%priority]
			$enemyMoveCrit = [%crit]
			$enemyMovePower = [%power]
			$enemyMoveFxChance0 = [%accuracy]
			$enemyMoveFxCondID0 = [%moveCondID]
			$enemyMoveFxCondNum0 = [%moveCondNum]
			$enemyMoveEffect1 = [%effect]
			$enemyMoveFxPower1 = [%fxPower]
			$enemyMoveFxChance1 = [%fxChance]
			$enemyMoveFxCondID1 = [%fxCondID]
			$enemyMoveFxCondNum1 = [%fxCondNum]
			$enemyMoveEffect2 = [%effect2]
			$enemyMoveFxPower2 = [%fxPower2]
			$enemyMoveFxChance2 = [%fxChance2]
			$enemyMoveFxCondID2 = [%fxCondID2]
			$enemyMoveFxCondNum2 = [%fxCondNum2]
			$enemyMoveEffect3 = [%effect3]
			$enemyMoveFxPower3 = [%fxPower3]
			$enemyMoveFxChance3 = [%fxChance3]
			$enemyMoveFxCondID3 = [%fxCondID3]
			$enemyMoveFxCondNum3 = [%fxCondNum3]
		end
*include battler %number %region %variation %name %sprite %baseHP %baseAtk %baseDfs %baseSpAtk %baseSpDfs %baseSpeed %pType %sType %ability %move1 %move2 %move3 %move4 %move2lvl %move3lvl %move4lvl %move1Up %move2Up %move3Up %move4Up %move1UpLvl %move2UpLvl %move3UpLvl %move4UpLvl
	*ATV[%number]from[%region]var[%variation]
		name:[%name]
		icon:[%sprite]
		no tooltip
		tag:active
		on click:
			do return with this
		end
		on return:
			if (have this)
				toast <#00cc00><b>[n:this]</b></#>, já basta, volte!
				log <#00cc00><b>[n:this]</b></#> retornou para a pokébola, mas os pokémon selvagens fugiram!
				lose this
			end
		end
		on earn:
			gain activePkm
			activeCurHP = round(activeCurHP)
			do reset with tag:modifier
			if (have enemyPkm) gain inBattle
			activeMaxHP = round((([%baseHP]*activeLvl)/50)+activeLvl+10)
			activeCurHP = activeMaxHP
			activeBaseAttack = ((([%baseAtk]*activeLvl)/50)+5)
			activeBaseDefense = ((([%baseDfs]*activeLvl)/50)+5)
			activeBaseSpAttack = ((([%baseSpAtk]*activeLvl)/50)+5)
			activeBaseSpDefense = ((([%baseSpDfs]*activeLvl)/50)+5)
			activeBaseSpeed = ((([%baseSpeed]*activeLvl)/50)+5)
			activePtype = [%pType]
			activeStype = [%sType]
			activeAbility = [%ability]
			if ([%move1UpLvl]>activeLvl) gain aMOV[%move1]
			if ((activeLvl>=[%move2lvl]) and ([%move2UpLvl]>activeLvl)) gain aMOV[%move2]
			if ((activeLvl>=[%move3lvl]) and ([%move3UpLvl]>activeLvl)) gain aMOV[%move3]
			if ((activeLvl>=[%move4lvl]) and ([%move4UpLvl]>activeLvl)) gain aMOV[%move4]
			if (activeLvl>=[%move1UpLvl]) gain aMOV[%move1Up]
			if (activeLvl>=[%move2UpLvl]) gain aMOV[%move2Up]
			if (activeLvl>=[%move3UpLvl]) gain aMOV[%move3Up]
			if (activeLvl>=[%move4UpLvl]) gain aMOV[%move4Up]
		end
		on lose:
			lose activePkm
			lose tag:activeMove
			lose tag:enemy
			lose inBattle
			do reset with tag:base
			activeMaxHP = 0
			activeCurHP = 0
		end
		on getname:
			if (have this) log <#00cc00><b>[n:this]</b></#>
		end
	*ENM[%number]from[%region]var[%variation]
		name:[%name]
		icon:[%sprite]
		tag:enemy
		desc:<//>HP: [enemyCurHP]/[enemyMaxHP] ([((enemyCurHP/enemyMaxHP)*100)]%) LVL: [enemyLvl]<//>[?(0.15>=(enemyCurHP/enemyMaxHP))|<#cc0000>|<#e6b800>][?((enemyCurHP/enemyMaxHP)>=0.4)|<#009900>|][?((enemyCurHP/enemyMaxHP)=1)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.95)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.9)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.85)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.8)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.75)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.7)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.65)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.6)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.55)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.5)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.45)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.4)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.35)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.3)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.25)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.2)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.15)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.1)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.05)|▓|░]</#><//>Attack: [enemyAttack]<//>Defense: [enemyDefense]<//>Special Attack: [enemySpAttack]<//>Special Defense: [enemySpDefense]<//>Speed: [enemySpeed]
		on click:
			if (1>pokeballs)
				toast Você <#ff3300>não tem pokébolas</#> para capturar!
			else if (chance((enemyMaxHP*5)/(enemyCurHP)))
				pokeball = (pokeballs-1)
				toast Você capturou <#00cc00>[n:this]</#> lvl [enemyLvl]!
				do capture with this
				lose this
			else
				pokeball = (pokeballs-1)
				toast Você falhou em capturar <#ff3300>[n:this]</#> e ele fugiu da batalha!
				lose this
			end
		end
		on earn:
			gain enemyPkm
			gain FND[%number]from[%region]var[%variation]
			do reset with tag:modifierEnemy
			enemyMaxHP = round((([%baseHP]*frandom(0.9,1.1)*enemyLvl)/50)+enemyLvl+10)
			enemyCurHP = enemyMaxHP
			enemyStatus = 0
			enemyBaseAttack = ((([%baseAtk]*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseDefense = ((([%baseDfs]*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpAttack = ((([%baseSpAtk]*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpDefense = ((([%baseSpDfs]*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpeed = ((([%baseSpeed]*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyPtype = [%pType]
			enemyStype = [%sType]
			enemyBerry = random(13)
			enemyAbility = [%ability]
		end
		on capture:
			CPT[%number]from[%region]var[%variation] = max(enemyLvl,CPT[%number]from[%region]var[%variation])
		end
		on iaplay:
			$movesLearned = 1
			if (enemyLvl>=[%move2lvl])
				$movesLearned = ($movesLearned + 1)
			end	
			if (enemyLvl>=[%move3lvl])
				$movesLearned = ($movesLearned + 1)
			end	
			if (enemyLvl>=[%move4lvl])
				$movesLearned = ($movesLearned + 1)
			end	
			$bestMove = random(1,$movesLearned)
			if ((have this) and ($bestMove=1) and ([%move1UpLvl]>enemyLvl)) do select with eMOV[%move1]
			else if ((have this) and ($bestMove=2) and ([%move2UpLvl]>enemyLvl)) do select with eMOV[%move2]
			else if ((have this) and ($bestMove=3) and ([%move3UpLvl]>enemyLvl)) do select with eMOV[%move3]
			else if ((have this) and ($bestMove=4) and ([%move4UpLvl]>enemyLvl)) do select with eMOV[%move4]
			else if ((have this) and ($bestMove=1) and (enemyLvl>=[%move1UpLvl])) do select with eMOV[%move1Up]
			else if ((have this) and ($bestMove=2) and (enemyLvl>=[%move2UpLvl])) do select with eMOV[%move2Up]
			else if ((have this) and ($bestMove=3) and (enemyLvl>=[%move3UpLvl])) do select with eMOV[%move3Up]
			else if ((have this) and ($bestMove=4) and (enemyLvl>=[%move4UpLvl])) do select with eMOV[%move4Up]
		end
		on faint:
			if (have this)
				$expEarned = (pow((enemyMaxHP + enemyBaseAttack + enemyBaseDefense + enemyBaseSpAttack + enemyBaseSpDefense + enemyBaseSpeed),2)/100)*pow(((2*enemyLvl)/(enemyLvl + activeLvl)),2)
				toast <#00cc00>Vitória!</#>
				log <#ff3300><b>[n:this]</b></#> foi derrotado, você ganhou [$expEarned] xp.
				gain $expEarned xp
				lose this
			end
		end
		on lose:
			lose enemyPkm
			lose inBattle
		end
		on getname:
			if (have this) log <#ff3300><b>[n:this]</b></#>
		end
*include activeMoveTypeAdvantage %type
	if ((activeCurHP>0) and ($auxMtype=[%type]))
		do type[%type]advantage with this
		$activePadvg = $auxAdvg
		$auxType = enemyStype
		do type[%type]advantage with this
	end
*include enemyMoveTypeAdvantage %type
	if ((enemyCurHP>0) and ($auxMtype=[%type]))
		do type[%type]advantage with this
		$enemyPadvg = $auxAdvg
		$auxType = activeStype
		do type[%type]advantage with this
	end
*include multipleHit %caster %target %hit
	if (($[%caster]MoveClass>=[%hit]) and ($numberOfHits>=[%hit]))
		$[%caster]Aim = chance($[%caster]MoveFxChance0*[%caster]AccuracyMod*[%target]EvasionMod)
		echo hit [%hit]: Variable Update >> $[%caster]Aim [?$[%caster]Aim|✔️|❌]. [$[%caster]MoveFxChance0*[%caster]AccuracyMod*[%target]EvasionMod]% chance, from $[%caster]MoveFxChance0 ([$[%caster]MoveFxChance0]) * [%caster]AccuracyMod ([[%caster]AccuracyMod*100]) * [%target]EvasionMod ([[%target]EvasionMod*100]])
	end
	if (($[%caster]Aim=0) and ($numberOfHits>=[%hit]) and ($[%caster]MoveClass>=[%hit]))
		do getname with tag:[%caster]
		log errou o [%hit]º golpe!
		[%caster]MovesInARow = 0
		log <#3366ff>-----------------------------------</#>
	end
	if (([%caster]CurHP>0) and ([%target]CurHP>0) and ($[%caster]MoveClass>=[%hit]) and ($numberOfHits>=[%hit]) and ($[%caster]Aim = 1))
		do getname with tag:[%caster]
		log atacou pela [%hit]ª vez...
		$[%caster]Crit = 1
		$[%caster]RandomVar = frandom(0.85,1)
	end
	if (([%caster]CurHP>0) and ([%target]CurHP>0) and ($[%caster]MoveClass>=[%hit]) and ($numberOfHits>=[%hit]) and (chance($[%caster]MoveCrit)) and ($[%caster]Aim = 1))
		$[%caster]Crit = 1.5
		log ... e realizou um golpe crítico!
	end
	if (([%caster]CurHP>0) and ([%target]CurHP>0) and ($[%caster]MoveClass>=[%hit]) and ($numberOfHits>=[%hit]) and ($[%caster]Aim = 1))
		$[%caster]DamageMods = $[%caster]TypeAdvg*$[%caster]RandomVar*$[%caster]STAB*$[%caster]Crit*$[%caster]StatusMod*$[%caster]ShapeMod
		$[%caster]EffectiveDamage = round(min(([%target]CurHP + $[%caster]NoKill),($[%caster]RawDamage*$[%caster]DamageMods)))
		lose $[%caster]EffectiveDamage [%target]CurHP
		echo hit [%hit]: Variable Update >> $[%caster]EffectiveDamage = [$[%caster]EffectiveDamage]. Capped at [%target]CurHP ([[%target]CurHP]) - $[%caster]NoKill ([$[%caster]NoKill]). From $[%caster]RawDamage and $[%caster]DamageMods = [$[%caster]DamageMods*100]%, which is $[%caster]TypeAdvg ([$[%caster]TypeAdvg*100]) * $[%caster]RandomVar ([$[%caster]RandomVar*100]) * $[%caster]STAB ([$[%caster]STAB*100]) * $[%caster]Crit ([$[%caster]Crit*100]) * $[%caster]StatusMod ([$[%caster]StatusMod*100]) * $[%caster]ShapeMod ([$[%caster]ShapeMod*100])
		do getname with tag:[%target]
		log tomou [$[%caster]EffectiveDamage] dano!
		log <#3366ff>-----------------------------------</#>
	end
*include defModByStage %stage %mod
	if (activeAttackStage=[%stage]) activeAttackMod = [%mod]
	if (activeDefenseStage=[%stage]) activeDefenseMod = [%mod]
	if (activeSpAttackStage=[%stage]) activeSpAttackMod = [%mod]
	if (activeSpDefenseStage=[%stage]) activeSpDefenseMod = [%mod]
	if (activeSpeedStage=[%stage])activeSpeedMod = [%mod]
	if (enemyAttackStage=[%stage]) enemyAttackMod = [%mod]
	if (enemyDefenseStage=[%stage]) enemyDefenseMod = [%mod]
	if (enemySpAttackStage=[%stage]) enemySpAttackMod = [%mod]
	if (enemySpDefenseStage=[%stage]) enemySpDefenseMod = [%mod]
	if (enemySpeedStage=[%stage]) enemySpeedMod = [%mod]
*include defAcEvModByStage %stage %mod
	if (activeAccuracyStage=[%stage]) activeAccuracyMod = [%mod]
	if (activeEvasionStage=(-[%stage])) activeEvasionMod = [%mod]
	if (enemyAccuracyStage=[%stage]) enemyAccuracyMod = [%mod]
	if (enemyEvasionStage=(-[%stage])) enemyEvasionMod = [%mod]
*include triggerPeriodicAbilities %caster %target
	$half = random(1,2)
	if (([%caster]Ability=1) and ($half=1))
		[%caster]Berry = 8
		do getname with tag:[%caster]
		log ativou sua habilidade <#00bfff><b>Frutífero</b></#> e brotou uma <#e65c00><b>[n:BER8]</b></#>!
		log <#3366ff>-----------------------------------</#>
	end
	if (([%caster]Ability=2) and ($[%caster]Aim=0))
		[%caster]AccuracyStage=min(([%caster]AccuracyStage + 1),6)
		[%caster]DefenseStage=min(([%caster]DefenseStage + 1),6)
		[%caster]SpeedStage=min(([%caster]SpeedStage + 1),6)
		do getname with tag:[%caster]
		log ativou sua habilidade <#00bfff><b>Levar na Esportiva</b></#> e aumentou sua precisão, defesa e velocidade!
		log <#3366ff>-----------------------------------</#>
	end
	if (([%caster]Ability=3) and ($[%caster]MoveShape=10))
		gain ceil(min(([%caster]MaxHP/8),([%caster]MaxHP-[%caster]CurHP))) [%caster]CurHP
		do getname with tag:[%caster]
		log ativou sua habilidade <#00bfff><b>Tagarela</b></#> e se curou em 12,5% de sua vida máxima!
		log <#3366ff>-----------------------------------</#>
	end
	if (([%caster]Ability=4) and ([%caster]Status=-3))
		[%caster]Status=0
		[%caster]StatusTimer=0
		do getname with tag:[%caster]
		log ativou sua habilidade <#00bfff><b>Insômnia</b></#> e acordou!
		log <#3366ff>-----------------------------------</#>
	end
	if (([%caster]Ability=5) and ($[%caster]Aim=1) and !(([%caster]Status=0) or ([%caster]Status=-1)) and ([%target]Status=0))
		[%target]Status=[%caster]Status
		do getname with tag:[%caster]
		log ativou sua habilidade <#00bfff><b>Vetor de Doenças</b></#> e proliferou seus status para o adversário!
		log <#3366ff>-----------------------------------</#>
	end
*include capturedNoIcon %name %number %region %variation %evoLvl %evoNumber
	*CPT[%number]from[%region]var[%variation]
		name:[%name]
		on click:
			if (([%evoLvl]>0) and (this>=[%evoLvl]) and (CPT[%evoNumber]from[%region]var[%variation]=0))
				toast <#00cc00><b>[n:this]</b></#> evoluiu para <#00cc00><b>[n:CPT[%evoNumber]from[%region]var[%variation]]</b></#>!
				gain FND[%evoNumber]from[%region]var[%variation]
				hide FND[%evoNumber]from[%region]var[%variation]
				CPT[%evoNumber]from[%region]var[%variation] = this
				this = 0
			end
			if ((this>0) and (no activePkm) and (switches>=1))
				switch = (switch-1)
				activeLvl = this
				gain ATV[%number]from[%region]var[%variation]
				toast <#00cc00><b>[n:this]</b></#>, vai!
			end
		end
*include placeNoIcon %id %region %name %desc %common1Lvl %common1Range %common1PkmNum %common1PkmVar %common2Lvl %common2Range %common2PkmNum %common2PkmVar %common3Lvl %common3Range %common3PkmNum %common3PkmVar %uncommonLvl %uncommonRange %uncommonPkmNum %uncommonPkmVar %rareLvl %rareRange %rarePkmNum %rarePkmVar %exoticLvl %exoticRange %exoticPkmNum %exoticPkmVar %legendLvl %legendRange %legendPkmNum %legendPkmVar
    *place[%id]from[%region]
		name:[%name]
		desc:[%desc]
		icon:exploration[0,0]
		on common1:
			enemyLvl = ([%common1Lvl] + random(-[%common1Range],[%common1Range]))
			gain ENM[%common1PkmNum]from[%region]var[%common1PkmVar]
		end
		on common2:
			enemyLvl = ([%common2Lvl] + random(-[%common2Range],[%common2Range]))
			gain ENM[%common2PkmNum]from[%region]var[%common2PkmVar]
		end
		on common3:
			enemyLvl = ([%common3Lvl] + random(-[%common3Range],[%common3Range]))
			gain ENM[%common3PkmNum]from[%region]var[%common3PkmVar]
		end
		on uncommon:
			enemyLvl = ([%uncommonLvl] + random(-[%uncommonRange],[%uncommonRange]))
			gain ENM[%uncommonPkmNum]from[%region]var[%uncommonPkmVar]
		end
		on rare:
			enemyLvl = ([%rareLvl] + random(-[%rareRange],[%rareRange]))
			gain ENM[%rarePkmNum]from[%region]var[%rarePkmVar]
		end
		on exotic:
			enemyLvl = ([%exoticLvl] + random(-[%exoticRange],[%exoticRange]))
			gain ENM[%exoticPkmNum]from[%region]var[%exoticPkmVar]
		end
		on legend:
			enemyLvl = ([%legendLvl] + random(-[%legendRange],[%legendRange]))
			gain ENM[%legendPkmNum]from[%region]var[%legendPkmVar]
		end
*include moveDrainEffect %caster %number %fxNum
	if (($[%caster]MoveEffect[%fxNum]=[%number]) and (chance($[%caster]MoveFxChance[%fxNum])) and ($[%caster]Aim = 1) and ($[%caster]FxCondSuccess[%fxNum]=1))
		$[%caster]EffectiveHeal = ceil(min((($[%caster]EffectiveDamage/100)*$[%caster]MoveFxPower[%fxNum]),([%caster]MaxHP-[%caster]CurHP)))
		gain $[%caster]EffectiveHeal [%caster]CurHP
		echo Triggering $[%caster]MoveEffect[%fxNum] (#[%number]) with [$[%caster]MovePower[%fxNum]] power, ([$[%caster]MoveFxChance[%fxNum]]% chance)✔️ and condition #[$[%caster]MoveFxCondID[%fxNum]] satisfied ✔️. Attribute Update >> [%caster]CurHP = [[%caster]CurHP] (It was healed by [$[%caster]MoveFxPower[%fxNum]]% of [$[%caster]EffectiveDamage], the $[%caster]EffectiveDamage, capped at [[%caster]MaxHP-[%caster]CurHP], the [%caster]MaxHP-[%caster]CurHP)
		do getname with tag:[%caster]
		log drenou [$[%caster]EffectiveHeal] pontos de vida!
		log <#3366ff>-----------------------------------</#>
	end
*include moveRecoilEffect %caster %number %fxNum
	if (($[%caster]MoveEffect[%fxNum]=[%number]) and (chance($[%caster]MoveFxChance[%fxNum])) and ($[%caster]Aim = 1) and ($[%caster]FxCondSuccess[%fxNum]=1))
		$[%caster]EffectiveRecoil = ceil(min((($[%caster]EffectiveDamage/100)*$[%caster]MoveFxPower[%fxNum]),[%caster]CurHP))
		lose $[%caster]EffectiveRecoil [%caster]CurHP
		echo Triggering $[%caster]MoveEffect[%fxNum] (#[%number]) with [$[%caster]MovePower[%fxNum]] power, ([$[%caster]MoveFxChance[%fxNum]]% chance)✔️ and condition #[$[%caster]MoveFxCondID[%fxNum]] satisfied ✔️. Attribute Update >> [%caster]CurHP = [[%caster]CurHP] (It was damaged by [$[%caster]MoveFxPower[%fxNum]]% of [$[%caster]EffectiveDamage], the $[%caster]EffectiveDamage, capped at [[%caster]CurHP], the [%caster]CurHP)
		do getname with tag:[%caster]
		log também se feriu, tomando [$[%caster]EffectiveRecoil] dano!
		log <#3366ff>-----------------------------------</#>
	end
*include moveStatusEffect %caster %target %number %fxNum
	$timer[%fxNum]=1
	if ($[%caster]MoveFxPower[%fxNum]=(-3))
		$timer[%fxNum] = random(1,3)
	end
	if (($[%caster]MoveEffect[%fxNum]=[%number]) and (chance($[%caster]MoveFxChance[%fxNum])) and ($[%caster]Aim = 1) and ([%target]Status = 0) and ($[%caster]FxCondSuccess[%fxNum]=1))
		[%target]Status = $[%caster]MoveFxPower[%fxNum]
		[%target]StatusTimer = $timer[%fxNum]
		echo Triggering $[%caster]MoveEffect[%fxNum] (#[%number]) with [$[%caster]MovePower[%fxNum]] power, ([$[%caster]MoveFxChance[%fxNum]]% chance)✔️ and condition #[$[%caster]MoveFxCondID[%fxNum]] satisfied ✔️. Attribute Update >> [%target]Status = [[%target]Status]; [%target]StatusTimer =[[%target]StatusTimer]
		do getname with tag:[%target]
		log [?([%target]Status=1)|foi envenenado!|][?([%target]Status=2)|foi intoxicado!|][?([%target]Status=3)|está em chamas!|][?([%target]Status=(-2))|foi paralisado!|][?([%target]Status=(-3))|caiu no sono!|][?([%target]Status=(-4))|foi congelado!|][?([%target]Status=(-5))|ficou hesitante!|][?([%target]Status=(-6))|se prepara para fugir!|][?([%target]Status=(-7))|ficou confuso!|][?([%target]Status=(-8))|ficou fadigado!|]
		log <#3366ff>-----------------------------------</#>
	end
*include moveHealStatusEffect %caster %target %number %fxNum
	if ($[%caster]MoveFxPower[%fxNum]=0)
		$[%caster]MoveFxPower[%fxNum] = [%target]Status
	end
	if (($[%caster]MoveEffect[%fxNum]=[%number]) and (chance($[%caster]MoveFxChance[%fxNum])) and ($[%caster]Aim = 1) and ([%target]Status=$[%caster]MoveFxPower[%fxNum]) and ($[%caster]FxCondSuccess[%fxNum]=1))
		[%target]Status = 0
		echo Triggering $[%caster]MoveEffect[%fxNum] (#[%number]) with [$[%caster]MovePower[%fxNum]] power, ([$[%caster]MoveFxChance[%fxNum]]% chance)✔️ and condition #[$[%caster]MoveFxCondID[%fxNum]] satisfied ✔️. Attribute Update >> [%target]Status = [[%target]Status]
		do getname with tag:[%target]
		log foi curado de sua condição!
		log <#3366ff>-----------------------------------</#>
	end
*include moveAllDebuffEffect %caster %target %number %fxNum
	if (($[%caster]MoveEffect[%fxNum]=[%number]) and (chance($[%caster]MoveFxChance[%fxNum])) and ($[%caster]Aim = 1) and ($[%caster]FxCondSuccess[%fxNum]=1))
		[%target]AttackStage = max((-6),([%target]AttackStage-$[%caster]MoveFxPower[%fxNum]))
		[%target]DefenseStage = max((-6),([%target]DefenseStage-$[%caster]MoveFxPower[%fxNum]))
		[%target]SpAttackStage = max((-6),([%target]SpAttackStage-$[%caster]MoveFxPower[%fxNum]))
		[%target]SpDefenseStage = max((-6),([%target]SpDefenseStage-$[%caster]MoveFxPower[%fxNum]))
		[%target]SpeedStage = max((-6),([%target]SpeedStage-$[%caster]MoveFxPower[%fxNum]))
		echo Triggering $[%caster]MoveEffect[%fxNum] (#[%number]) with [$[%caster]MovePower[%fxNum]] power, ([$[%caster]MoveFxChance[%fxNum]]% chance)✔️ and condition #[$[%caster]MoveFxCondID[%fxNum]] satisfied ✔️. Attribute Update >> [%target]AttackStage  = [[%target]AttackStage]; [%target]DefenseStage = [[%target]DefenseStage]; [%target]SpAttackStage = [[%target]SpAttackStage]; [%target]SpDefenseStage = [[%target]SpDefenseStage]; [%target]SpeedStage = [[%target]SpeedStage]
		do getname with tag:[%target]
		log teve todos os seus atributos reduzidos em [$[%caster]MoveFxPower[%fxNum]] estágio[?($[%caster]MoveFxPower[%fxNum]>1)|s|]!
		log <#3366ff>-----------------------------------</#>
	end
*include moveAllBuffEffect %caster %target %number %fxNum
	if (($[%caster]MoveEffect[%fxNum]=[%number]) and (chance($[%caster]MoveFxChance[%fxNum])) and ($[%caster]Aim = 1) and ($[%caster]FxCondSuccess[%fxNum]=1))
		[%target]AttackStage = min(6,([%target]AttackStage+$[%caster]MoveFxPower[%fxNum]))
		[%target]DefenseStage = min(6,([%target]DefenseStage+$[%caster]MoveFxPower[%fxNum]))
		[%target]SpAttackStage = min(6,([%target]SpAttackStage+$[%caster]MoveFxPower[%fxNum]))
		[%target]SpDefenseStage = min(6,([%target]SpDefenseStage+$[%caster]MoveFxPower[%fxNum]))
		[%target]SpeedStage = min(6,([%target]SpeedStage+$[%caster]MoveFxPower[%fxNum]))
		echo Triggering $[%caster]MoveEffect[%fxNum] (#[%number]) with [$[%caster]MovePower[%fxNum]] power, ([$[%caster]MoveFxChance[%fxNum]]% chance)✔️ and condition #[$[%caster]MoveFxCondID[%fxNum]] satisfied ✔️. Attribute Update >> [%target]AttackStage  = [[%target]AttackStage]; [%target]DefenseStage = [[%target]DefenseStage]; [%target]SpAttackStage = [[%target]SpAttackStage]; [%target]SpDefenseStage = [[%target]SpDefenseStage]; [%target]SpeedStage = [[%target]SpeedStage]
		do getname with tag:[%target]
