		$[%caster]MoveFxPower = [%target]Ability
	end
	if (($[%caster]MoveEffect2=[%number]) and ($[%caster]MoveFxPower2=(-1)))
		$[%caster]MoveFxPower2 = [%caster]Ability
	end
	if (($[%caster]MoveEffect2=[%number]) and ($[%caster]MoveFxPower2=(-2)))
		$[%caster]MoveFxPower2 = [%target]Ability
	end
