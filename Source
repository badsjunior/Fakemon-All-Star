		[include hpHealingBerry %name="Cítrus" %heal="100"]
		desc:Ao ser consumida, cura <b>100%</b> da vida.
	
	*TEMPLATE

//----------------------------------------------Includes----------------------------------------------------------------//

Includes
*include move %id %key %priority %name %crit %type %isSpecial %power %class %effect %fxPower %fxChance %effect2 %fxPower2 %fxChance2 %fxCondID %fxCondNum %accuracy %desc
	*aMOV[%key]
		name:[%name]
		desc:<t>Poder: [%power]</t><t>[?([%isSpecial]=0)|Físico|Especial]</t><t>Precisão: [?([%accuracy]>=0)|[%accuracy]%|Garantida]</t></>[%desc]
		tags:activeMove type[%type]
		on select:
			do getname with tag:active
			log se prepara para usar <#ffcc00><b>[n:this]</b></#>...
			$activeMoveID = [%id]
			$activeMovePriority = [%priority]
			$activeMoveCrit = [%crit]
			$activeMoveType = [%type]
			$activeMoveIsSpecial = [%isSpecial]
			$activeMovePower = [%power]
			$activeMoveClass = [%class]
			$activeMoveEffect = [%effect]
			$activeMoveFxPower = [%fxPower]
			$activeMoveFxChance = [%fxChance]
			$activeMoveEffect2 = [%effect2]
			$activeMoveFxPower2 = [%fxPower2]
			$activeMoveFxChance2 = [%fxChance2]
			$activeMoveCondID = [%fxCondID]
			$activeMoveCondNum = [%fxCondNum]
			$activeMoveAccuracy = [%accuracy]
		end
	*eMOV[%key]
		name:[%name]
		tags:enemyMove hidden
		on select:
			do getname with tag:enemy
			log se prepara para usar <#ffcc00><b>[n:this]</b></#>...
			$enemyMoveID = [%id]
			$enemyMovePriority = [%priority]
			$enemyMoveCrit = [%crit]
			$enemyMoveType = [%type]
			$enemyMoveIsSpecial = [%isSpecial]
			$enemyMovePower = [%power]
			$enemyMoveClass = [%class]
			$enemyMoveEffect = [%effect]
			$enemyMoveFxPower = [%fxPower]
			$enemyMoveFxChance = [%fxChance]
			$enemyMoveEffect2 = [%effect2]
			$enemyMoveFxPower2 = [%fxPower2]
			$enemyMoveFxChance2 = [%fxChance2]
			$enemyMoveCondID = [%fxCondID]
			$enemyMoveCondNum = [%fxCondNum]
			$enemyMoveAccuracy = [%accuracy]
		end
*include nonDamagingMove %id %key %name %priority %type %isSelf %effect %fxPower %fxChance %effect2 %fxPower2 %fxChance2 %fxCondID %fxCondNum %accuracy %desc
	*aMOV[%key]
		name:[%name]
		desc:<t>Estado</t><t>Precisão: [?([%accuracy]>=0)|[%accuracy]%|Garantida]</t></>[%desc]
		tags:activeMove type[%type]
		on select:
			do getname with tag:active
			log se prepara para usar <#ffcc00><b>[n:this]</b></#>...
			$activeMoveID = [%id]
			$activeMovePriority = [%priority]
			$activeMoveType = [%type]
			$activeMoveClass = ((3*[%isSelf]) - 4)
			$activeMoveEffect = [%effect]
			$activeMoveFxPower = [%fxPower]
			$activeMoveFxChance = [%fxChance]
			$activeMoveEffect2 = [%effect2]
			$activeMoveFxPower2 = [%fxPower2]
			$activeMoveFxChance2 = [%fxChance2]
			$activeMoveCondID = [%fxCondID]
			$activeMoveCondNum = [%fxCondNum]
			$activeMoveAccuracy = [%accuracy]
		end
	*eMOV[%key]
		name:[%name]
		tags:enemyMove hidden
		on select:
			do getname with tag:enemy
			log se prepara para usar <#ffcc00><b>[n:this]</b></#>...
			$enemyMoveID = [%id]
			$enemyMovePriority = [%priority]
			$enemyMoveType = [%type]
			$enemyMoveClass = ((3*[%isSelf]) - 4)
			$enemyMoveEffect = [%effect]
			$enemyMoveFxPower = [%fxPower]
			$enemyMoveFxChance = [%fxChance]
			$enemyMoveEffect2 = [%effect2]
			$enemyMoveFxPower2 = [%fxPower2]
			$enemyMoveFxChance2 = [%fxChance2]
			$enemyMoveCondID = [%fxCondID]
			$enemyMoveCondNum = [%fxCondNum]
			$enemyMoveAccuracy = [%accuracy]
		end
*include battler %number %region %variation %name %sprite %baseHP %baseAtk %baseDfs %baseSpAtk %baseSpDfs %baseSpeed %pType %sType %ability %move1 %move2 %move3 %move4 %move2lvl %move3lvl %move4lvl %move1Up %move2Up %move3Up %move4Up %move1UpLvl %move2UpLvl %move3UpLvl %move4UpLvl
	*ATV[%number]from[%region]var[%variation]
		name:[%name]
		icon:[%sprite]
		no tooltip
		tag:active
		on click:
			do return with this
		end
		on return:
			if (have this)
				toast <#00cc00><b>[n:this]</b></#>, já basta, volte!
				log <#00cc00><b>[n:this]</b></#> retornou para a pokébola, mas os pokémon selvagens fugiram!
				lose this
			end
		end
		on earn:
			gain activePkm
			activeCurHP = round(activeCurHP)
			do reset with tag:modifier
			if (have enemyPkm) gain inBattle
			activeMaxHP = round((([%baseHP]*activeLvl)/50)+activeLvl+10)
			activeCurHP = activeMaxHP
			activeBaseAttack = ((([%baseAtk]*activeLvl)/50)+5)
			activeBaseDefense = ((([%baseDfs]*activeLvl)/50)+5)
			activeBaseSpAttack = ((([%baseSpAtk]*activeLvl)/50)+5)
			activeBaseSpDefense = ((([%baseSpDfs]*activeLvl)/50)+5)
			activeBaseSpeed = ((([%baseSpeed]*activeLvl)/50)+5)
			activePtype = [%pType]
			activeStype = [%sType]
			activeAbility = [%ability]
			if ([%move1UpLvl]>activeLvl) gain aMOV[%move1]
			if ((activeLvl>=[%move2lvl]) and ([%move2UpLvl]>activeLvl)) gain aMOV[%move2]
			if ((activeLvl>=[%move3lvl]) and ([%move3UpLvl]>activeLvl)) gain aMOV[%move3]
			if ((activeLvl>=[%move4lvl]) and ([%move4UpLvl]>activeLvl)) gain aMOV[%move4]
			if (activeLvl>=[%move1UpLvl]) gain aMOV[%move1Up]
			if (activeLvl>=[%move2UpLvl]) gain aMOV[%move2Up]
			if (activeLvl>=[%move3UpLvl]) gain aMOV[%move3Up]
			if (activeLvl>=[%move4UpLvl]) gain aMOV[%move4Up]
		end
		on lose:
			lose activePkm
			lose tag:activeMove
			lose tag:enemy
			lose inBattle
			do reset with tag:base
			activeMaxHP = 0
			activeCurHP = 0
		end
		on getname:
			if (have this) log <#00cc00><b>[n:this]</b></#>
		end
	*ENM[%number]from[%region]var[%variation]
		name:[%name]
		icon:[%sprite]
		tag:enemy
		desc:<//>HP: [enemyCurHP]/[enemyMaxHP] ([((enemyCurHP/enemyMaxHP)*100)]%) LVL: [enemyLvl]<//>[?(0.15>=(enemyCurHP/enemyMaxHP))|<#cc0000>|<#e6b800>][?((enemyCurHP/enemyMaxHP)>=0.4)|<#009900>|][?((enemyCurHP/enemyMaxHP)=1)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.95)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.9)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.85)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.8)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.75)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.7)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.65)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.6)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.55)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.5)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.45)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.4)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.35)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.3)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.25)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.2)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.15)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.1)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.05)|▓|░]</#><//>Attack: [enemyAttack]<//>Defense: [enemyDefense]<//>Special Attack: [enemySpAttack]<//>Special Defense: [enemySpDefense]<//>Speed: [enemySpeed]
		on click:
			if (1>pokeballs)
				toast Você <#ff3300>não tem pokébolas</#> para capturar!
			else if (chance((enemyMaxHP*5)/(enemyCurHP)))
				pokeball = (pokeballs-1)
				toast Você capturou <#00cc00>[n:this]</#> lvl [enemyLvl]!
				do capture with this
				lose this
			else
				pokeball = (pokeballs-1)
				toast Você falhou em capturar <#ff3300>[n:this]</#> e ele fugiu da batalha!
				lose this
			end
		end
		on earn:
			gain enemyPkm
			gain FND[%number]from[%region]var[%variation]
			do reset with tag:modifierEnemy
			enemyMaxHP = round((([%baseHP]*frandom(0.9,1.1)*enemyLvl)/50)+enemyLvl+10)
			enemyCurHP = enemyMaxHP
			enemyBaseAttack = ((([%baseAtk]*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseDefense = ((([%baseDfs]*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpAttack = ((([%baseSpAtk]*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpDefense = ((([%baseSpDfs]*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpeed = ((([%baseSpeed]*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyPtype = [%pType]
			enemyStype = [%sType]
			enemyBerry = random(13)
			enemyAbility = [%ability]
		end
		on capture:
			CPT[%number]from[%region]var[%variation] = max(enemyLvl,CPT[%number]from[%region]var[%variation])
		end
		on iaplay:
			$movesLearned = 1
			if (enemyLvl>=[%move2lvl])
				$movesLearned = ($movesLearned + 1)
			end	
			if (enemyLvl>=[%move3lvl])
				$movesLearned = ($movesLearned + 1)
			end	
			if (enemyLvl>=[%move4lvl])
				$movesLearned = ($movesLearned + 1)
			end	
			$bestMove = random(1,$movesLearned)
			if ((have this) and ($bestMove=1) and ([%move1UpLvl]>enemyLvl)) do select with eMOV[%move1]
			else if ((have this) and ($bestMove=2) and ([%move2UpLvl]>enemyLvl)) do select with eMOV[%move2]
			else if ((have this) and ($bestMove=3) and ([%move3UpLvl]>enemyLvl)) do select with eMOV[%move3]
			else if ((have this) and ($bestMove=4) and ([%move4UpLvl]>enemyLvl)) do select with eMOV[%move4]
			else if ((have this) and ($bestMove=1) and (enemyLvl>=[%move1UpLvl])) do select with eMOV[%move1Up]
			else if ((have this) and ($bestMove=2) and (enemyLvl>=[%move2UpLvl])) do select with eMOV[%move2Up]
			else if ((have this) and ($bestMove=3) and (enemyLvl>=[%move3UpLvl])) do select with eMOV[%move3Up]
			else if ((have this) and ($bestMove=4) and (enemyLvl>=[%move4UpLvl])) do select with eMOV[%move4Up]
		end
		on faint:
			if (have this)
				$expEarned = (pow((enemyMaxHP + enemyBaseAttack + enemyBaseDefense + enemyBaseSpAttack + enemyBaseSpDefense + enemyBaseSpeed),2)/100)*pow(((2*enemyLvl)/(enemyLvl + activeLvl)),2)
				toast <#00cc00>Vitória!</#>
				log <#ff3300><b>[n:this]</b></#> foi derrotado, você ganhou [$expEarned] xp.
				gain $expEarned xp
				lose this
			end
		end
		on lose:
			lose enemyPkm
			lose inBattle
		end
		on getname:
			if (have this) log <#ff3300><b>[n:this]</b></#>
		end
*include activeMoveTypeAdvantage %type
	if ((activeCurHP>0) and ($auxMtype=[%type]))
		do type[%type]advantage with this
		$activePadvg = $auxAdvg
		$auxType = enemyStype
		do type[%type]advantage with this
	end
*include enemyMoveTypeAdvantage %type
	if ((enemyCurHP>0) and ($auxMtype=[%type]))
		do type[%type]advantage with this
		$enemyPadvg = $auxAdvg
		$auxType = activeStype
		do type[%type]advantage with this
	end
*include multipleHit %caster %target %hit
	$[%caster]Aim = chance(($[%caster]MoveAccuracy*[%caster]AccuracyMod*[%target]EvasionMod)*100)
	if ($[%caster]Aim = 0)
		do getname with tag:[%caster]
		log errou o golpe!
		[%caster]MovesInARow = 0
	end
	if (([%caster]CurHP>0) and ([%target]CurHP>0) and ($[%caster]MoveClassM>=[%hit]) and ($numberOfHits>=[%hit]) and ($[%caster]Aim = 1))
		do getname with tag:[%caster]
		log atacou pela [%hit]ª vez...
		$[%caster]Crit = 1
		$[%caster]RandomVar = frandom(0.85,1)
	end
	if (([%caster]CurHP>0) and ([%target]CurHP>0) and ($[%caster]MoveClassM>=[%hit]) and ($numberOfHits>=[%hit]) and (chance($[%caster]MoveCrit)) and ($[%caster]Aim = 1))
		$[%caster]Crit = 1.5
		log ... e realizou um golpe crítico!
	end
	if (([%caster]CurHP>0) and ([%target]CurHP>0) and ($[%caster]MoveClassM>=[%hit]) and ($numberOfHits>=[%hit]) and ($[%caster]Aim = 1))
		$[%caster]DamageMods = $[%caster]TypeAdvg*$[%caster]RandomVar*$[%caster]STAB*$[%caster]Crit*$[%caster]StatusMod
		$[%caster]EffectiveDamage = round(min([%target]CurHP,($[%caster]RawDamage*$[%caster]DamageMods)))
		lose $[%caster]EffectiveDamage [%target]CurHP
		do getname with tag:[%target]
		log tomou [$[%caster]EffectiveDamage] dano!
		log <#3366ff>-----------------------------------</#>
	end
*include defModByStage %stage %mod
	if (activeAttackStage=[%stage]) activeAttackMod = [%mod]
	if (activeDefenseStage=[%stage]) activeDefenseMod = [%mod]
	if (activeSpAttackStage=[%stage]) activeSpAttackMod = [%mod]
	if (activeSpDefenseStage=[%stage]) activeSpDefenseMod = [%mod]
	if (activeSpeedStage=[%stage])activeSpeedMod = [%mod]
	if (enemyAttackStage=[%stage]) enemyAttackMod = [%mod]
	if (enemyDefenseStage=[%stage]) enemyDefenseMod = [%mod]
	if (enemySpAttackStage=[%stage]) enemySpAttackMod = [%mod]
	if (enemySpDefenseStage=[%stage]) enemySpDefenseMod = [%mod]
	if (enemySpeedStage=[%stage]) enemySpeedMod = [%mod]
*include defAcEvModByStage %stage %mod
	if (activeAccuracyStage=[%stage]) activeAccuracyMod = [%mod]
	if (activeEvasionStage=(-[%stage])) activeEvasionMod = [%mod]
	if (enemyAccuracyStage=[%stage]) enemyAccuracyMod = [%mod]
	if (enemyEvasionStage=(-[%stage])) enemyEvasionMod = [%mod]
*include triggerPeriodicAbilities %caster %target
	$half = random(1,2)
	if (([%caster]Ability=1) and ($half=1))
		[%caster]Berry = 8
		do getname with tag:[%caster]
		log ativou sua habilidade <#00bfff><b>Frutífero</b></#> e brotou uma <#e65c00><b>[n:BER8]</b></#>!
		log <#3366ff>-----------------------------------</#>
	end
*include capturedNoIcon %name %number %region %variation %evoLvl %evoNumber
	*CPT[%number]from[%region]var[%variation]
		name:[%name]
		on click:
			if (([%evoLvl]>0) and (this>=[%evoLvl]) and (CPT[%evoNumber]from[%region]var[%variation]=0))
				toast <#00cc00><b>[n:this]</b></#> evoluiu para <#00cc00><b>[n:CPT[%evoNumber]from[%region]var[%variation]]</b></#>!
				gain FND[%evoNumber]from[%region]var[%variation]
				hide FND[%evoNumber]from[%region]var[%variation]
				CPT[%evoNumber]from[%region]var[%variation] = this
				this = 0
			end
			if ((this>0) and (no activePkm) and (switches>=1))
				switch = (switch-1)
				activeLvl = this
				gain ATV[%number]from[%region]var[%variation]
				toast <#00cc00><b>[n:this]</b></#>, vai!
			end
		end
*include placeNoIcon %id %region %name %desc %common1Lvl %common1Range %common1PkmNum %common1PkmVar %common2Lvl %common2Range %common2PkmNum %common2PkmVar %common3Lvl %common3Range %common3PkmNum %common3PkmVar %uncommonLvl %uncommonRange %uncommonPkmNum %uncommonPkmVar %rareLvl %rareRange %rarePkmNum %rarePkmVar %exoticLvl %exoticRange %exoticPkmNum %exoticPkmVar %legendLvl %legendRange %legendPkmNum %legendPkmVar
    *place[%id]from[%region]
		name:[%name]
		desc:[%desc]
		icon:exploration[0,0]
		on common1:
			enemyLvl = ([%common1Lvl] + random(-[%common1Range],[%common1Range]))
			gain ENM[%common1PkmNum]from[%region]var[%common1PkmVar]
		end
		on common2:
			enemyLvl = ([%common2Lvl] + random(-[%common2Range],[%common2Range]))
			gain ENM[%common2PkmNum]from[%region]var[%common2PkmVar]
		end
		on common3:
			enemyLvl = ([%common3Lvl] + random(-[%common3Range],[%common3Range]))
			gain ENM[%common3PkmNum]from[%region]var[%common3PkmVar]
		end
		on uncommon:
			enemyLvl = ([%uncommonLvl] + random(-[%uncommonRange],[%uncommonRange]))
			gain ENM[%uncommonPkmNum]from[%region]var[%uncommonPkmVar]
		end
		on rare:
			enemyLvl = ([%rareLvl] + random(-[%rareRange],[%rareRange]))
			gain ENM[%rarePkmNum]from[%region]var[%rarePkmVar]
		end
		on exotic:
			enemyLvl = ([%exoticLvl] + random(-[%exoticRange],[%exoticRange]))
			gain ENM[%exoticPkmNum]from[%region]var[%exoticPkmVar]
		end
		on legend:
			enemyLvl = ([%legendLvl] + random(-[%legendRange],[%legendRange]))
			gain ENM[%legendPkmNum]from[%region]var[%legendPkmVar]
		end
*include moveDrainEffect %caster %number
	if (($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance)) and ($[%caster]Aim = 1))
		$[%caster]EffectiveHeal = ceil(min((($[%caster]EffectiveDamage/100)*$[%caster]MoveFxPower),([%caster]MaxHP-[%caster]CurHP)))
		gain $[%caster]EffectiveHeal [%caster]CurHP
		do getname with tag:[%caster]
		log drenou [$[%caster]EffectiveHeal] pontos de vida!
		log <#3366ff>-----------------------------------</#>
	end
