//----------------------------------------------Includes----------------------------------------------------------------//

Includes
*include move %id %key %priority %name %crit %type %isSpecial %power %class %effect %fxChance %accuracy
	*aMOV[%key]
		name:[%name]
		tags:activeMove type[%type]
		on select:
			do getname with tag:active
			log se prepara para usar <#ffcc00><b>[n:this]</b></#>...
			$activeMoveID = [%id]
			$activeMovePriority = [%priority]
			$activeMoveCrit = [%crit]
			$activeMoveType = [%type]
			$activeMoveIsSpecial = [%isSpecial]
			$activeMovePower = [%power]
			$activeMoveClass = [%class]
			$activeMoveEffect = [%effect]
			$activeMoveFxChance = [%fxChance]
			$activeMoveAccuracy = [%accuracy]
		end
	*eMOV[%key]
		name:[%name]
		tags:enemyMove hidden
		on select:
			do getname with tag:enemy
			log se prepara para usar <#ffcc00><b>[n:this]</b></#>...
			$enemyMoveID = [%id]
			$enemyMovePriority = [%priority]
			$enemyMoveCrit = [%crit]
			$enemyMoveType = [%type]
			$enemyMoveIsSpecial = [%isSpecial]
			$enemyMovePower = [%power]
			$enemyMoveClass = [%class]
			$enemyMoveEffect = [%effect]
			$enemyMoveFxChance = [%fxChance]
			$enemyMoveAccuracy = [%accuracy]
		end

*include extraFxMove %id %key %priority %name %crit %type %isSpecial %power %class %effect %fxChance %effect2 %fxChance2 %accuracy
	*aMOV[%key]
		name:[%name]
		tags:activeMove type[%type]
		on select:
			do getname with tag:active
			log se prepara para usar <#ffcc00><b>[n:this]</b></#>...
			$activeMoveID = [%id]
			$activeMovePriority = [%priority]
			$activeMoveCrit = [%crit]
			$activeMoveType = [%type]
			$activeMoveIsSpecial = [%isSpecial]
			$activeMovePower = [%power]
			$activeMoveClass = [%class]
			$activeMoveEffect = [%effect]
			$activeMoveFxChance = [%fxChance]
			$activeMoveEffect2 = [%effect2]
			$activeMoveFxChance2 = [%fxChance2]
			$activeMoveAccuracy = [%accuracy]
		end
	*eMOV[%key]
		name:[%name]
		tags:enemyMove hidden
		on select:
			do getname with tag:enemy
			log se prepara para usar <#ffcc00><b>[n:this]</b></#>...
			$enemyMoveID = [%id]
			$enemyMovePriority = [%priority]
			$enemyMoveCrit = [%crit]
			$enemyMoveType = [%type]
			$enemyMoveIsSpecial = [%isSpecial]
			$enemyMovePower = [%power]
			$enemyMoveClass = [%class]
			$enemyMoveEffect = [%effect]
			$enemyMoveFxChance = [%fxChance]
			$enemyMoveEffect2 = [%effect2]
			$enemyMoveFxChance2 = [%fxChance2]
			$enemyMoveAccuracy = [%accuracy]
		end

*include nonDamagingMove %id %key %priority %name %type %effect %fxChance %effect2 %fxChance2 %accuracy
