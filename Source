		do getname with tag:[%target]
		log mudou de posição, agora ele está [?[%target]Position=0|no campo de batalha|][?[%target]Position=1|voando no céu|][?[%target]Position=2|cavando debaixo da terra|][?[%target]Position=3|nadando debaixo d'água|]!
		log <#3366ff>-----------------------------------</#>
	end
*include moveMetaEffects %caster %fxNum
	if (($[%caster]MoveEffect[%fxNum]=13) or ($[%caster]MoveEffect[%fxNum]=23))
		$[%caster]MoveEffect[%fxNum] = $[%caster]MoveEffect[%fxNum] - random(1,8)
	end
	if (($[%caster]MoveEffect[%fxNum]=-13) or ($[%caster]MoveEffect[%fxNum]=-23))
		$[%caster]MoveEffect[%fxNum] = $[%caster]MoveEffect[%fxNum] + random(1,8)
	end
*include moveFxIf %caster %number %fxNum
	if (($[%caster]MoveEffect[%fxNum]=[%number]) and (chance($[%caster]MoveFxChance[%fxNum])) and ($[%caster]FxCondSuccess[%fxNum]=1))
		echo Triggering $[%caster]MoveEffect[%fxNum] (#[%number]) with [$[%caster]MovePower[%fxNum]] power, ([$[%caster]MoveFxChance[%fxNum]]% chance)✔️ and condition #[$[%caster]MoveFxCondID[%fxNum]] satisfied ✔️.
*include moveCoreCopy %caster %target
		$[%caster]MoveID = $[%target]MoveID
		$[%caster]MoveType = $[%target]MoveType
		$[%caster]MoveCategory = $[%target]MoveCategory
		$[%caster]MoveClass = $[%target]MoveClass
		$[%caster]MoveShape = $[%target]MoveShape
		$[%caster]MovePriority = $[%target]MovePriority
		$[%caster]MoveCrit = $[%target]MoveCrit
		$[%caster]MovePower = $[%target]MovePower
		$[%caster]MoveFxChance0 = $[%target]MoveFxChance0
		$[%caster]MoveFxCondID0 = $[%target]MoveFxCondID0
		$[%caster]MoveFxCondNum0 = $[%target]MoveFxCondNum0
*include moveFxCopy %caster %target %fxNum
		$[%caster]MoveEffect[%fxNum] = $[%target]MoveEffect[%fxNum]
		$[%caster]MoveFxPower[%fxNum] = $[%target]MoveFxPower[%fxNum]
		$[%caster]MoveFxChance[%fxNum] = $[%target]MoveFxChance[%fxNum]
		$[%caster]MoveFxCondID[%fxNum] = $[%target]MoveFxCondID[%fxNum]
		$[%caster]MoveFxCondNum[%fxNum] = $[%target]MoveFxCondNum[%fxNum]
*include checkConditions %caster %target %number
	$[%caster]FxCondSuccess[%number] = 0
	if ($[%caster]MoveFxCondID[%number] = 0)
		$[%caster]FxCondSuccess[%number] = 1
	end
	if (($[%caster]MoveFxCondID[%number] = 1) and ([%caster]Berry>0))
		$[%caster]FxCondSuccess[%number] = 1
	end
	if (($[%caster]MoveFxCondID[%number] = 2) and ([%caster]Berry=$[%caster]MoveFxCondNum[%number]))
		$[%caster]FxCondSuccess[%number] = 1
	end
	if (($[%caster]MoveFxCondID[%number] = 3) and ([%caster]LastMoveShape=$[%caster]MoveFxCondNum[%number]))
		$[%caster]FxCondSuccess[%number] = 1
	end
	if (($[%caster]MoveFxCondID[%number] = 4) and ($[%target]MoveShape=$[%caster]MoveFxCondNum[%number]))
		$[%caster]FxCondSuccess[%number] = 1
	end
	echo [%caster] Move Effect [%number] Condition (#[$[%caster]MoveFxCondID[%number]]) was [?$[%caster]FxCondSuccess[%number]=0|❌ not|✔️] satisfied.
*include onMove %caster %target
	$[%caster]STAB = 1
	$[%caster]Crit = 1
	$[%caster]StatusMod = 1
	$[%caster]ShapeMod = 1
	if ($[%caster]FxCondSuccess0=1)
		$[%caster]Aim = chance($[%caster]MoveFxChance0*[%caster]AccuracyMod*[%target]EvasionMod)
		echo Variable Update >> $[%caster]Aim [?$[%caster]Aim|✔️|❌]. [$[%caster]MoveFxChance0*[%caster]AccuracyMod*[%target]EvasionMod]% chance, from $[%caster]MoveFxChance0 ([$[%caster]MoveFxChance0]) * [%caster]AccuracyMod ([[%caster]AccuracyMod*100]) * [%target]EvasionMod ([[%target]EvasionMod*100]])
	end
	if ($protection=1)
		$[%caster]Aim = 0
		echo Variable Update >> $[%caster]Aim ❌. Enemy has Protection.
		do getname with tag:[%target]
		log está protegido!
	end
	if ($[%caster]MoveClass>=2)
		do getname with tag:[%caster]
		$numberOfHits = random(2,$[%caster]MoveClass)
		echo Variable Update >> $numberOfHits = [$numberOfHits]. Random between 2 and [$[%caster]MoveClass] ($[%caster]MoveClass).
		log ... irá atacar [$numberOfHits] vezes neste turno ...
		log <#3366ff>-----------------------------------</#>
	end
	if ($[%caster]Aim = 0)
		do getname with tag:[%caster]
		log errou o [?($[%caster]MoveClass>=2)|1º |]golpe!
		[%caster]MovesInARow = 0
		log <#3366ff>-----------------------------------</#>
	end
	if (($[%caster]MoveID=[%caster]LastMoveID) and ($[%caster]Aim = 1))
		[%caster]MovesInARow = ([%caster]MovesInARow+1)
	end
	if ((!($[%caster]MoveID=[%caster]LastMoveID)) and ($[%caster]Aim = 1))
		[%caster]MovesInARow = 1
	end
	echo Attribute Update >> [%caster]MovesInARow [?[%caster]MovesInARow=1|❌|✔️] = [[%caster]MovesInARow].
	if ((0>$[%caster]MoveClass) and ($[%caster]Aim = 1))
		do getname with tag:[%caster]
		log usou seu golpe ...
	end
	if (($[%caster]MoveClass>=0) and ($[%caster]Aim = 1))
		do getname with tag:[%caster]
		log atacou [?($[%caster]MoveClass>=2)|pela 1ª vez |]...
	end
	if (($[%caster]MoveClass=1) and ($[%caster]Aim = 1))
		$[%caster]MovePower = ($[%caster]MovePower*(min(5,[%caster]MovesInARow)))
		echo Variable Update >> $[%caster]MovePower = [$[%caster]MovePower]. Consecutive move empowerment.
		log ... com um poder acumulado de [[%caster]MovesInARow] golpes seguidos ...
	end
	echo Type Checking: [%caster]Ptype = [[%caster]Ptype]; [%caster]Stype = [[%caster]Stype]; $[%caster]Movetype = [$[%caster]Movetype]
	if ((($[%caster]MoveClass>=0) or ($[%caster]MoveClass=(-3))) and ([%caster]Ptype=$[%caster]Movetype) and ($[%caster]Aim = 1))
		$[%caster]STAB = $[%caster]STAB + 0.5
		echo Variable Update >> $[%caster]STAB = [$[%caster]STAB]. Move type equal to the user's primary type ✔️.
		log ... o golpe foi fortalecido pelo seu tipo principal ...
	end
	if ((($[%caster]MoveClass>=0) or ($[%caster]MoveClass=(-3))) and ([%caster]Stype=$[%caster]Movetype) and ($[%caster]Aim = 1))
		$[%caster]STAB = $[%caster]STAB + 0.5
		echo Variable Update >> $[%caster]STAB = [$[%caster]STAB]. Move type equal to the user's secondary type ✔️.
		log ... o golpe foi fortalecido pelo seu tipo secundário ...
	end
	echo Stance-Shape Checking: [%caster]Stance = [[%caster]Stance]; $[%caster]MoveShape = [$[%caster]MoveShape].
	if (!([%caster]Stance=0) and ([%caster]Stance=$[%caster]MoveShape))
		$[%caster]ShapeMod = 2
		echo Variable Update >> $[%caster]ShapeMod = 2. Move shape equal to the user's stance ✔️.
		log ... o golpe foi fortalecido pela sua postura ...
	end
	if (!([%caster]Stance=0) and !([%caster]Stance=$[%caster]MoveShape))
		$[%caster]ShapeMod = 0.5
		echo Variable Update >> $[%caster]ShapeMod = 0.5. Move shape not equal to the user's stance ❌.
		log ... o golpe foi enfraquecido pela sua postura ...
	end
	if (($[%caster]MoveClass=(-3)) and ($[%caster]Aim = 1))
		$[%caster]Heal = round(min(([%caster]MaxHP-[%caster]CurHP),(([%caster]MaxHP/100)*$[%caster]MovePower*$[%caster]STAB)))
		gain $[%caster]Heal [%caster]CurHP
		log se curou em [$[%caster]Heal] pontos de vida!
	end
	if (($[%caster]MoveClass>=0) and (chance($[%caster]MoveCrit*pow(2,[%caster]CriticalStage))) and ($[%caster]Aim = 1))
		$[%caster]Crit = 1.5
		echo Variable Update >> $[%caster]Crit ✔️. [$[%caster]MoveCrit*pow(2,[%caster]CriticalStage)]% chance, from $[%caster]MoveCrit ([$[%caster]MoveCrit]) * 2^[%caster]CriticalStage ([[%caster]CriticalStage])
		log ... e realizou um golpe crítico!
	end
	$auxMtype = $[%caster]MoveType
	$auxType = [%target]Ptype
*include effectivenessVerification %caster %target
	$[%caster]Sadvg = $auxAdvg
	$[%caster]TypeAdvg = $[%caster]Padvg*$[%caster]Sadvg
	if ([%target]Ptype=[%target]Stype)
		$[%caster]TypeAdvg = $[%caster]Padvg
	end
	if (($[%caster]MoveClass>=0) and ($[%caster]TypeAdvg=4) and ($[%caster]Aim = 1))
		log ... <#cc33ff><b>foi hiper efetivo!</b></#>
	end
	if (($[%caster]MoveClass>=0) and ($[%caster]TypeAdvg=2) and ($[%caster]Aim = 1))
		log ... <#4dffa6><b>foi super efetivo!</b></#>
	end
	if (($[%caster]MoveClass>=0) and ($[%caster]TypeAdvg=0.5) and ($[%caster]Aim = 1))
		log ... mas foi pouco efetivo!
	end
	if (($[%caster]MoveClass>=0) and ($[%caster]TypeAdvg=0.25) and ($[%caster]Aim = 1))
		log ... <#999999><b>mas não foi quase nada efetivo!</b></#>
	end
	if (($[%caster]MoveClass>=0) and ($[%caster]TypeAdvg=0) and ($[%caster]Aim = 1))
		log ... <#b30000><b>mas o golpe não afeta o adversário!</b></#>
	end
*include damageCalculation %caster %target
	$[%caster]RandomVar = frandom(0.85,1)
	if ($[%caster]MoveCategory=0)
		$aA = [%caster]Attack
		$aD = [%target]Defense
	end
	if ($[%caster]MoveCategory=1)
		$aA = [%caster]SpAttack
		$aD = [%target]SpDefense
	end
	if (($[%caster]MoveCategory=0) and ([%caster]Status=3))
		$[%caster]StatusMod = 0.5
	end
	$[%caster]RawDamage = ((((0.4*[%caster]Lvl + 2)*$[%caster]MovePower*($aA/$aD))/50)+2)
	echo Variable Update >> $[%caster]RawDamage = [$[%caster]RawDamage]. From ((((0.4*[%caster]Lvl + 2)*$[%caster]MovePower*($aA/$aD))/50)+2), where [%caster]Lvl = [[%caster]Lvl], $[%caster]MovePower = [$[%caster]MovePower], $aA = [$aA] ([?$[%caster]MoveCategory|[%caster]SpAttack|[%caster]Attack]), $aD = [$aD] ([?$[%caster]MoveCategory|[%target]SpDefense|[%target]Defense])
	if (($[%caster]MoveClass>=0) and ($[%caster]Aim = 1))
		$[%caster]DamageMods = $[%caster]TypeAdvg*$[%caster]RandomVar*$[%caster]STAB*$[%caster]Crit*$[%caster]StatusMod*$[%caster]ShapeMod
		$[%caster]EffectiveDamage = round(min(([%target]CurHP - $[%caster]NoKill),($[%caster]RawDamage*$[%caster]DamageMods)))
		echo Variable Update >> $[%caster]EffectiveDamage = [$[%caster]EffectiveDamage]. Capped at [%target]CurHP ([[%target]CurHP]) - $[%caster]NoKill ([$[%caster]NoKill]). From $[%caster]RawDamage and $[%caster]DamageMods = [$[%caster]DamageMods*100]%, which is $[%caster]TypeAdvg ([$[%caster]TypeAdvg*100]) * $[%caster]RandomVar ([$[%caster]RandomVar*100]) * $[%caster]STAB ([$[%caster]STAB*100]) * $[%caster]Crit ([$[%caster]Crit*100]) * $[%caster]StatusMod ([$[%caster]StatusMod*100]) * $[%caster]ShapeMod ([$[%caster]ShapeMod*100])
		lose $[%caster]EffectiveDamage [%target]CurHP
		echo Attribute Update >> [%target]CurHP = [[%target]CurHP]
		do getname with tag:[%target]
		log tomou [$[%caster]EffectiveDamage] dano!
		log <#3366ff>-----------------------------------</#>
	end
*include checkMoveFirst
	if (((activeSpeed+$activeMovePriority*10000)>=(enemySpeed+$enemyMovePriority*10000)) and (activeStatus>=0) and ($reflection=0) and ($activeMoveAttempt=0)) do activemove with this
	if (((activeSpeed+$activeMovePriority*10000)>=(enemySpeed+$enemyMovePriority*10000)) and (activeStatus>=0) and ($reflection=1) and ($activeMoveAttempt=0)) do activeconfusion with this
*include checkMoveFirstUnderStatus %statusID %statusName
	if ((activeSpeed+$activeMovePriority*10000)>=(enemySpeed+$enemyMovePriority*10000) and (activeStatus=(-[%statusID])) and ($[%statusName]=0) and ($reflection=0) and ($activeMoveAttempt=0)) do activemove with this
	if ((activeSpeed+$activeMovePriority*10000)>=(enemySpeed+$enemyMovePriority*10000) and (activeStatus=(-[%statusID])) and ($[%statusName]=0) and ($reflection=1) and ($activeMoveAttempt=0)) do activeconfusion with this
	if ((activeSpeed+$activeMovePriority*10000)>=(enemySpeed+$enemyMovePriority*10000) and (activeStatus=(-[%statusID])) and ($[%statusName]=1) and ($activeMoveAttempt=0)) do active[%statusName] with this
*include checkEnemyMoveUnderStatus %statusID %statusName
	if ((enemyStatus=(-[%statusID])) and ($e[%statusName]=0) and ($reflection=0) and ($enemyMoveAttempt=0)) do enemymove with this
	if ((enemyStatus=(-[%statusID])) and ($e[%statusName]=0) and ($reflection=1) and ($enemyMoveAttempt=0)) do enemyconfusion with this
	if ((enemyStatus=(-[%statusID])) and ($e[%statusName]=1)) do enemy[%statusName] with this
*include checkMoveSecond
	if ((enemySpeed+$enemyMovePriority*10000)>(activeSpeed+$activeMovePriority*10000) and (activeStatus>=0) and ($reflection=0) and ($activeMoveAttempt=0)) do activemove with this
	if ((enemySpeed+$enemyMovePriority*10000)>(activeSpeed+$activeMovePriority*10000) and (activeStatus>=0) and ($reflection=1) and ($activeMoveAttempt=0)) do activeconfusion with this
*include checkMoveSecondUnderStatus %statusID %statusName
	if ((enemySpeed+$enemyMovePriority*10000)>(activeSpeed+$activeMovePriority*10000) and (activeStatus=(-[%statusID])) and ($[%statusName]=0) and ($reflection=0) and ($activeMoveAttempt=0)) do activemove with this
	if ((enemySpeed+$enemyMovePriority*10000)>(activeSpeed+$activeMovePriority*10000) and (activeStatus=(-[%statusID])) and ($[%statusName]=0) and ($reflection=1) and ($activeMoveAttempt=0)) do activeconfusion with this
	if ((enemySpeed+$enemyMovePriority*10000)>(activeSpeed+$activeMovePriority*10000) and (activeStatus=(-[%statusID])) and ($[%statusName]=1) and ($activeMoveAttempt=0)) do active[%statusName] with this
*include statusHealingBerry %name %status %log
	on activeeatactive:
		do getname with tag:active
		log consumiu uma <#e65c00><b>[%name]</b></#> ...
		if (activeStatus=([%status]))
			activeStatus=0
			log [%log]
			log <#3366ff>-----------------------------------</#>
		end
		activeBerry = 0
	end
	on activeeatenemy:
		do getname with tag:active
		log mordeu a <#e65c00><b>[%name]</b></#> do adversário ...
		if (activeStatus=([%status]))
			activeStatus=0
			log [%log]
			log <#3366ff>-----------------------------------</#>
		end
		enemyBerry = 0
	end
	on enemyeatenemy:
		do getname with tag:enemy
		log consumiu uma <#e65c00><b>[%name]</b></#> ...
		if (enemyStatus=([%status]))
			enemyStatus=0
			log [%log]
			log <#3366ff>-----------------------------------</#>
		end
		enemyBerry = 0
	end
	on enemyeatactive:
		do getname with tag:enemy
		log mordeu a <#e65c00><b>[%name]</b></#> do adversário ...
		if (enemyStatus=([%status]))
			enemyStatus=0
			log [%log]
			log <#3366ff>-----------------------------------</#>
		end
		activeBerry = 0
	end
*include hpHealingBerry %name %heal
	on activeeatactive:
		do getname with tag:active
		log consumiu uma <#e65c00>[%name]</#> ...
		$heal = min(round((activeMaxHP/100)*[%heal]),(activeMaxHP-activeCurHP))
		gain $heal activeCurHP
		log se curou em [$heal] pontos de vida!
		log <#3366ff>-----------------------------------</#>
		activeBerry = 0
	end
	on activeeatenemy:
		do getname with tag:active
		log mordeu a <#e65c00>[%name]</#> do adversário ...
		$heal = min(round((activeMaxHP/100)*[%heal]),(activeMaxHP-activeCurHP))
		gain $heal activeCurHP
		log se curou em [$heal] pontos de vida!
		log <#3366ff>-----------------------------------</#>
		enemyBerry = 0
	end
	on enemyeatenemy:
		do getname with tag:enemy
		log consumiu uma <#e65c00>[%name]</#> ...
		$heal = min(round((enemyMaxHP/100)*[%heal]),(enemyMaxHP-enemyCurHP))
		gain $heal enemyCurHP
		log se curou em [$heal] pontos de vida!
		log <#3366ff>-----------------------------------</#>
		enemyBerry = 0
	end
	on enemyeatactive:
		do getname with tag:enemy
		log mordeu a <#e65c00>[%name]</#> do adversário ...
		$heal = min(round((enemyMaxHP/100)*[%heal]),(enemyMaxHP-enemyCurHP))
		gain $heal enemyCurHP
		log se curou em [$heal] pontos de vida!
		log <#3366ff>-----------------------------------</#>
		activeBerry = 0
	end
*include setFxToAbsValue %caster %fxNum
	if (0>$[%caster]MoveEffect[%fxNum])
		$[%caster]MoveEffect[%fxNum] = $[%caster]MoveEffect[%fxNum]*(-1)
	end
*include bugWorkAround
