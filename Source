		icon:pindohDex[7,0] pType[4,1] sType[4,1]

	[include capturedNoIcon %name="Dengorm" %number="10" %region="0" %variation="0" %evoLvl="7" %evoNumber="11"]
		icon:pindohDex[0,1] pType[0,1] sType[4,1]

	[include capturedNoIcon %name="Chikukoon" %number="11" %region="0" %variation="0" %evoLvl="10" %evoNumber="12"]
		icon:pindohDex[1,1] pType[0,1] sType[4,1]

	[include capturedNoIcon %name="Aedarrier" %number="12" %region="0" %variation="0" %evoLvl="0" %evoNumber="1"]
		icon:pindohDex[2,1] pType[0,1] sType[4,2]

    *TEMPLATE

//-----------------------------------------------Hidden------------------------------------------------------------------//

Upgrades
	*activePkm
	*enemyPkm
	*inBattle

//----------------------------------------------Battlers----------------------------------------------------------------//
    *TEMPLATE
        class:bigButton hasFlares
		no text
		hidden when 0

	[include battler %number="1" %region="0" %variation="0" %name="Guarimple" %sprite="https://i.imgur.com/Kco4d9D.png" %baseHP="70" %baseAtk="49" %baseDfs="30" %baseSpAtk="54" %baseSpDfs="32" %baseSpeed="75" %pType="11" %sType="11" %move1="tackle" %move2="tackle" %move3="tackle" %move4="tackle"]

	[include battler %number="4" %region="0" %variation="0" %name="Balldillo" %sprite="https://i.imgur.com/FNP4vop.png" %baseHP="58" %baseAtk="23" %baseDfs="81" %baseSpAtk="19" %baseSpDfs="84" %baseSpeed="45" %pType="9" %sType="9" %move1="tackle" %move2="tackle" %move3="tackle" %move4="tackle"]

	[include battler %number="7" %region="0" %variation="0" %name="Gossipavy" %sprite="https://i.imgur.com/xDNs4ss.png" %baseHP="76" %baseAtk="44" %baseDfs="40" %baseSpAtk="51" %baseSpDfs="37" %baseSpeed="63" %pType="10" %sType="10" %move1="tackle" %move2="tackle" %move3="tackle" %move4="tackle"]

	[include battler %number="10" %region="0" %variation="0" %name="Dengorm" %sprite="https://i.imgur.com/9Ehfwgo.png" %baseHP="23" %baseAtk="57" %baseDfs="27" %baseSpAtk="32" %baseSpDfs="27" %baseSpeed="19" %pType="6" %sType="10" %move1="vineWhip" %move2="vineWhip" %move3="vineWhip" %move4="vineWhip"]

	[include battler %number="11" %region="0" %variation="0" %name="Chikukoon" %sprite="https://i.imgur.com/YvnsJ8L.png" %baseHP="25" %baseAtk="25" %baseDfs="49" %baseSpAtk="22" %baseSpDfs="50" %baseSpeed="19" %pType="6" %sType="10" %move1="tackle" %move2="tackle" %move3="vineWhip" %move4="vineWhip"]

	[include battler %number="12" %region="0" %variation="0" %name="Aedarrier" %sprite="https://i.imgur.com/jFPFWeG.png" %baseHP="148" %baseAtk="147" %baseDfs="37" %baseSpAtk="32" %baseSpDfs="37" %baseSpeed="79" %pType="6" %sType="16" %move1="tackle" %move2="tackle" %move3="vineWhip" %move4="vineWhip"]
    *TEMPLATE

//----------------------------------------------Found Pkm----------------------------------------------------------------//

Achievements
	*TEMPLATE
		tag:foundTab
		hidden when 0
//	*FND[number]from[region]var[variation]
	*FND1from0var0
		name:Guarimple
		tags:foundTab starterFound
		desc:Inicial de <b>Pindoh</b>.
		icon:pindohDex[1,0]
