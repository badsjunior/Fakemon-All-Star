			if (0>=activeCurHP)
				activeStatus=-1
			end

			do getname with tag:active
			log não está mais confuso.

			activeLastMove = $activeMoveID
		end

//-------------------------------------------------------------------------------------------------//

		on enemyconfusion:
			enemyStatus = 0

			do getname with tag:enemy
			log se atacou por conta da confusão!

			[include onMove %caster="enemy" %target="enemy"]
			[include enemyMoveTypeAdvantage %type="0"]
			[include enemyMoveTypeAdvantage %type="1"]
			[include enemyMoveTypeAdvantage %type="2"]
			[include enemyMoveTypeAdvantage %type="3"]
			[include enemyMoveTypeAdvantage %type="4"]
			[include enemyMoveTypeAdvantage %type="5"]
			[include enemyMoveTypeAdvantage %type="6"]
			[include enemyMoveTypeAdvantage %type="7"]
			[include enemyMoveTypeAdvantage %type="8"]
			[include enemyMoveTypeAdvantage %type="9"]
			[include enemyMoveTypeAdvantage %type="10"]
			[include enemyMoveTypeAdvantage %type="11"]
			[include enemyMoveTypeAdvantage %type="12"]
			[include enemyMoveTypeAdvantage %type="13"]
			[include enemyMoveTypeAdvantage %type="14"]
			[include enemyMoveTypeAdvantage %type="15"]
			[include enemyMoveTypeAdvantage %type="16"]
			[include enemyMoveTypeAdvantage %type="17"]
			[include effectivenessVerification %caster="enemy" %target="enemy"]
			[include damageCalculation %caster="enemy" %target="enemy"]

			if ($enemyMoveEffect=59)
				$enemyMoveEffect = 25 + random(0,4)
			end
			if ($enemyMoveEffect=60)
				$enemyMoveEffect = 30 + random(0,4)
			end
			if ($enemyMoveEffect=61)
				$enemyMoveEffect = 35 + random(0,4)
			end

			[include moveEffect %who="enemy" %number="1"]
				$enemyEffectiveHeal = ceil(min(($enemyEffectiveDamage/2),(enemyMaxHP-enemyCurHP)))
				gain $enemyEffectiveHeal enemyCurHP
			[include announce %who="enemy" %log="drenou [$enemyEffectiveHeal] pontos de vida!"]

			[include moveEffect %who="enemy" %number="2"]
				$enemyEffectiveRecoil = ceil(min(($enemyEffectiveDamage/4),enemyCurHP))
				lose $enemyEffectiveRecoil enemyCurHP
			[include announce %who="enemy" %log="também se feriu, tomando [$enemyEffectiveRecoil] dano!"]

			[include moveEffect %who="enemy" %number="3"]
				$enemyEffectiveRecoil = ceil(min(($enemyEffectiveDamage/3),enemyCurHP))
				lose $enemyEffectiveRecoil enemyCurHP
			[include announce %who="enemy" %log="também se feriu, tomando [$enemyEffectiveRecoil] dano!"]

			[include moveStatusEffect %caster="enemy" %target="enemy" %number="4" %status="1" %log="foi envenenado!"]
			[include moveStatusEffect %caster="enemy" %target="enemy" %number="5" %status="2" %log="foi intoxicado!"]
			[include moveStatusEffect %caster="enemy" %target="enemy" %number="6" %status="3" %log="está em chamas!"]
			[include moveStatusEffect %caster="enemy" %target="enemy" %number="7" %status="-2" %log="foi paralisado!"]
			if ((($enemyMoveEffect=8) or ($enemyMoveEffect2=8)) and (chance($enemyMoveFxChance)) and ($enemyAim = 1) and (enemyStatus = 0))
				enemyStatus = -3
				enemySleep = random(1,3)
			[include announce %who="enemy" %log="caiu no sono!"]
			[include moveStatusEffect %caster="enemy" %target="enemy" %number="9" %status="-4" %log="foi congelado!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="10" %status="Attack" %amount="1" %log="teve seu Ataque ligeiramente reduzido!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="11" %status="Defense" %amount="1" %log="teve sua Defesa ligeiramente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="12" %status="SpAttack" %amount="1" %log="teve seu Ataque Especial ligeiramente reduzido!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="13" %status="SpDefense" %amount="1" %log="teve sua Defesa Especial ligeiramente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="14" %status="Speed" %amount="1" %log="teve sua Velocidade ligeiramente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="15" %status="Attack" %amount="2" %log="teve seu Ataque duramente reduzido!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="16" %status="Defense" %amount="2" %log="teve sua Defesa duramente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="17" %status="SpAttack" %amount="2" %log="teve seu Ataque Especial duramente reduzido!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="18" %status="SpDefense" %amount="2" %log="teve sua Defesa Especial duramente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="19" %status="Speed" %amount="2" %log="teve sua Velocidade duramente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="20" %status="Attack" %amount="3" %log="teve seu Ataque severamente reduzido!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="21" %status="Defense" %amount="3" %log="teve sua Defesa severamente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="22" %status="SpAttack" %amount="3" %log="teve seu Ataque Especial severamente reduzido!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="23" %status="SpDefense" %amount="3" %log="teve sua Defesa Especial severamente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="24" %status="Speed" %amount="3" %log="teve sua Velocidade severamente reduzida!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="25" %status="Attack" %amount="1" %log="teve seu Ataque ligeiramente aumentado!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="26" %status="Defense" %amount="1" %log="teve sua Defesa ligeiramente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="27" %status="SpAttack" %amount="1" %log="teve seu Ataque Especial ligeiramente aumentado!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="28" %status="SpDefense" %amount="1" %log="teve sua Defesa Especial ligeiramente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="29" %status="Speed" %amount="1" %log="teve sua Velocidade ligeiramente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="30" %status="Attack" %amount="2" %log="teve seu Ataque bruscamente aumentado!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="31" %status="Defense" %amount="2" %log="teve sua Defesa bruscamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="32" %status="SpAttack" %amount="2" %log="teve seu Ataque Especial bruscamente aumentado!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="33" %status="SpDefense" %amount="2" %log="teve sua Defesa Especial bruscamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="34" %status="Speed" %amount="2" %log="teve sua Velocidade bruscamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="35" %status="Attack" %amount="3" %log="teve seu Ataque drasticamente aumentado!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="36" %status="Defense" %amount="3" %log="teve sua Defesa drasticamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="37" %status="SpAttack" %amount="3" %log="teve seu Ataque Especial drasticamente aumentado!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="38" %status="SpDefense" %amount="3" %log="teve sua Defesa Especial drasticamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="39" %status="Speed" %amount="3" %log="teve sua Velocidade drasticamente aumentada!"]
			[include moveStatusEffect %caster="enemy" %target="enemy" %number="40" %status="-5" %log="ficou hesitante!"]
			[include moveStatusEffect %caster="enemy" %target="enemy" %number="41" %status="-6" %log="foi forçado a fugir!"]
			[include moveStatusEffect %caster="enemy" %target="enemy" %number="(-41)" %status="-6" %log="fugiu espontaneamente!"]
			[include moveEffect %who="enemy" %number="42"]
				enemyAttackStage = min(6,(enemyAttackStage+1))
				enemyDefenseStage = min(6,(enemyDefenseStage+1))
				enemySpAttackStage = min(6,(enemySpAttackStage+1))
				enemySpDefenseStage = min(6,(enemySpDefenseStage+1))
				enemySpeedStage = min(6,(enemySpeedStage+1))
			[include announce %who="enemy" %log="teve todos os seus status ligeiramente aumentados!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="43" %status="Accuracy" %amount="1" %log="teve sua precisão ligeiramente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="44" %status="Accuracy" %amount="2" %log="teve sua precisão bruscamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="45" %status="Accuracy" %amount="3" %log="teve sua precisão drasticamente aumentada!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="46" %status="Evasion" %amount="1" %log="teve sua evasão ligeiramente diminuída!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="47" %status="Evasion" %amount="2" %log="teve sua evasão duramente diminuída!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="48" %status="Evasion" %amount="2" %log="teve sua evasão severamente diminuída!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="49" %status="Accuracy" %amount="1" %log="teve sua precisão ligeiramente diminuída!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="50" %status="Accuracy" %amount="2" %log="teve sua precisão duramente diminuída!"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="51" %status="Accuracy" %amount="2" %log="teve sua precisão severamente diminuída!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="52" %status="Evasion" %amount="1" %log="teve sua evasão ligeiramente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="53" %status="Evasion" %amount="2" %log="teve sua evasão bruscamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="54" %status="Evasion" %amount="3" %log="teve sua evasão drasticamente aumentada!"]
			[include moveStatusEffect %caster="enemy" %target="enemy" %number="55" %status="-7" %log="ficou confuso!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="56" %status="Critical" %amount="1" %log="teve sua chance de crítico ligeiramente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="57" %status="Critical" %amount="2" %log="teve sua chance de crítico bruscamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="58" %status="Critical" %amount="3" %log="teve sua chance de crítico drasticamente aumentada!"]
			[include moveEatEffect %caster="enemy" %target="enemy" %number="62"]
			[include moveTypeChangeEffect %caster="enemy" %target="enemy" %number="63"]
			[include moveTypeChangeEffect %caster="enemy" %target="enemy" %number="64"]
			[include moveHealStatusEffect %caster="enemy" %target="enemy" %number="65" %log="se curou de seus status!"]
			[include moveStageResetEffect %caster="enemy" %target="enemy" %number="66" %log="zerou todos os seus modificadores!"]
			[include moveStageResetEffect %caster="enemy" %target="enemy" %number="(-66)" %log="teve todos os seus modificadores zerados!"]
			[include moveDirectDamageEffect %caster="enemy" %target="enemy" %number="67"]

			[include enemyMultipleHit %hit="2"]
			[include enemyMultipleHit %hit="3"]
			[include enemyMultipleHit %hit="4"]
			[include enemyMultipleHit %hit="5"]

			if (0>=enemyCurHP)
				enemyStatus=-1
			end

			do getname with tag:enemy
			log não está mais confuso.

			enemyLastMove = $enemyMoveID
		end

//-------------------------------------------------------------------------------------------------//

		on activefaint:
			do getname with tag:active
			log está fora de batalha!
			do return with tag:active
		end
		on enemyfaint:
			do getname with tag:enemy
			log está fora de batalha!
			do faint with tag:enemy
		end

		on activeflee:
			do getname with tag:active
			log deixou a batalha!
			do return with tag:active
		end
		on enemyflee:
			do getname with tag:enemy
			log deixou a batalha!
			lose tag:enemy
		end

		on activeflinch:
			do getname with tag:active
			log hesitou em atacar!
			activeStatus = 0
		end
		on enemyflinch:
			do getname with tag:enemy
			log hesitou em atacar!
			activeStatus = 0
		end

		on activethaw:
			do getname with tag:active
			log descongelou!
			activeStatus = 0
		end
		on enemythaw:
			do getname with tag:enemy
			log descongelou!
			activeStatus = 0
		end

		on activefreeze:
			do getname with tag:active
			log nem se moveu, pois ainda está dentro de uma pedra de gelo!
			activeSleep = (activeSleep-1)
		end
		on enemyfreeze:
			do getname with tag:enemy
			log nem se moveu, pois ainda está dentro de uma pedra de gelo!
			activeSleep = (activeSleep-1)
		end

		on activewake:
			do getname with tag:active
			log acordou!
			activeStatus = 0
		end
		on enemywake:
			do getname with tag:enemy
			log acordou!
			activeStatus = 0
		end

		on activesleep:
			do getname with tag:active
			log nem se moveu, pois ainda está dormindo!
			activeSleep = (activeSleep-1)
		end
		on enemysleep:
			do getname with tag:enemy
			log nem se moveu, pois ainda está dormindo!
			activeSleep = (activeSleep-1)
		end

		on activeparalysis:
			do getname with tag:active
			log não conseguiu se mover por causa da paralisia!
		end
		on enemyparalysis:
			do getname with tag:enemy
			log não conseguiu se mover por causa da paralisia!
		end

		on activepoison:
			$activePoisonDamage = round(min((activeMaxHP/8),activeCurHP))
			lose $activePoisonDamage activeCurHP
			do getname with tag:active
			log tomou [$activePoisonDamage] dano por envenenamento!
		end
		on enemypoison:
			$enemyPoisonDamage = round(min((enemyMaxHP/8),enemyCurHP))
			lose $enemyPoisonDamage enemyCurHP
			do getname with tag:enemy
			log tomou [$enemyPoisonDamage] dano por envenenamento!
		end

		on activebadpoison:
			$activePoisonDamage = round(min((activeBadPoison*activeMaxHP/16),activeCurHP))
			lose $activePoisonDamage activeCurHP
			do getname with tag:active
			log tomou [$activePoisonDamage] dano por intoxicação!
		end
		on enemybadpoison:
			$enemyPoisonDamage = round(min((enemyBadPoison*enemyMaxHP/8),enemyCurHP))
			lose $enemyPoisonDamage enemyCurHP
			do getname with tag:enemy
			log tomou [$enemyPoisonDamage] dano por intoxicação!
		end

		on activeburn:
			$activePoisonDamage = round(min((activeMaxHP/16),activeCurHP))
			lose $activePoisonDamage activeCurHP
			do getname with tag:active
			log tomou [$activePoisonDamage] dano por queimadura!
		end
		on enemyburn:
			$enemyPoisonDamage = round(min((enemyMaxHP/16),enemyCurHP))
			lose $enemyPoisonDamage enemyCurHP
			do getname with tag:enemy
			log tomou [$enemyPoisonDamage] dano por queimadura!
		end

		on endturn:
			if (activeStatus=1) do activepoison with this
			if (enemyStatus=1) do enemypoison with this
			if (activeStatus=2) do activebadpoison with this
			if (enemyStatus=2) do enemybadpoison with this
			if (activeStatus=3) do activeburn with this
			if (enemyStatus=3) do enemyburn with this
			if ((activeStatus=(-3)) and (activeSleep>0)) do activesleep with this
			if ((enemyStatus=(-3)) and (enemySleep>0)) do enemysleep with this
			if ((activeStatus=(-3)) and (0>=activeSleep)) do activewake with this
			if ((enemyStatus=(-3)) and (0>=enemySleep)) do enemywake with this
			$fifth = random(1,5)
			$thaw = 0
			if ($fifth=1) $thaw=1
			if ((activeStatus=(-4)) and ($thaw=0)) do activefreeze with this
			if ((enemyStatus=(-4)) and ($thaw=0)) do enemyfreeze with this
			if ((activeStatus=(-4)) and ($thaw=1)) do activethaw with this
			if ((enemyStatus=(-4)) and ($thaw=1)) do enemythaw with this
			if (activeStatus=(-5)) do activeflinch with this
			if (enemyStatus=(-5)) do enemyflinch with this
			if (activeStatus=(-6)) do activeflee with this
			if (enemyStatus=(-6)) do enemyflee with this

			[include defModByStage %stage="(-6)" %mod="0.25"]
			[include defModByStage %stage="(-5)" %mod="0.2857"]
			[include defModByStage %stage="(-4)" %mod="0.3333"]
			[include defModByStage %stage="(-3)" %mod="0.4"]
			[include defModByStage %stage="(-2)" %mod="0.5"]
			[include defModByStage %stage="(-1)" %mod="0.6666"]
			[include defModByStage %stage="0" %mod="1"]
			[include defModByStage %stage="1" %mod="1.5"]
			[include defModByStage %stage="2" %mod="2"]
			[include defModByStage %stage="3" %mod="2.5"]
			[include defModByStage %stage="4" %mod="3"]
			[include defModByStage %stage="5" %mod="3.5"]
			[include defModByStage %stage="6" %mod="4"]

			[include defAcEvModByStage %stage="(-6)" %mod="0.3333"]
			[include defAcEvModByStage %stage="(-5)" %mod="0.375"]
			[include defAcEvModByStage %stage="(-4)" %mod="0.4286"]
			[include defAcEvModByStage %stage="(-3)" %mod="0.5"]
			[include defAcEvModByStage %stage="(-2)" %mod="0.6"]
			[include defAcEvModByStage %stage="(-1)" %mod="0.75"]
			[include defAcEvModByStage %stage="0" %mod="1"]
			[include defAcEvModByStage %stage="1" %mod="1.3333"]
			[include defAcEvModByStage %stage="2" %mod="1.6666"]
			[include defAcEvModByStage %stage="3" %mod="2"]
			[include defAcEvModByStage %stage="4" %mod="2.3333"]
			[include defAcEvModByStage %stage="5" %mod="2.6666"]
			[include defAcEvModByStage %stage="6" %mod="3"]

			if ((enemyBerry=1) and (enemyStatus=(-2))) do enemyeat with BER1
			if ((enemyBerry=2) and (enemyStatus=(-3))) do enemyeat with BER2
			if ((enemyBerry=3) and (enemyStatus=1)) do enemyeat with BER3
			if ((enemyBerry=4) and (enemyStatus=3)) do enemyeat with BER4
			if ((enemyBerry=5) and (enemyStatus=(-4))) do enemyeat with BER5
			if ((enemyBerry=6) and (enemyStatus=(-7))) do enemyeat with BER6
			if ((enemyBerry=7) and (!((enemyStatus=(-1)) or (enemyStatus=0)))) do enemyeat with BER7
			if ((enemyBerry=8) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER8
			if ((enemyBerry=9) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER9
			if ((enemyBerry=10) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER10
			if ((enemyBerry=11) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER11
			if ((enemyBerry=12) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER12
			if ((enemyBerry=13) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER13

			if ((activeStatus=(-1)) or (0>=round(activeCurHP))) do activefaint with this
			if ((enemyStatus=(-1)) or (0>=round(enemyCurHP))) do enemyfaint with this
		end

		on type0advantage:
			$auxAdvg = 1
			if (($auxType=5) or ($auxType=8)) $auxAdvg = 0.5
			if ($auxType=7) $auxAdvg = 0
		end
		on type1advantage:
			$auxAdvg = 1
			if (($auxType=0) or ($auxType=5) or ($auxType=8) or ($auxType=14) or ($auxType=16)) $auxAdvg = 2
			if (($auxType=2) or ($auxType=3) or ($auxType=6) or ($auxType=13) or ($auxType=17)) $auxAdvg = 0.5
			if ($auxType=7) $auxAdvg = 0
		end
		on type2advantage:
			$auxAdvg = 1
			if (($auxType=1) or ($auxType=6) or ($auxType=11)) $auxAdvg = 2
			if (($auxType=5) or ($auxType=8) or ($auxType=12)) $auxAdvg = 0.5
		end
		on type3advantage:
			$auxAdvg = 1
			if (($auxType=11) or ($auxType=17)) $auxAdvg = 2
			if (($auxType=3) or ($auxType=4) or ($auxType=5) or ($auxType=7)) $auxAdvg = 0.5
			if ($auxType=8) $auxAdvg = 0
		end
		on type4advantage:
			$auxAdvg = 1
			if (($auxType=3) or ($auxType=5) or ($auxType=8) or ($auxType=9) or ($auxType=12)) $auxAdvg = 2
			if (($auxType=6) or ($auxType=11)) $auxAdvg = 0.5
			if ($auxType=2) $auxAdvg = 0
		end
		on type5advantage:
			$auxAdvg = 1
			if (($auxType=2) or ($auxType=6) or ($auxType=9) or ($auxType=14)) $auxAdvg = 2
			if (($auxType=1) or ($auxType=4) or ($auxType=8)) $auxAdvg = 0.5
		end
		on type6advantage:
			$auxAdvg = 1
			if (($auxType=11) or ($auxType=13) or ($auxType=16)) $auxAdvg = 2
			if (($auxType=1) or ($auxType=2) or ($auxType=3) or ($auxType=7) or ($auxType=8) or ($auxType=9) or ($auxType=17)) $auxAdvg = 0.5
		end
		on type7advantage:
			$auxAdvg = 1
			if (($auxType=7) or ($auxType=13)) $auxAdvg = 2
			if ($auxType=16) $auxAdvg = 0.5
			if ($auxType=0) $auxAdvg = 0
		end
		on type8advantage:
			$auxAdvg = 1
			if (($auxType=5) or ($auxType=14) or ($auxType=17)) $auxAdvg = 2
			if (($auxType=8) or ($auxType=9) or ($auxType=10) or ($auxType=12)) $auxAdvg = 0.5
		end
		on type9advantage:
			$auxAdvg = 1
			if (($auxType=6) or ($auxType=8) or ($auxType=11) or ($auxType=14)) $auxAdvg = 2
			if (($auxType=5) or ($auxType=9) or ($auxType=10) or ($auxType=15)) $auxAdvg = 0.5
		end
		on type10advantage:
			$auxAdvg = 1
			if (($auxType=4) or ($auxType=5) or ($auxType=9)) $auxAdvg = 2
			if (($auxType=10) or ($auxType=11) or ($auxType=15)) $auxAdvg = 0.5
		end
		on type11advantage:
			$auxAdvg = 1
			if (($auxType=4) or ($auxType=5) or ($auxType=10)) $auxAdvg = 2
			if (($auxType=2) or ($auxType=3) or ($auxType=6) or ($auxType=8) or ($auxType=9) or ($auxType=11) or ($auxType=15)) $auxAdvg = 0.5
		end
		on type12advantage:
			$auxAdvg = 1
			if (($auxType=2) or ($auxType=10)) $auxAdvg = 2
			if (($auxType=11) or ($auxType=12) or ($auxType=15)) $auxAdvg = 0.5
			if ($auxType=4) $auxAdvg = 0
		end
		on type13advantage:
			$auxAdvg = 1
			if (($auxType=1) or ($auxType=3)) $auxAdvg = 2
			if (($auxType=8) or ($auxType=13)) $auxAdvg = 0.5
			if ($auxType=0) $auxAdvg = 0
		end
		on type14advantage:
			$auxAdvg = 1
			if (($auxType=2) or ($auxType=4) or ($auxType=11) or ($auxType=15)) $auxAdvg = 2
			if (($auxType=8) or ($auxType=9) or ($auxType=10) or ($auxType=14)) $auxAdvg = 0.5
		end
		on type15advantage:
			$auxAdvg = 1
			if ($auxType=15) $auxAdvg = 2
			if ($auxType=8) $auxAdvg = 0.5
			if ($auxType=17) $auxAdvg = 0
		end
		on type16advantage:
			$auxAdvg = 1
			if (($auxType=7) or ($auxType=13)) $auxAdvg = 2
			if (($auxType=1) or ($auxType=16) or ($auxType=17)) $auxAdvg = 0.5
		end
		on type17advantage:
			$auxAdvg = 1
			if (($auxType=1) or ($auxType=15) or ($auxType=16)) $auxAdvg = 2
			if (($auxType=3) or ($auxType=8) or ($auxType=9)) $auxAdvg = 0.5
		end
//		on type#advantage:
//			$auxAdvg = 1
//			if (($auxType=5) or ($auxType=8)) $auxAdvg = 2
//			if (($auxType=5) or ($auxType=8)) $auxAdvg = 0.5
