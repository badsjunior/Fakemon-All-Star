			[include moveDebuffEffect %caster="active" %target="active" %number="46" %status="Evasion" %amount="1" %log="teve sua evasão ligeiramente diminuída!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="47" %status="Evasion" %amount="2" %log="teve sua evasão duramente diminuída!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="48" %status="Evasion" %amount="2" %log="teve sua evasão severamente diminuída!"]
