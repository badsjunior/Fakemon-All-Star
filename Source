			end
			if (($activeMoveEffect=58) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeCriticalStage = min(6,(activeCriticalStage+3))
				do getname with tag:active
				log teve sua chance de crítico drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end

			[include activeMultipleHit %hit="2"]
			[include activeMultipleHit %hit="3"]
			[include activeMultipleHit %hit="4"]
			[include activeMultipleHit %hit="5"]

			if (0>=enemyCurHP)
				enemyStatus=-1
			end

			activeLastMove = $activeMoveID
		end

//-------------------------------------------------------------------------------------------------//

		on enemymove:
			$enemyAim = 1

			if ($enemyMoveAccuracy>=0)
				$enemyAim = chance(($enemyMoveAccuracy*enemyAccuracyMod*enemyEvasionMod)*100)
			end

			if ($enemyAim = 0)
				do getname with tag:enemy
				log errou o golpe!
				enemyMovesInARow = 0
			end

			if (($enemyMoveID=enemyLastMove) and ($enemyAim = 1))
				enemyMovesInARow = (enemyMovesInARow+1)
			end
			if ((!($enemyMoveID=enemyLastMove)) and ($enemyAim = 1))
				enemyMovesInARow = 1
			end

			if (($enemyMoveClass>=0) and ($enemyAim = 1))
				do getname with tag:enemy
				log atacou...
				$enemySTAB = 1
				$enemyCrit = 1
				$enemyStatusMod = 1
			end
			if (($enemyMoveClass=1) and ($enemyAim = 1))
				$enemyMovePower = ($enemyMovePower*(min(5,enemyMovesInARow)))
				log ... com um poder acumulado de [enemyMovesInARow] golpes seguidos ...
			end
			if (($enemyMoveClass>=2) and ($enemyAim = 1))
				$numberOfHits = random(2,$enemyMoveClass)
				log ... [$numberOfHits] vezes ...
			end

			if (($enemyMoveClass>=0) and (enemyPtype=$enemyMovetype) and ($enemyAim = 1))
				$enemySTAB = $enemySTAB + 0.5
				log ... o golpe foi fortalecido pelo seu tipo principal ...
			end
			if (($enemyMoveClass>=0) and (enemyStype=$enemyMovetype) and ($enemyAim = 1))
				$enemySTAB = $enemySTAB + 0.5
				log ... o golpe foi fortalecido pelo seu tipo secundário ...
			end
			if (($enemyMoveClass>=0) and (chance($enemyMoveCrit*pow(2,enemyCriticalStage))) and ($enemyAim = 1))
				$enemyCrit = 1.5
				log ... e realizou um golpe crítico!
			end

			$auxMtype = $enemyMoveType
			$auxType = enemyPtype

			[include enemyMoveTypeAdvantage %type="0"]
			[include enemyMoveTypeAdvantage %type="1"]
			[include enemyMoveTypeAdvantage %type="2"]
			[include enemyMoveTypeAdvantage %type="3"]
			[include enemyMoveTypeAdvantage %type="4"]
			[include enemyMoveTypeAdvantage %type="5"]
			[include enemyMoveTypeAdvantage %type="6"]
			[include enemyMoveTypeAdvantage %type="7"]
			[include enemyMoveTypeAdvantage %type="8"]
			[include enemyMoveTypeAdvantage %type="9"]
			[include enemyMoveTypeAdvantage %type="10"]
			[include enemyMoveTypeAdvantage %type="11"]
			[include enemyMoveTypeAdvantage %type="12"]
			[include enemyMoveTypeAdvantage %type="13"]
			[include enemyMoveTypeAdvantage %type="14"]
			[include enemyMoveTypeAdvantage %type="15"]
			[include enemyMoveTypeAdvantage %type="16"]
			[include enemyMoveTypeAdvantage %type="17"]

			$enemySadvg = $auxAdvg
			$enemyTypeAdvg = $enemyPadvg*$enemySadvg

			if (enemyPtype=enemyStype)
				$enemyTypeAdvg = $enemyPadvg
			end

			if (($enemyMoveClass>=0) and ($enemyTypeAdvg=4) and ($enemyAim = 1))
				log ... <#cc33ff><b>foi hiper efetivo!</b></#>
			end
			if (($enemyMoveClass>=0) and ($enemyTypeAdvg=2) and ($enemyAim = 1))
				log ... <#4dffa6><b>foi super efetivo!</b></#>
			end
			if (($enemyMoveClass>=0) and ($enemyTypeAdvg=0.5) and ($enemyAim = 1))
				log ... mas foi pouco efetivo!
			end
			if (($enemyMoveClass>=0) and ($enemyTypeAdvg=0.25) and ($enemyAim = 1))
				log ... <#999999><b>mas não foi quase nada efetivo!</b></#>
			end
			if (($enemyMoveClass>=0) and ($enemyTypeAdvg=0) and ($enemyAim = 1))
				log ... <#b30000><b>mas o golpe não afeta o adversário!</b></#>
			end

			$enemyRandomVar = frandom(0.85,1)

			if ($enemyMoveIsSpecial=0)
				$aA = enemyAttack
				$aD = enemyDefense
			end
			if ($enemyMoveIsSpecial=1)
				$aA = enemySpAttack
				$aD = enemySpDefense
			end
			if (($enemyMoveIsSpecial=0) and (enemyStatus=3))
				$enemyStatusMod = 0.5
			end

			if (($enemyMoveClass>=0) and ($enemyAim = 1))
				$enemyRawDamage = ((((0.4*enemyLvl + 2)*$enemyMovePower*($aA/$aD))/50)+2)
				$enemyDamageMods = $enemyTypeAdvg*$enemyRandomVar*$enemySTAB*$enemyCrit*$enemyStatusMod
				$enemyEffectiveDamage = round(min(enemyCurHP,($enemyRawDamage*$enemyDamageMods)))
				lose $enemyEffectiveDamage enemyCurHP
				do getname with tag:enemy
				log tomou [$enemyEffectiveDamage] dano!
				log <#3366ff>-----------------------------------</#>
			end

			if ($enemyMoveEffect=59)
				$enemyMoveEffect = 25 + random(0,4)
			end
			if ($enemyMoveEffect=60)
				$enemyMoveEffect = 30 + random(0,4)
			end
			if ($enemyMoveEffect=61)
				$enemyMoveEffect = 35 + random(0,4)
			end

			if (($enemyMoveEffect=1) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				$enemyEffectiveHeal = ceil(min(($enemyEffectiveDamage/2),(enemyMaxHP-enemyCurHP)))
				gain $enemyEffectiveHeal enemyCurHP
				do getname with tag:enemy
				log drenou [$enemyEffectiveHeal] pontos de vida!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=2) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				$enemyEffectiveRecoil = ceil(min(($enemyEffectiveDamage/4),enemyCurHP))
				lose $enemyEffectiveRecoil enemyCurHP
				do getname with tag:enemy
				log também se feriu, tomando [$enemyEffectiveRecoil] dano!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=3) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				$enemyEffectiveRecoil = ceil(min(($enemyEffectiveDamage/3),enemyCurHP))
				lose $enemyEffectiveRecoil enemyCurHP
				do getname with tag:enemy
				log também se feriu, tomando [$enemyEffectiveRecoil] dano!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=4) and (chance($enemyMoveFxChance)) and (enemyStatus=0) and ($enemyAim = 1))
				enemyStatus = 1
				do getname with tag:enemy
				log foi envenenado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=5) and (chance($enemyMoveFxChance)) and (enemyStatus=0) and ($enemyAim = 1))
				enemyStatus = 2
				do getname with tag:enemy
				log foi intoxicado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=6) and (chance($enemyMoveFxChance)) and (enemyStatus=0) and ($enemyAim = 1))
				enemyStatus = 3
				do getname with tag:enemy
				log está em chamas!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=7) and (chance($enemyMoveFxChance)) and (enemyStatus=0) and ($enemyAim = 1))
				enemyStatus = -2
				do getname with tag:enemy
				log foi paralisado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=8) and (chance($enemyMoveFxChance)) and (enemyStatus=0) and ($enemyAim = 1))
				enemyStatus = -3
				do getname with tag:enemy
				enemySleep = random(1,3)
				log caiu no sono!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=8) and (chance($enemyMoveFxChance)) and (enemyStatus=0) and ($enemyAim = 1))
				enemyStatus = -4
				do getname with tag:enemy
				log foi congelado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=10) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyAttackStage = max((-6),(enemyAttackStage-1))
				do getname with tag:enemy
				log teve seu Ataque ligeiramente reduzido!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=11) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyDefenseStage = max((-6),(enemyDefenseStage-1))
				do getname with tag:enemy
				log teve sua Defesa ligeiramente reduzida!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=12) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemySpAttackStage = max((-6),(enemySpAttackStage-1))
				do getname with tag:enemy
				log teve seu Ataque Especial ligeiramente reduzido!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=13) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemySpDefenseStage = max((-6),(enemySpDefenseStage-1))
				do getname with tag:enemy
				log teve sua Defesa Especial ligeiramente reduzida!
