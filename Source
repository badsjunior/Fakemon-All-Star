		$activeCrit = 1.5
		log ... e realizou um golpe crítico!
	end

	if ((activeCurHP>0) and (activeCurHP>0) and ($activeMoveClass>=[%hit]) and ($numberOfHits>=[%hit]) and ($activeAim = 1))
		$activeDamageMods = $activeTypeAdvg*$activeRandomVar*$activeSTAB*$activeCrit*$activeStatusMod
		$activeEffectiveDamage = round(min(activeCurHP,($activeRawDamage*$activeDamageMods)))
		lose $activeEffectiveDamage activeCurHP
		do getname with tag:active
		log tomou [$activeEffectiveDamage] dano!
		log <#3366ff>-----------------------------------</#>
	end

*include enemyConfusedMultHit %hit
	$enemyAim = chance(($enemyMoveAccuracy*enemyAccuracyMod*enemyEvasionMod)*100)

	if ($enemyAim = 0)
		do getname with tag:enemy
		log errou o golpe!
		enemyMovesInARow = 0
