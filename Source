Layout
*wrapper
	contains:upperArea, lowerArea, leftArea, middleArea, rightArea
	*upperArea
		contains:statusDisplay
		*statusDisplay
			contains:tag:status
			class:fullWidth
	*leftArea
		contains:logDisplay
		*logDisplay
			contains:Log
			header:Log
	*middleArea
		contains:clickablesDisplay
		*clickablesDisplay
			contains:starterDisplay, activeDisplay, enemyDisplay, moveDisplay
			*starterDisplay
				contains:tag:starterChoice
			*activeDisplay
				contains:tag:active
			*enemyDisplay
				contains:tag:enemy
			*moveDisplay
				contains:tag:activeMove, tag:enemyMove
	*rightArea
		contains:exploreDisplay
		*exploreDisplay
			contains:tag:explore
			header:Exploração
			tooltip origin:left
	*lowerArea
		contains:mainMenu
			*mainMenu
				contains:navigationDisplay, contentDisplay
				class:fullWidth
				*navigationDisplay
					contains:tag:navigationButton
					costs:hide
					names:hide
				*contentDisplay
					contains:tag:foundTab, tag:capturedTab, tag:regionTab, tag:achievTab, tag:statisticTab
					costs:hide
					names:hide

//-----------------------------------------------UI Text------------------------------------------------------------------//

Upgrades
	*TEMPLATE
		class:UItext
		no tooltip
		owned	
	*foundText
		name:Pokémon Avistados
		tag:foundTab
	*capturedText
		name:Pokémon Capturados
		tag:capturedTab
	*regionText
		name:Regiões
		tag:regionTab
	*achievText
		name:Conquistas
		tag:achievTab
	*statisticText
		name:Estatísticas
		tag:statisticTab
	*TEMPLATE

//---------------------------------------------------GLOBAL---------------------------------------------------------------//

Resources
	*TEMPLATE
		tags:status hidden
	*global
		name:Global Controler and Initializer
		passive:
			gain 1 secondPlayed
			if (no inBattle)
				gain min((activeMaxHP/100),(activeMaxHP-activeCurHP)) activeCurHP
				gain 0.01 pokeball
				gain 0.025 switch
			end
		end
		on start:
			pokeball = 5
			switch = 1
			gain tag:starterFound
			show tag:foundTab
			hide tag:capturedTab
			hide tag:regionTab
			hide tag:achievTab
			hide tag:statisticTab
			log Bem vindo! Depois que <#ffcc00><b>escolher</b></#> um dos pokémon iniciais, veja seus pokémon capturados na segunda aba do <#ff33ff><b>menu</b></#> abaixo (ícone de pokébola). <#ffcc00><b>Clique</b></#> em um de seus pokémon capturados para chamá-lo para batalhar. Isto irá gastar um ponto de <u>troca</u>. <#ffcc00><b>Selecione</b></#> uma área no <#ff33ff><b>menu de exploração</b></#> à direita para encontrar pokémon selvagens. <#ffcc00><b>Clique</b></#> em um pokémon selvagem para tentar <#ffcc00><b>capturá-lo</b></#>. Isto irá gastar uma <u>pokébola</u>. Te dei 5 <u>pokébolas</u> de presente, não as desperdice! É muito mais fácil <#ffcc00><b>capturar</b></#> um pokémon quando ele está enfraquecido. Enquanto estiver fora de batalha, seu pokémon irá regenerar pontos de <u>vida</u> e você irá conseguir mais <u>pokébolas</u> e pontos de <u>troca</u> com o tempo.
		end

		on beginturn:
			[include randomBasedConditions]
			[include checkMoveFirst]
			[include checkMoveFirstUnderStatus %statusID="2" %statusName="paralysis"]
			[include checkMoveFirstUnderStatus %statusID="7" %statusName="confusion"]

			if (enemyStatus>=0) do enemymove with this
			[include checkEnemyMoveUnderStatus %statusID="2" %statusName="paralysis"]
			[include checkEnemyMoveUnderStatus %statusID="7" %statusName="confusion"]

			[include checkMoveSecond]
			[include checkMoveSecondUnderStatus %statusID="2" %statusName="paralysis"]
			[include checkMoveSecondUnderStatus %statusID="7" %statusName="confusion"]

			do endturn with this
		end

		on activemove:
			[include moveEffect %who="active" %number="68"]
				[include moveSettingsCopy %caster="active" %target="enemy"]
				[include moveEffectsCopy %caster="active" %target="enemy"]
			[include announce %who="active" %log="copiou a habilidade do adversário!"]

			[include moveEffect %who="active" %number="69"]
				[include moveEffectsCopy %caster="active" %target="enemy"]
			[include announce %who="active" %log="copiou os efeitos do movimento do adversário!"]

			[include conditionalMoveEffect %who="active" %number="70" %condition="$enemyMoveClass>=0"]
				[include moveSettingsCopy %caster="active" %target="enemy"]
				[include moveEffectsCopy %caster="active" %target="enemy"]
			[include announce %who="active" %log="copiou a habilidade do adversário!"]

			[include conditionalMoveEffect %who="active" %number="71" %condition="($enemyMoveClass=(-1) or $enemyMoveClass=(-4))"]
				[include moveSettingsCopy %caster="active" %target="enemy"]
				[include moveEffectsCopy %caster="active" %target="enemy"]
			[include announce %who="active" %log="copiou a habilidade do adversário!"]

			[include conditionalMoveEffect %who="active" %number="72" %condition="$enemyMoveClass=(-3)"]
				[include moveSettingsCopy %caster="active" %target="enemy"]
				[include moveEffectsCopy %caster="active" %target="enemy"]
			[include announce %who="active" %log="copiou a habilidade do adversário!"]

			[include onMove %caster="active" %target="enemy"]
			[include activeMoveTypeAdvantage %type="0"]
			[include activeMoveTypeAdvantage %type="1"]
			[include activeMoveTypeAdvantage %type="2"]
			[include activeMoveTypeAdvantage %type="3"]
			[include activeMoveTypeAdvantage %type="4"]
			[include activeMoveTypeAdvantage %type="5"]
			[include activeMoveTypeAdvantage %type="6"]
			[include activeMoveTypeAdvantage %type="7"]
			[include activeMoveTypeAdvantage %type="8"]
			[include activeMoveTypeAdvantage %type="9"]
			[include activeMoveTypeAdvantage %type="10"]
			[include activeMoveTypeAdvantage %type="11"]
			[include activeMoveTypeAdvantage %type="12"]
			[include activeMoveTypeAdvantage %type="13"]
			[include activeMoveTypeAdvantage %type="14"]
			[include activeMoveTypeAdvantage %type="15"]
			[include activeMoveTypeAdvantage %type="16"]
			[include activeMoveTypeAdvantage %type="17"]
			[include effectivenessVerification %caster="active" %target="enemy"]
			[include damageCalculation %caster="active" %target="enemy"]

			if ($activeMoveEffect=59)
				$activeMoveEffect = 25 + random(0,4)
			end
			if ($activeMoveEffect=60)
				$activeMoveEffect = 30 + random(0,4)
			end
			if ($activeMoveEffect=61)
				$activeMoveEffect = 35 + random(0,4)
			end

			[include moveEffect %who="active" %number="1"]
				$activeEffectiveHeal = ceil(min(($activeEffectiveDamage/2),(activeMaxHP-activeCurHP)))
				gain $activeEffectiveHeal activeCurHP
			[include announce %who="active" %log="drenou [$activeEffectiveHeal] pontos de vida!"]

			[include moveEffect %who="active" %number="2"]
				$activeEffectiveRecoil = ceil(min(($activeEffectiveDamage/4),activeCurHP))
				lose $activeEffectiveRecoil activeCurHP
			[include announce %who="active" %log="também se feriu, tomando [$activeEffectiveRecoil] dano!"]

			[include moveEffect %who="active" %number="3"]
				$activeEffectiveRecoil = ceil(min(($activeEffectiveDamage/3),activeCurHP))
				lose $activeEffectiveRecoil activeCurHP
			[include announce %who="active" %log="também se feriu, tomando [$activeEffectiveRecoil] dano!"]

			[include moveStatusEffect %caster="active" %target="enemy" %number="4" %status="1" %log="foi envenenado!"]
			[include moveStatusEffect %caster="active" %target="enemy" %number="5" %status="2" %log="foi intoxicado!"]
			[include moveStatusEffect %caster="active" %target="enemy" %number="6" %status="3" %log="está em chamas!"]
			[include moveStatusEffect %caster="active" %target="enemy" %number="7" %status="-2" %log="foi paralisado!"]
			if ((($activeMoveEffect=8) or ($activeMoveEffect2=8)) and (chance($activeMoveFxChance)) and ($activeAim = 1) and (enemyStatus = 0))
				enemyStatus = -3
				enemySleep = random(1,3)
			[include announce %who="enemy" %log="caiu no sono!"]
			[include moveStatusEffect %caster="active" %target="enemy" %number="9" %status="-4" %log="foi congelado!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="10" %status="Attack" %amount="1" %log="teve seu Ataque ligeiramente reduzido!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="11" %status="Defense" %amount="1" %log="teve sua Defesa ligeiramente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="12" %status="SpAttack" %amount="1" %log="teve seu Ataque Especial ligeiramente reduzido!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="13" %status="SpDefense" %amount="1" %log="teve sua Defesa Especial ligeiramente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="14" %status="Speed" %amount="1" %log="teve sua Velocidade ligeiramente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="15" %status="Attack" %amount="2" %log="teve seu Ataque duramente reduzido!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="16" %status="Defense" %amount="2" %log="teve sua Defesa duramente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="17" %status="SpAttack" %amount="2" %log="teve seu Ataque Especial duramente reduzido!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="18" %status="SpDefense" %amount="2" %log="teve sua Defesa Especial duramente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="19" %status="Speed" %amount="2" %log="teve sua Velocidade duramente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="20" %status="Attack" %amount="3" %log="teve seu Ataque severamente reduzido!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="21" %status="Defense" %amount="3" %log="teve sua Defesa severamente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="22" %status="SpAttack" %amount="3" %log="teve seu Ataque Especial severamente reduzido!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="23" %status="SpDefense" %amount="3" %log="teve sua Defesa Especial severamente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="24" %status="Speed" %amount="3" %log="teve sua Velocidade severamente reduzida!"]
			[include moveBuffEffect %caster="active" %target="active" %number="25" %status="Attack" %amount="1" %log="teve seu Ataque ligeiramente aumentado!"]
			[include moveBuffEffect %caster="active" %target="active" %number="26" %status="Defense" %amount="1" %log="teve sua Defesa ligeiramente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="27" %status="SpAttack" %amount="1" %log="teve seu Ataque Especial ligeiramente aumentado!"]
			[include moveBuffEffect %caster="active" %target="active" %number="28" %status="SpDefense" %amount="1" %log="teve sua Defesa Especial ligeiramente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="29" %status="Speed" %amount="1" %log="teve sua Velocidade ligeiramente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="30" %status="Attack" %amount="2" %log="teve seu Ataque bruscamente aumentado!"]
			[include moveBuffEffect %caster="active" %target="active" %number="31" %status="Defense" %amount="2" %log="teve sua Defesa bruscamente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="32" %status="SpAttack" %amount="2" %log="teve seu Ataque Especial bruscamente aumentado!"]
			[include moveBuffEffect %caster="active" %target="active" %number="33" %status="SpDefense" %amount="2" %log="teve sua Defesa Especial bruscamente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="34" %status="Speed" %amount="2" %log="teve sua Velocidade bruscamente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="35" %status="Attack" %amount="3" %log="teve seu Ataque drasticamente aumentado!"]
			[include moveBuffEffect %caster="active" %target="active" %number="36" %status="Defense" %amount="3" %log="teve sua Defesa drasticamente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="37" %status="SpAttack" %amount="3" %log="teve seu Ataque Especial drasticamente aumentado!"]
			[include moveBuffEffect %caster="active" %target="active" %number="38" %status="SpDefense" %amount="3" %log="teve sua Defesa Especial drasticamente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="39" %status="Speed" %amount="3" %log="teve sua Velocidade drasticamente aumentada!"]
			[include moveStatusEffect %caster="active" %target="enemy" %number="40" %status="-5" %log="ficou hesitante!"]
			[include moveStatusEffect %caster="active" %target="enemy" %number="41" %status="-6" %log="foi forçado a fugir!"]
			[include moveStatusEffect %caster="active" %target="active" %number="(-41)" %status="-6" %log="fugiu espontaneamente!"]
			[include moveEffect %who="active" %number="42"]
				activeAttackStage = min(6,(activeAttackStage+1))
				activeDefenseStage = min(6,(activeDefenseStage+1))
				activeSpAttackStage = min(6,(activeSpAttackStage+1))
				activeSpDefenseStage = min(6,(activeSpDefenseStage+1))
				activeSpeedStage = min(6,(activeSpeedStage+1))
			[include announce %who="active" %log="teve todos os seus status ligeiramente aumentados!"]
			[include moveBuffEffect %caster="active" %target="active" %number="43" %status="Accuracy" %amount="1" %log="teve sua precisão ligeiramente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="44" %status="Accuracy" %amount="2" %log="teve sua precisão bruscamente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="45" %status="Accuracy" %amount="3" %log="teve sua precisão drasticamente aumentada!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="46" %status="Evasion" %amount="1" %log="teve sua evasão ligeiramente diminuída!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="47" %status="Evasion" %amount="2" %log="teve sua evasão duramente diminuída!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="48" %status="Evasion" %amount="2" %log="teve sua evasão severamente diminuída!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="49" %status="Accuracy" %amount="1" %log="teve sua precisão ligeiramente diminuída!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="50" %status="Accuracy" %amount="2" %log="teve sua precisão duramente diminuída!"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="51" %status="Accuracy" %amount="2" %log="teve sua precisão severamente diminuída!"]
			[include moveBuffEffect %caster="active" %target="active" %number="52" %status="Evasion" %amount="1" %log="teve sua evasão ligeiramente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="53" %status="Evasion" %amount="2" %log="teve sua evasão bruscamente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="54" %status="Evasion" %amount="3" %log="teve sua evasão drasticamente aumentada!"]
			[include moveStatusEffect %caster="active" %target="enemy" %number="55" %status="-7" %log="ficou confuso!"]
			[include moveBuffEffect %caster="active" %target="active" %number="56" %status="Critical" %amount="1" %log="teve sua chance de crítico ligeiramente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="57" %status="Critical" %amount="2" %log="teve sua chance de crítico bruscamente aumentada!"]
			[include moveBuffEffect %caster="active" %target="active" %number="58" %status="Critical" %amount="3" %log="teve sua chance de crítico drasticamente aumentada!"]
			[include moveEatEffect %caster="active" %target="enemy" %number="62"]
			[include moveTypeChangeEffect %caster="active" %target="enemy" %number="63"]
			[include moveTypeChangeEffect %caster="active" %target="active" %number="64"]
			[include moveHealStatusEffect %caster="active" %target="active" %number="65" %log="se curou de seus status!"]
			[include moveStageResetEffect %caster="active" %target="active" %number="66" %log="zerou todos os seus modificadores!"]
			[include moveStageResetEffect %caster="active" %target="enemy" %number="(-66)" %log="teve todos os seus modificadores zerados!"]
			[include moveDirectDamageEffect %caster="active" %target="enemy" %number="67"]

			[include activeMultipleHit %hit="2"]
			[include activeMultipleHit %hit="3"]
			[include activeMultipleHit %hit="4"]
			[include activeMultipleHit %hit="5"]

			if (0>=enemyCurHP)
				enemyStatus=-1
			end

			activeLastMove = $activeMoveID
		end

//-------------------------------------------------------------------------------------------------//

		on enemymove:
			[include moveEffect %who="enemy" %number="68"]
				[include moveSettingsCopy %caster="enemy" %target="active"]
				[include moveEffectsCopy %caster="enemy" %target="active"]
			[include announce %who="enemy" %log="copiou a habilidade do adversário!"]

			[include moveEffect %who="enemy" %number="69"]
				[include moveEffectsCopy %caster="enemy" %target="active"]
			[include announce %who="enemy" %log="copiou os efeitos do movimento do adversário!"]

			[include conditionalMoveEffect %who="enemy" %number="70" %condition="$activeMoveClass>=0"]
				[include moveSettingsCopy %caster="enemy" %target="active"]
				[include moveEffectsCopy %caster="enemy" %target="active"]
			[include announce %who="enemy" %log="copiou a habilidade do adversário!"]

			[include conditionalMoveEffect %who="enemy" %number="71" %condition="($activeMoveClass=(-1) or $activeMoveClass=(-4))"]
				[include moveSettingsCopy %caster="enemy" %target="active"]
				[include moveEffectsCopy %caster="enemy" %target="active"]
			[include announce %who="enemy" %log="copiou a habilidade do adversário!"]

			[include conditionalMoveEffect %who="enemy" %number="72" %condition="$activeMoveClass=(-3)"]
				[include moveSettingsCopy %caster="enemy" %target="active"]
				[include moveEffectsCopy %caster="enemy" %target="active"]
			[include announce %who="enemy" %log="copiou a habilidade do adversário!"]

			[include onMove %caster="enemy" %target="active"]
			[include enemyMoveTypeAdvantage %type="0"]
			[include enemyMoveTypeAdvantage %type="1"]
			[include enemyMoveTypeAdvantage %type="2"]
			[include enemyMoveTypeAdvantage %type="3"]
			[include enemyMoveTypeAdvantage %type="4"]
			[include enemyMoveTypeAdvantage %type="5"]
			[include enemyMoveTypeAdvantage %type="6"]
			[include enemyMoveTypeAdvantage %type="7"]
			[include enemyMoveTypeAdvantage %type="8"]
			[include enemyMoveTypeAdvantage %type="9"]
			[include enemyMoveTypeAdvantage %type="10"]
			[include enemyMoveTypeAdvantage %type="11"]
			[include enemyMoveTypeAdvantage %type="12"]
			[include enemyMoveTypeAdvantage %type="13"]
			[include enemyMoveTypeAdvantage %type="14"]
			[include enemyMoveTypeAdvantage %type="15"]
			[include enemyMoveTypeAdvantage %type="16"]
			[include enemyMoveTypeAdvantage %type="17"]
			[include effectivenessVerification %caster="enemy" %target="active"]
			[include damageCalculation %caster="enemy" %target="active"]

			if ($enemyMoveEffect=59)
				$enemyMoveEffect = 25 + random(0,4)
			end
			if ($enemyMoveEffect=60)
				$enemyMoveEffect = 30 + random(0,4)
			end
			if ($enemyMoveEffect=61)
				$enemyMoveEffect = 35 + random(0,4)
			end

			[include moveEffect %who="enemy" %number="1"]
				$enemyEffectiveHeal = ceil(min(($enemyEffectiveDamage/2),(enemyMaxHP-enemyCurHP)))
				gain $enemyEffectiveHeal enemyCurHP
			[include announce %who="enemy" %log="drenou [$enemyEffectiveHeal] pontos de vida!"]

			[include moveEffect %who="enemy" %number="2"]
				$enemyEffectiveRecoil = ceil(min(($enemyEffectiveDamage/4),enemyCurHP))
				lose $enemyEffectiveRecoil enemyCurHP
			[include announce %who="enemy" %log="também se feriu, tomando [$enemyEffectiveRecoil] dano!"]

			[include moveEffect %who="enemy" %number="3"]
				$enemyEffectiveRecoil = ceil(min(($enemyEffectiveDamage/3),enemyCurHP))
				lose $enemyEffectiveRecoil enemyCurHP
			[include announce %who="enemy" %log="também se feriu, tomando [$enemyEffectiveRecoil] dano!"]

			[include moveStatusEffect %caster="enemy" %target="active" %number="4" %status="1" %log="foi envenenado!"]
			[include moveStatusEffect %caster="enemy" %target="active" %number="5" %status="2" %log="foi intoxicado!"]
			[include moveStatusEffect %caster="enemy" %target="active" %number="6" %status="3" %log="está em chamas!"]
			[include moveStatusEffect %caster="enemy" %target="active" %number="7" %status="-2" %log="foi paralisado!"]
			if ((($enemyMoveEffect=8) or ($enemyMoveEffect2=8)) and (chance($enemyMoveFxChance)) and ($enemyAim = 1) and (activeStatus = 0))
				activeStatus = -3
				activeSleep = random(1,3)
			[include announce %who="active" %log="caiu no sono!"]
			[include moveStatusEffect %caster="enemy" %target="active" %number="9" %status="-4" %log="foi congelado!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="10" %status="Attack" %amount="1" %log="teve seu Ataque ligeiramente reduzido!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="11" %status="Defense" %amount="1" %log="teve sua Defesa ligeiramente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="12" %status="SpAttack" %amount="1" %log="teve seu Ataque Especial ligeiramente reduzido!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="13" %status="SpDefense" %amount="1" %log="teve sua Defesa Especial ligeiramente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="14" %status="Speed" %amount="1" %log="teve sua Velocidade ligeiramente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="15" %status="Attack" %amount="2" %log="teve seu Ataque duramente reduzido!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="16" %status="Defense" %amount="2" %log="teve sua Defesa duramente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="17" %status="SpAttack" %amount="2" %log="teve seu Ataque Especial duramente reduzido!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="18" %status="SpDefense" %amount="2" %log="teve sua Defesa Especial duramente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="19" %status="Speed" %amount="2" %log="teve sua Velocidade duramente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="20" %status="Attack" %amount="3" %log="teve seu Ataque severamente reduzido!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="21" %status="Defense" %amount="3" %log="teve sua Defesa severamente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="22" %status="SpAttack" %amount="3" %log="teve seu Ataque Especial severamente reduzido!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="23" %status="SpDefense" %amount="3" %log="teve sua Defesa Especial severamente reduzida!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="24" %status="Speed" %amount="3" %log="teve sua Velocidade severamente reduzida!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="25" %status="Attack" %amount="1" %log="teve seu Ataque ligeiramente aumentado!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="26" %status="Defense" %amount="1" %log="teve sua Defesa ligeiramente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="27" %status="SpAttack" %amount="1" %log="teve seu Ataque Especial ligeiramente aumentado!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="28" %status="SpDefense" %amount="1" %log="teve sua Defesa Especial ligeiramente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="29" %status="Speed" %amount="1" %log="teve sua Velocidade ligeiramente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="30" %status="Attack" %amount="2" %log="teve seu Ataque bruscamente aumentado!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="31" %status="Defense" %amount="2" %log="teve sua Defesa bruscamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="32" %status="SpAttack" %amount="2" %log="teve seu Ataque Especial bruscamente aumentado!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="33" %status="SpDefense" %amount="2" %log="teve sua Defesa Especial bruscamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="34" %status="Speed" %amount="2" %log="teve sua Velocidade bruscamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="35" %status="Attack" %amount="3" %log="teve seu Ataque drasticamente aumentado!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="36" %status="Defense" %amount="3" %log="teve sua Defesa drasticamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="37" %status="SpAttack" %amount="3" %log="teve seu Ataque Especial drasticamente aumentado!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="38" %status="SpDefense" %amount="3" %log="teve sua Defesa Especial drasticamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="39" %status="Speed" %amount="3" %log="teve sua Velocidade drasticamente aumentada!"]
			[include moveStatusEffect %caster="enemy" %target="active" %number="40" %status="-5" %log="ficou hesitante!"]
			[include moveStatusEffect %caster="enemy" %target="active" %number="41" %status="-6" %log="foi forçado a fugir!"]
			[include moveStatusEffect %caster="enemy" %target="enemy" %number="(-41)" %status="-6" %log="fugiu espontaneamente!"]
			[include moveEffect %who="enemy" %number="42"]
				enemyAttackStage = min(6,(enemyAttackStage+1))
				enemyDefenseStage = min(6,(enemyDefenseStage+1))
				enemySpAttackStage = min(6,(enemySpAttackStage+1))
				enemySpDefenseStage = min(6,(enemySpDefenseStage+1))
				enemySpeedStage = min(6,(enemySpeedStage+1))
			[include announce %who="enemy" %log="teve todos os seus status ligeiramente aumentados!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="43" %status="Accuracy" %amount="1" %log="teve sua precisão ligeiramente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="44" %status="Accuracy" %amount="2" %log="teve sua precisão bruscamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="45" %status="Accuracy" %amount="3" %log="teve sua precisão drasticamente aumentada!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="46" %status="Evasion" %amount="1" %log="teve sua evasão ligeiramente diminuída!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="47" %status="Evasion" %amount="2" %log="teve sua evasão duramente diminuída!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="48" %status="Evasion" %amount="2" %log="teve sua evasão severamente diminuída!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="49" %status="Accuracy" %amount="1" %log="teve sua precisão ligeiramente diminuída!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="50" %status="Accuracy" %amount="2" %log="teve sua precisão duramente diminuída!"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="51" %status="Accuracy" %amount="2" %log="teve sua precisão severamente diminuída!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="52" %status="Evasion" %amount="1" %log="teve sua evasão ligeiramente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="53" %status="Evasion" %amount="2" %log="teve sua evasão bruscamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="54" %status="Evasion" %amount="3" %log="teve sua evasão drasticamente aumentada!"]
			[include moveStatusEffect %caster="enemy" %target="active" %number="55" %status="-7" %log="ficou confuso!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="56" %status="Critical" %amount="1" %log="teve sua chance de crítico ligeiramente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="57" %status="Critical" %amount="2" %log="teve sua chance de crítico bruscamente aumentada!"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="58" %status="Critical" %amount="3" %log="teve sua chance de crítico drasticamente aumentada!"]
			[include moveEatEffect %caster="enemy" %target="active" %number="62"]
			[include moveTypeChangeEffect %caster="enemy" %target="active" %number="63"]
			[include moveTypeChangeEffect %caster="enemy" %target="enemy" %number="64"]
			[include moveHealStatusEffect %caster="enemy" %target="enemy" %number="65" %log="se curou de seus status!"]
			[include moveStageResetEffect %caster="enemy" %target="enemy" %number="66" %log="zerou todos os seus modificadores!"]
			[include moveStageResetEffect %caster="enemy" %target="active" %number="(-66)" %log="teve todos os seus modificadores zerados!"]
			[include moveDirectDamageEffect %caster="enemy" %target="active" %number="67"]

			[include enemyMultipleHit %hit="2"]
			[include enemyMultipleHit %hit="3"]
			[include enemyMultipleHit %hit="4"]
			[include enemyMultipleHit %hit="5"]

			if (0>=activeCurHP)
				activeStatus=-1
			end

			enemyLastMove = $enemyMoveID
		end

//-------------------------------------------------------------------------------------------------//

		on activeconfusion:
			activeStatus = 0

			do getname with tag:active
			log se atacou por conta da confusão!

			[include onMove %caster="active" %target="active"]
			[include activeMoveTypeAdvantage %type="0"]
			[include activeMoveTypeAdvantage %type="1"]
			[include activeMoveTypeAdvantage %type="2"]
			[include activeMoveTypeAdvantage %type="3"]
			[include activeMoveTypeAdvantage %type="4"]
			[include activeMoveTypeAdvantage %type="5"]
			[include activeMoveTypeAdvantage %type="6"]
			[include activeMoveTypeAdvantage %type="7"]
			[include activeMoveTypeAdvantage %type="8"]
			[include activeMoveTypeAdvantage %type="9"]
			[include activeMoveTypeAdvantage %type="10"]
			[include activeMoveTypeAdvantage %type="11"]
			[include activeMoveTypeAdvantage %type="12"]
			[include activeMoveTypeAdvantage %type="13"]
			[include activeMoveTypeAdvantage %type="14"]
			[include activeMoveTypeAdvantage %type="15"]
			[include activeMoveTypeAdvantage %type="16"]
			[include activeMoveTypeAdvantage %type="17"]
			[include effectivenessVerification %caster="active" %target="active"]
			[include damageCalculation %caster="active" %target="active"]

			if ($activeMoveEffect=59)
				$activeMoveEffect = 25 + random(0,4)
			end
			if ($activeMoveEffect=60)
				$activeMoveEffect = 30 + random(0,4)
			end
			if ($activeMoveEffect=61)
				$activeMoveEffect = 35 + random(0,4)
			end

			[include moveEffect %who="active" %number="1"]
				$activeEffectiveHeal = ceil(min(($activeEffectiveDamage/2),(activeMaxHP-activeCurHP)))
				gain $activeEffectiveHeal activeCurHP
			[include announce %who="active" %log="drenou [$activeEffectiveHeal] pontos de vida!"]

			[include moveEffect %who="active" %number="2"]
				$activeEffectiveRecoil = ceil(min(($activeEffectiveDamage/4),activeCurHP))
				lose $activeEffectiveRecoil activeCurHP
			[include announce %who="active" %log="também se feriu, tomando [$activeEffectiveRecoil] dano!"]

			[include moveEffect %who="active" %number="3"]
				$activeEffectiveRecoil = ceil(min(($activeEffectiveDamage/3),activeCurHP))
				lose $activeEffectiveRecoil activeCurHP
			[include announce %who="active" %log="também se feriu, tomando [$activeEffectiveRecoil] dano!"]

			[include moveStatusEffect %caster="active" %target="active" %number="4" %status="1" %log="foi envenenado!"]
			[include moveStatusEffect %caster="active" %target="active" %number="5" %status="2" %log="foi intoxicado!"]
			[include moveStatusEffect %caster="active" %target="active" %number="6" %status="3" %log="está em chamas!"]
			[include moveStatusEffect %caster="active" %target="active" %number="7" %status="-2" %log="foi paralisado!"]
			if ((($activeMoveEffect=8) or ($activeMoveEffect2=8)) and (chance($activeMoveFxChance)) and ($activeAim = 1) and (activeStatus = 0))
				activeStatus = -3
				activeSleep = random(1,3)
			[include announce %who="active" %log="caiu no sono!"]
			[include moveStatusEffect %caster="active" %target="active" %number="9" %status="-4" %log="foi congelado!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="10" %status="Attack" %amount="1" %log="teve seu Ataque ligeiramente reduzido!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="11" %status="Defense" %amount="1" %log="teve sua Defesa ligeiramente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="12" %status="SpAttack" %amount="1" %log="teve seu Ataque Especial ligeiramente reduzido!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="13" %status="SpDefense" %amount="1" %log="teve sua Defesa Especial ligeiramente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="14" %status="Speed" %amount="1" %log="teve sua Velocidade ligeiramente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="15" %status="Attack" %amount="2" %log="teve seu Ataque duramente reduzido!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="16" %status="Defense" %amount="2" %log="teve sua Defesa duramente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="17" %status="SpAttack" %amount="2" %log="teve seu Ataque Especial duramente reduzido!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="18" %status="SpDefense" %amount="2" %log="teve sua Defesa Especial duramente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="19" %status="Speed" %amount="2" %log="teve sua Velocidade duramente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="20" %status="Attack" %amount="3" %log="teve seu Ataque severamente reduzido!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="21" %status="Defense" %amount="3" %log="teve sua Defesa severamente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="22" %status="SpAttack" %amount="3" %log="teve seu Ataque Especial severamente reduzido!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="23" %status="SpDefense" %amount="3" %log="teve sua Defesa Especial severamente reduzida!"]
			[include moveDebuffEffect %caster="active" %target="active" %number="24" %status="Speed" %amount="3" %log="teve sua Velocidade severamente reduzida!"]
			[include moveBuffEffect %caster="active" %target="active" %number="25" %status="Attack" %amount="1" %log="teve seu Ataque ligeiramente aumentado!"]
