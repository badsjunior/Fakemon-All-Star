//-----------------------------------------------Exploração---------------------------------------------------------------//

    *TEMPLATE
		tag:explore
		on click:
			log Um pokémon selvagem apareceu na sua frente enquanto explorava [n:this]!
			gain inBattle
			lose tag:enemy
			$rand = random(2)
			if (chance(0.1%))
				toast <#ff9900><b>LENDÁRIO!</b></#>
				do legend with this
			else if (chance(1%))
				toast <#cc33ff><b>EXÓTICO!</b></#>
				do exotic with this
			else if (chance(10%))
				toast <#00cccc><b>RARO!</b></#>
				do rare with this
			else if (chance(40%))
				toast <#4dffa6><b>INCOMUM!</b></#>
				do uncommon with this
			else if ($rand=0) 
				toast <#999999><b>COMUM!</b></#>
				do common1 with this
			else if ($rand=1) 
				toast <#999999><b>COMUM!</b></#>
				do common2 with this
			else if ($rand=2) 
				toast <#999999><b>COMUM!</b></#>
				do common3 with this
			end
		end
    *rota1
		name:Rota 1
		desc:Rota inicial de Pindoh. Com pokémon de lvl 2-6
		icon:exploration[0,0]
		on common1:
			enemyLvl = (4 + random(-2,2))
			gain ENMguarimple
		end
		on common2:
			enemyLvl = (4 + random(-2,2))
			gain ENMballdillo
		end
		on common3:
			enemyLvl = (4 + random(-2,2))
			gain ENMgossipavy
		end
		on uncommon:
			enemyLvl = (6 + random(-2,2))
			gain ENMguarimple
		end
		on rare:
			enemyLvl = (6 + random(-2,2))
			gain ENMballdillo
		end
		on exotic:
			enemyLvl = (8 + random(-2,2))
			gain ENMgossipavy
		end
    *TEMPLATE

//-------------------------------------------------Moves-----------------------------------------------------------------//
Upgrades
    *TEMPLATE
		tag:activeMove
		hidden when 0
		on click:
			if (no inBattle)
				log Você não pode usar <#ffcc00><b>[n:this]</b></#> fora de batalha!
			else
				clear log
				do select with this
				do iaplay with tag:enemy
				do beginturn with global
			end
		end	

    *aMOVtackle
		name:Investida
		on select:
			do getname with tag:active
			log se prepara para usar <#ffcc00><b>[n:this]</b></#>...
			$activeMovePriority = 0
			$activeMoveCrit = 6.25
			$activeMoveType = 0
			$activeMoveIsSpecial = 0
			$activeMovePower = 40
		end
    *TEMPLATE
		tags:enemyMove hidden
    *eMOVtackle
		name:Investida
		on select:
			do getname with tag:enemy
			log se prepara para usar <#ffcc00><b>[n:this]</b></#>...
			$enemyMovePriority = 0
			$enemyMoveCrit = 6.25
			$enemyMoveType = 0
			$enemyMoveIsSpecial = 0
			$enemyMovePower = 40
		end
    *TEMPLATE

//---------------------------------------------Pkm Capturados------------------------------------------------------------//

Buildings
    *TEMPLATE
        on earn:
			anim glow
		end
		on click:
			if ((no activePkm) and (switches>=1))
				activeLvl = this
				toast <#00cc00><b>[n:this]</b></#>, vai!
			end
		end
		text:[n:this] lvl [this]
		cost:10 xp
		tag:capturedTab
		hidden when 0
	*CPTguarimple
		name:Guarimple
		icon:pindohDex[1,0] pType[5,1] sType[5,1]
		on click:
			if ((no activePkm) and (switches>=1))
				switch = (switch-1)
				gain ATVguarimple
				gain activePkm
			end
		end
	*CPTballdillo
		name:Balldillo
		icon:pindohDex[4,0] pType[3,1] sType[2,1]
		on click:
			if ((no activePkm) and (switches>=1))
				switch = (switch-1)
				gain ATVballdillo
				gain activePkm
			end
		end
	*CPTgossipavy
		name:Gossipavy
		icon:pindohDex[7,0] pType[4,1] sType[0,0]
		on click:
			if ((no activePkm) and (switches>=1))
				switch = (switch-1)
				gain ATVgossipavy
				gain activePkm
			end
		end
    *TEMPLATE

//-----------------------------------------------Hidden------------------------------------------------------------------//

Upgrades
	*activePkm
	*inBattle
		on earn:
			do reset with tag:modifier
			do reset with tag:modifierEnemy
		end

//------------------------------------------------Inimigos----------------------------------------------------------------//
    *TEMPLATE
		desc:<//>HP: [enemyCurHP]/[enemyMaxHP] ([((enemyCurHP/enemyMaxHP)*100)]%) LVL: [enemyLvl]<//>[?(0.15>=(enemyCurHP/enemyMaxHP))|<#cc0000>|<#e6b800>][?((enemyCurHP/enemyMaxHP)>=0.4)|<#009900>|][?((enemyCurHP/enemyMaxHP)=1)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.95)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.9)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.85)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.8)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.75)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.7)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.65)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.6)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.55)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.5)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.45)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.4)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.35)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.3)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.25)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.2)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.15)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.1)|▓|░][?((enemyCurHP/enemyMaxHP)>=0.05)|▓|░]</#><//>Attack: [enemyAttack]<//>Defense: [enemyDefense]<//>Special Attack: [enemySpAttack]<//>Special Defense: [enemySpDefense]<//>Speed: [enemySpeed]
        class:bigButton hasFlares
		no text
		tag:enemy
		on getname:
			if (have this) log <#ff3300><b>[n:this]</b></#>
		end
		on faint:
			if (have this)
				$expEarned = (pow((enemyMaxHP + enemyBaseAttack + enemyBaseDefense + enemyBaseSpAttack + enemyBaseSpDefense + enemyBaseSpeed),2)/100)*pow(((2*enemyLvl)/(enemyLvl + activeLvl)),2)
				toast <#00cc00>Vitória!</#>
				log <#ff3300><b>[n:this]</b></#> foi derrotado, você ganhou [$expEarned] xp.
				gain $expEarned xp
				lose inBattle
				do reset with tag:baseEnemy
				enemyMaxHP = 0
				enemyCurHP = 0
				lose this
			end
		end
		hidden when 0
		on click:
			if (1>pokeballs)
				toast Você <#ff3300>não tem pokébolas</#> para capturar!
			else if (chance((enemyMaxHP*5)/(enemyCurHP)))
				pokeball = (pokeballs-1)
				toast Você capturou <#00cc00>[n:this]</#> lvl [enemyLvl]!
				do capture with this
				lose inBattle
				do reset with tag:baseEnemy
				enemyMaxHP = 0
				enemyCurHP = 0
				lose this
			else
				pokeball = (pokeballs-1)
				toast Você falhou em capturar <#ff3300>[n:this]</#> e ele fugiu da batalha!
				lose inBattle
				do reset with tag:baseEnemy
				enemyMaxHP = 0
				enemyCurHP = 0
				lose this
			end
		end
		on iaplay:
			if (have this) do select with eMOVtackle
		end
	*ENMguarimple
		name:Guarimple
		icon:https://i.imgur.com/Kco4d9D.png
		on earn:
			gain FNDguarimple
			enemyMaxHP = round(((70*frandom(0.9,1.1)*enemyLvl)/50)+enemyLvl+10)
			enemyCurHP = enemyMaxHP
			enemyBaseAttack = (((49*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseDefense = (((30*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpAttack = (((54*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpDefense = (((32*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpeed = (((75*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyPtype = 11
			enemyStype = 11
		end
		on capture:
			CPTguarimple = max(enemyLvl,CPTguarimple)
		end
	*ENMballdillo
		name:Balldillo
		icon:https://i.imgur.com/FNP4vop.png
		on earn:
			gain FNDballdillo
			enemyMaxHP = round(((58*frandom(0.9,1.1)*enemyLvl)/50)+enemyLvl+10)
			enemyCurHP = enemyMaxHP
			enemyBaseAttack = (((23*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseDefense = (((81*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpAttack = (((19*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpDefense = (((84*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpeed = (((45*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyPtype = 9
			enemyStype = 9
		end
		on capture:
			CPTballdillo = max(enemyLvl,CPTballdillo)
		end
	*ENMgossipavy
		name:Gossipavy
		icon:https://i.imgur.com/xDNs4ss.png
		on earn:
			gain FNDgossipavy
			enemyMaxHP = round(((76*frandom(0.9,1.1)*enemyLvl)/50)+enemyLvl+10)
			enemyCurHP = enemyMaxHP
			enemyBaseAttack = (((44*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseDefense = (((40*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpAttack = (((51*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpDefense = (((37*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyBaseSpeed = (((63*frandom(0.9,1.1)*enemyLvl)/50)+5)
			enemyPtype = 10
			enemyStype = 10
		end
		on capture:
			CPTgossipavy = max(enemyLvl,CPTgossipavy)
		end
//-------------------------------------------Pkm Ativos------------------------------------------------------------------//

    *TEMPLATE
        class:bigButton hasFlares
		no text
		tag:active
		on click:
			do return with this
		end
		on return:
			if (have this)
				toast <#00cc00><b>[n:this]</b></#>, já basta, volte!
				log <#00cc00><b>[n:this]</b></#> retornou para a pokébola, mas os pokémon selvagens fugiram!
				lose activePkm
				lose inBattle
				lose tag:activeMove
				lose tag:enemy
				do reset with tag:base
				activeMaxHP = 0
				activeCurHP = 0
				lose this
			end
		end
		on getname:
			if (have this) log <#00cc00><b>[n:this]</b></#>
		end
		hidden when 0		
	*ATVguarimple
		name:Guarimple
		icon:https://i.imgur.com/Kco4d9D.png
		on earn:
			activeMaxHP = round(((70*activeLvl)/50)+activeLvl+10)
			activeCurHP = activeMaxHP
			activeBaseAttack = (((49*activeLvl)/50)+5)
			activeBaseDefense = (((30*activeLvl)/50)+5)
			activeBaseSpAttack = (((54*activeLvl)/50)+5)
			activeBaseSpDefense = (((32*activeLvl)/50)+5)
			activeBaseSpeed = (((75*activeLvl)/50)+5)
			activePtype = 11
			activeStype = 11
			gain aMOVtackle
		end
	*ATVballdillo
		name:Balldillo
		icon:https://i.imgur.com/FNP4vop.png
		on earn:
			activeMaxHP = round(((58*activeLvl)/50)+activeLvl+10)
			activeCurHP = activeMaxHP
			activeBaseAttack = (((23*activeLvl)/50)+5)
			activeBaseDefense = (((81*activeLvl)/50)+5)
			activeBaseSpAttack = (((19*activeLvl)/50)+5)
			activeBaseSpDefense = (((84*activeLvl)/50)+5)
			activeBaseSpeed = (((45*activeLvl)/50)+5)
			activePtype = 9
