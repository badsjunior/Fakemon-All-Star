			[include multipleHit %caster="enemy" %target="active" %hit="3"]
			[include multipleHit %caster="enemy" %target="active" %hit="4"]
			[include multipleHit %caster="enemy" %target="active" %hit="5"]

			if (0>=activeCurHP)
				activeStatus=-1
			end

			enemyLastMove = $enemyMoveID
		end

//-------------------------------------------------------------------------------------------------//

		on activeconfusion:
			activeStatus = 0

			do getname with tag:active
			log se atacou por conta da confusão!

			[include checkFxConditions %caster="active" %target="active"]
			[include moveFullCopyEffect %caster="active" %target="active" %number="31"]
			[include moveFxCopyEffect %caster="active" %target="active" %number="32"]
			[include onMove %caster="active" %target="active"]
			[include activeMoveTypeAdvantage %type="0"]
			[include activeMoveTypeAdvantage %type="1"]
			[include activeMoveTypeAdvantage %type="2"]
			[include activeMoveTypeAdvantage %type="3"]
			[include activeMoveTypeAdvantage %type="4"]
			[include activeMoveTypeAdvantage %type="5"]
			[include activeMoveTypeAdvantage %type="6"]
			[include activeMoveTypeAdvantage %type="7"]
			[include activeMoveTypeAdvantage %type="8"]
			[include activeMoveTypeAdvantage %type="9"]
			[include activeMoveTypeAdvantage %type="10"]
			[include activeMoveTypeAdvantage %type="11"]
			[include activeMoveTypeAdvantage %type="12"]
			[include activeMoveTypeAdvantage %type="13"]
			[include activeMoveTypeAdvantage %type="14"]
			[include activeMoveTypeAdvantage %type="15"]
			[include activeMoveTypeAdvantage %type="16"]
			[include activeMoveTypeAdvantage %type="17"]
			[include effectivenessVerification %caster="active" %target="active"]
			[include damageCalculation %caster="active" %target="active"]
			if (($activeMoveEffect=13) or ($activeMoveEffect=23))
				$activeMoveEffect = $activeMoveEffect - random(1,8)
			end
			if (($activeMoveEffect=-13) or ($activeMoveEffect=-23))
				$activeMoveEffect = $activeMoveEffect + random(1,8)
			end
			[include moveDrainEffect %caster="active" %number="1"]
			[include moveRecoilEffect %caster="active" %number="2"]
			[include moveStatusEffect %caster="active" %target="active" %number="3"]
			[include moveHealStatusEffect %caster="active" %target="active" %number="4"]
			[include moveDebuffEffect %caster="active" %target="active" %number="5" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
			[include moveDebuffEffect %caster="active" %target="active" %number="6" %statusKey="Defense" %statusName="Defesa" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="7" %statusKey="SpAttack" %statusName="Ataque Especial" %gender="o" %gender2="seu"]
			[include moveDebuffEffect %caster="active" %target="active" %number="8" %statusKey="SpDefense" %statusName="Defesa Especial" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="9" %statusKey="Speed" %statusName="Velocidade" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="10" %statusKey="Accuracy" %statusName="Precisão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="11" %statusKey="Evasion" %statusName="Evasão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="12" %statusKey="Critical" %statusName="Chance de Crítico" %gender="a" %gender2="sua"]
			[include moveAllDebuffEffect %caster="active" %target="active" %number="14"]
			[include moveBuffEffect %caster="active" %target="active" %number="15" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
			[include moveBuffEffect %caster="active" %target="active" %number="16" %statusKey="Defense" %statusName="Defesa" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="17" %statusKey="SpAttack" %statusName="Ataque Especial" %gender="o" %gender2="seu"]
			[include moveBuffEffect %caster="active" %target="active" %number="18" %statusKey="SpDefense" %statusName="Defesa Especial" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="19" %statusKey="Speed" %statusName="Velocidade" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="20" %statusKey="Accuracy" %statusName="Precisão" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="21" %statusKey="Evasion" %statusName="Evasão" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="22" %statusKey="Critical" %statusName="Chance de Crítico" %gender="a" %gender2="sua"]
			[include moveAllBuffEffect %caster="active" %target="active" %number="24"]
			[include moveStageResetEffect %caster="active" %target="active" %number="25"]
			[include moveLoseBerryEffect %caster="active" %target="active" %number="26"]
			[include moveEatEffect %caster="active" %target="active" %number="27"]
			[include moveTypeChangeEffect %caster="active" %target="active" %number="28"]
			[include moveAbilityChangeEffect %caster="active" %target="active" %number="29"]
			[include moveDirectDamageEffect %caster="active" %target="active" %number="30"]
			[include moveHealStatusEffect %caster="active" %target="active" %number="-4"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-5" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-6" %statusKey="Defense" %statusName="Defesa" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-7" %statusKey="SpAttack" %statusName="Ataque Especial" %gender="o" %gender2="seu"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-8" %statusKey="SpDefense" %statusName="Defesa Especial" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-9" %statusKey="Speed" %statusName="Velocidade" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-10" %statusKey="Accuracy" %statusName="Precisão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-11" %statusKey="Evasion" %statusName="Evasão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-12" %statusKey="Critical" %statusName="Chance de Crítico" %gender="a" %gender2="sua"]
			[include moveAllDebuffEffect %caster="active" %target="active" %number="-14"]
			[include moveBuffEffect %caster="active" %target="active" %number="-15" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
			[include moveBuffEffect %caster="active" %target="active" %number="-16" %statusKey="Defense" %statusName="Defesa" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="-17" %statusKey="SpAttack" %statusName="Ataque Especial" %gender="o" %gender2="seu"]
			[include moveBuffEffect %caster="active" %target="active" %number="-18" %statusKey="SpDefense" %statusName="Defesa Especial" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="-19" %statusKey="Speed" %statusName="Velocidade" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="-20" %statusKey="Accuracy" %statusName="Precisão" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="-21" %statusKey="Evasion" %statusName="Evasão" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="-22" %statusKey="Critical" %statusName="Chance de Crítico" %gender="a" %gender2="sua"]
			[include moveAllBuffEffect %caster="active" %target="active" %number="-24"]
			[include moveStageResetEffect %caster="active" %target="active" %number="-25"]
			[include moveLoseBerryEffect %caster="active" %target="active" %number="-26"]
			[include moveEatEffect %caster="active" %target="active" %number="-27"]
			[include moveTypeChangeEffect %caster="active" %target="active" %number="-28"]
			[include moveAbilityChangeEffect %caster="active" %target="active" %number="-29"]
			[include moveDirectDamageEffect %caster="active" %target="active" %number="-30"]

			[include multipleHit %caster="active" %target="active" %hit="2"]
			[include multipleHit %caster="active" %target="active" %hit="3"]
			[include multipleHit %caster="active" %target="active" %hit="4"]
			[include multipleHit %caster="active" %target="active" %hit="5"]

			if (0>=activeCurHP)
				activeStatus=-1
			end

			do getname with tag:active
			log não está mais confuso.

			activeLastMove = $activeMoveID
		end

//-------------------------------------------------------------------------------------------------//

		on enemyconfusion:
			enemyStatus = 0

			do getname with tag:enemy
			log se atacou por conta da confusão!

			[include checkFxConditions %caster="enemy" %target="enemy"]
			[include moveFullCopyEffect %caster="enemy" %target="enemy" %number="31"]
			[include moveFxCopyEffect %caster="enemy" %target="enemy" %number="32"]
			[include onMove %caster="enemy" %target="enemy"]
			[include enemyMoveTypeAdvantage %type="0"]
			[include enemyMoveTypeAdvantage %type="1"]
			[include enemyMoveTypeAdvantage %type="2"]
			[include enemyMoveTypeAdvantage %type="3"]
			[include enemyMoveTypeAdvantage %type="4"]
			[include enemyMoveTypeAdvantage %type="5"]
			[include enemyMoveTypeAdvantage %type="6"]
			[include enemyMoveTypeAdvantage %type="7"]
			[include enemyMoveTypeAdvantage %type="8"]
			[include enemyMoveTypeAdvantage %type="9"]
			[include enemyMoveTypeAdvantage %type="10"]
			[include enemyMoveTypeAdvantage %type="11"]
			[include enemyMoveTypeAdvantage %type="12"]
			[include enemyMoveTypeAdvantage %type="13"]
			[include enemyMoveTypeAdvantage %type="14"]
			[include enemyMoveTypeAdvantage %type="15"]
			[include enemyMoveTypeAdvantage %type="16"]
			[include enemyMoveTypeAdvantage %type="17"]
			[include effectivenessVerification %caster="enemy" %target="enemy"]
			[include damageCalculation %caster="enemy" %target="enemy"]
			if (($enemyMoveEffect=13) or ($enemyMoveEffect=23))
				$enemyMoveEffect = $enemyMoveEffect - random(1,8)
			end
			if (($enemyMoveEffect=-13) or ($enemyMoveEffect=-23))
				$enemyMoveEffect = $enemyMoveEffect + random(1,8)
			end
			[include moveDrainEffect %caster="enemy" %number="1"]
			[include moveRecoilEffect %caster="enemy" %number="2"]
			[include moveStatusEffect %caster="enemy" %target="enemy" %number="3"]
			[include moveHealStatusEffect %caster="enemy" %target="enemy" %number="4"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="5" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="6" %statusKey="Defense" %statusName="Defesa" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="7" %statusKey="SpAttack" %statusName="Ataque Especial" %gender="o" %gender2="seu"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="8" %statusKey="SpDefense" %statusName="Defesa Especial" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="9" %statusKey="Speed" %statusName="Velocidade" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="10" %statusKey="Accuracy" %statusName="Precisão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="11" %statusKey="Evasion" %statusName="Evasão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="12" %statusKey="Critical" %statusName="Chance de Crítico" %gender="a" %gender2="sua"]
			[include moveAllDebuffEffect %caster="enemy" %target="enemy" %number="14"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="15" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="16" %statusKey="Defense" %statusName="Defesa" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="17" %statusKey="SpAttack" %statusName="Ataque Especial" %gender="o" %gender2="seu"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="18" %statusKey="SpDefense" %statusName="Defesa Especial" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="19" %statusKey="Speed" %statusName="Velocidade" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="20" %statusKey="Accuracy" %statusName="Precisão" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="21" %statusKey="Evasion" %statusName="Evasão" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="22" %statusKey="Critical" %statusName="Chance de Crítico" %gender="a" %gender2="sua"]
			[include moveAllBuffEffect %caster="enemy" %target="enemy" %number="24"]
			[include moveStageResetEffect %caster="enemy" %target="enemy" %number="25"]
			[include moveLoseBerryEffect %caster="enemy" %target="enemy" %number="26"]
			[include moveEatEffect %caster="enemy" %target="enemy" %number="27"]
			[include moveTypeChangeEffect %caster="enemy" %target="enemy" %number="28"]
			[include moveAbilityChangeEffect %caster="enemy" %target="enemy" %number="29"]
			[include moveDirectDamageEffect %caster="enemy" %target="enemy" %number="30"]
			[include moveHealStatusEffect %caster="enemy" %target="enemy" %number="-4"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="-5" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="-6" %statusKey="Defense" %statusName="Defesa" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="-7" %statusKey="SpAttack" %statusName="Ataque Especial" %gender="o" %gender2="seu"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="-8" %statusKey="SpDefense" %statusName="Defesa Especial" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="-9" %statusKey="Speed" %statusName="Velocidade" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="-10" %statusKey="Accuracy" %statusName="Precisão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="-11" %statusKey="Evasion" %statusName="Evasão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="enemy" %target="enemy" %number="-12" %statusKey="Critical" %statusName="Chance de Crítico" %gender="a" %gender2="sua"]
			[include moveAllDebuffEffect %caster="enemy" %target="enemy" %number="-14"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="-15" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="-16" %statusKey="Defense" %statusName="Defesa" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="-17" %statusKey="SpAttack" %statusName="Ataque Especial" %gender="o" %gender2="seu"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="-18" %statusKey="SpDefense" %statusName="Defesa Especial" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="-19" %statusKey="Speed" %statusName="Velocidade" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="-20" %statusKey="Accuracy" %statusName="Precisão" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="-21" %statusKey="Evasion" %statusName="Evasão" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="enemy" %target="enemy" %number="-22" %statusKey="Critical" %statusName="Chance de Crítico" %gender="a" %gender2="sua"]
			[include moveAllBuffEffect %caster="enemy" %target="enemy" %number="-24"]
			[include moveStageResetEffect %caster="enemy" %target="enemy" %number="-25"]
			[include moveLoseBerryEffect %caster="enemy" %target="enemy" %number="-26"]
			[include moveEatEffect %caster="enemy" %target="enemy" %number="-27"]
			[include moveTypeChangeEffect %caster="enemy" %target="enemy" %number="-28"]
			[include moveAbilityChangeEffect %caster="enemy" %target="enemy" %number="-29"]
			[include moveDirectDamageEffect %caster="enemy" %target="enemy" %number="-30"]

			[include multipleHit %caster="enemy" %target="enemy" %hit="2"]
			[include multipleHit %caster="enemy" %target="enemy" %hit="3"]
			[include multipleHit %caster="enemy" %target="enemy" %hit="4"]
			[include multipleHit %caster="enemy" %target="enemy" %hit="5"]

			if (0>=enemyCurHP)
				enemyStatus=-1
			end

			do getname with tag:enemy
			log não está mais confuso.

			enemyLastMove = $enemyMoveID
		end

//-------------------------------------------------------------------------------------------------//

		on activefaint:
			do getname with tag:active
			log está fora de batalha!
			do return with tag:active
		end
		on enemyfaint:
			do getname with tag:enemy
			log está fora de batalha!
			do faint with tag:enemy
		end

		on activeready:
			do getname with tag:active
			log usou este turno para se recuperar do esforço e agora está pronto para voltar para o combate!
			activeStatus = 0
		end
		on enemyready:
			do getname with tag:enemy
			log usou este turno para se recuperar do esforço e agora está pronto para voltar para o combate!
			activeStatus = 0
		end

		on activefatigue:
			do getname with tag:active
			log está se recuperando do esforço que fez no último golpe!
			activeStatusTimer = (activeStatusTimer-1)
		end
		on enemyfatigue:
			do getname with tag:enemy
			log está se recuperando do esforço que fez no último golpe!
			activeStatusTimer = (activeStatusTimer-1)
		end

		on activeflee:
			do getname with tag:active
			log deixou a batalha!
			do return with tag:active
		end
		on enemyflee:
			do getname with tag:enemy
			log deixou a batalha!
			lose tag:enemy
		end

		on activeflinch:
			do getname with tag:active
			log hesitou em atacar!
			activeStatus = 0
		end
		on enemyflinch:
			do getname with tag:enemy
			log hesitou em atacar!
			activeStatus = 0
		end

		on activethaw:
			do getname with tag:active
			log descongelou!
			activeStatus = 0
		end
		on enemythaw:
			do getname with tag:enemy
			log descongelou!
			activeStatus = 0
		end

		on activefreeze:
			do getname with tag:active
			log está dentro de uma pedra de gelo!
		end
		on enemyfreeze:
			do getname with tag:enemy
			log está dentro de uma pedra de gelo!
		end

		on activewake:
			do getname with tag:active
			log acordou!
			activeStatus = 0
		end
		on enemywake:
			do getname with tag:enemy
			log acordou!
			activeStatus = 0
		end

		on activesleep:
			do getname with tag:active
			log está dormindo!
			activeStatusTimer = (activeStatusTimer-1)
		end
		on enemysleep:
			do getname with tag:enemy
			log está dormindo!
			activeStatusTimer = (activeStatusTimer-1)
		end

		on activeparalysis:
			do getname with tag:active
			log não conseguiu se mover por causa da paralisia!
		end
		on enemyparalysis:
			do getname with tag:enemy
			log não conseguiu se mover por causa da paralisia!
		end

		on activepoison:
			$activePoisonDamage = round(min((activeMaxHP/8),activeCurHP))
			lose $activePoisonDamage activeCurHP
			do getname with tag:active
			log tomou [$activePoisonDamage] dano por envenenamento!
		end
		on enemypoison:
			$enemyPoisonDamage = round(min((enemyMaxHP/8),enemyCurHP))
			lose $enemyPoisonDamage enemyCurHP
			do getname with tag:enemy
			log tomou [$enemyPoisonDamage] dano por envenenamento!
		end

		on activebadpoison:
			$activePoisonDamage = round(min((activeBadPoison*activeMaxHP/16),activeCurHP))
			lose $activePoisonDamage activeCurHP
			do getname with tag:active
			log tomou [$activePoisonDamage] dano por intoxicação!
		end
		on enemybadpoison:
			$enemyPoisonDamage = round(min((enemyBadPoison*enemyMaxHP/8),enemyCurHP))
			lose $enemyPoisonDamage enemyCurHP
			do getname with tag:enemy
			log tomou [$enemyPoisonDamage] dano por intoxicação!
		end

		on activeburn:
			$activePoisonDamage = round(min((activeMaxHP/16),activeCurHP))
			lose $activePoisonDamage activeCurHP
			do getname with tag:active
			log tomou [$activePoisonDamage] dano por queimadura!
		end
		on enemyburn:
			$enemyPoisonDamage = round(min((enemyMaxHP/16),enemyCurHP))
			lose $enemyPoisonDamage enemyCurHP
			do getname with tag:enemy
			log tomou [$enemyPoisonDamage] dano por queimadura!
		end

		on endturn:
			if (activeStatus=1) do activepoison with this
			if (enemyStatus=1) do enemypoison with this
			if (activeStatus=2) do activebadpoison with this
			if (enemyStatus=2) do enemybadpoison with this
			if (activeStatus=3) do activeburn with this
			if (enemyStatus=3) do enemyburn with this
			if ((activeStatus=(-3)) and (0>=activeStatusTimer)) do activewake with this
			if ((enemyStatus=(-3)) and (0>=enemyStatusTimer)) do enemywake with this
			if ((activeStatus=(-3)) and (activeStatusTimer>0)) do activesleep with this
			if ((enemyStatus=(-3)) and (enemyStatusTimer>0)) do enemysleep with this
			if ((activeStatus=(-8)) and (0>=activeStatusTimer)) do activeready with this
			if ((enemyStatus=(-8)) and (0>=enemyStatusTimer)) do enemyready with this
			if ((activeStatus=(-8)) and (activeStatusTimer>0)) do activefatigue with this
			if ((enemyStatus=(-8)) and (enemyStatusTimer>0)) do enemyfatigue with this
			$fifth = random(1,5)
			$thaw = 0
			if ($fifth=1) $thaw=1
			if ((activeStatus=(-4)) and ($thaw=0)) do activefreeze with this
			if ((enemyStatus=(-4)) and ($thaw=0)) do enemyfreeze with this
			if ((activeStatus=(-4)) and ($thaw=1)) do activethaw with this
			if ((enemyStatus=(-4)) and ($thaw=1)) do enemythaw with this
			if (activeStatus=(-5)) do activeflinch with this
			if (enemyStatus=(-5)) do enemyflinch with this
			if (activeStatus=(-6)) do activeflee with this
			if (enemyStatus=(-6)) do enemyflee with this

			[include defModByStage %stage="(-6)" %mod="0.25"]
			[include defModByStage %stage="(-5)" %mod="0.2857"]
			[include defModByStage %stage="(-4)" %mod="0.3333"]
			[include defModByStage %stage="(-3)" %mod="0.4"]
			[include defModByStage %stage="(-2)" %mod="0.5"]
			[include defModByStage %stage="(-1)" %mod="0.6666"]
			[include defModByStage %stage="0" %mod="1"]
			[include defModByStage %stage="1" %mod="1.5"]
			[include defModByStage %stage="2" %mod="2"]
			[include defModByStage %stage="3" %mod="2.5"]
			[include defModByStage %stage="4" %mod="3"]
			[include defModByStage %stage="5" %mod="3.5"]
			[include defModByStage %stage="6" %mod="4"]

			[include defAcEvModByStage %stage="(-6)" %mod="0.3333"]
			[include defAcEvModByStage %stage="(-5)" %mod="0.375"]
			[include defAcEvModByStage %stage="(-4)" %mod="0.4286"]
			[include defAcEvModByStage %stage="(-3)" %mod="0.5"]
			[include defAcEvModByStage %stage="(-2)" %mod="0.6"]
			[include defAcEvModByStage %stage="(-1)" %mod="0.75"]
			[include defAcEvModByStage %stage="0" %mod="1"]
			[include defAcEvModByStage %stage="1" %mod="1.3333"]
			[include defAcEvModByStage %stage="2" %mod="1.6666"]
			[include defAcEvModByStage %stage="3" %mod="2"]
			[include defAcEvModByStage %stage="4" %mod="2.3333"]
			[include defAcEvModByStage %stage="5" %mod="2.6666"]
			[include defAcEvModByStage %stage="6" %mod="3"]

			if ((enemyBerry=1) and (enemyStatus=(-2))) do enemyeat with BER1
			if ((enemyBerry=2) and (enemyStatus=(-3))) do enemyeat with BER2
			if ((enemyBerry=3) and (enemyStatus=1)) do enemyeat with BER3
			if ((enemyBerry=4) and (enemyStatus=3)) do enemyeat with BER4
			if ((enemyBerry=5) and (enemyStatus=(-4))) do enemyeat with BER5
			if ((enemyBerry=6) and (enemyStatus=(-7))) do enemyeat with BER6
			if ((enemyBerry=7) and (!((enemyStatus=(-1)) or (enemyStatus=0)))) do enemyeat with BER7
			if ((enemyBerry=8) and (!(enemyStatus=(-1))) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER8
			if ((enemyBerry=9) and (!(enemyStatus=(-1))) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER9
			if ((enemyBerry=10) and (!(enemyStatus=(-1))) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER10
			if ((enemyBerry=11) and (!(enemyStatus=(-1))) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER11
			if ((enemyBerry=12) and (!(enemyStatus=(-1))) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER12
			if ((enemyBerry=13) and (!(enemyStatus=(-1))) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER13

			[include triggerPeriodicAbilities %caster="active" %target="enemy"]
			[include triggerPeriodicAbilities %caster="enemy" %target="active"]

			if ((activeStatus=(-1)) or (0>=round(activeCurHP))) do activefaint with this
			if ((enemyStatus=(-1)) or (0>=round(enemyCurHP))) do enemyfaint with this
		end

		on type0advantage:
			$auxAdvg = 1
			if (($auxType=5) or ($auxType=8)) $auxAdvg = 0.5
			if ($auxType=7) $auxAdvg = 0
		end
		on type1advantage:
			$auxAdvg = 1
			if (($auxType=0) or ($auxType=5) or ($auxType=8) or ($auxType=14) or ($auxType=16)) $auxAdvg = 2
			if (($auxType=2) or ($auxType=3) or ($auxType=6) or ($auxType=13) or ($auxType=17)) $auxAdvg = 0.5
			if ($auxType=7) $auxAdvg = 0
		end
		on type2advantage:
			$auxAdvg = 1
			if (($auxType=1) or ($auxType=6) or ($auxType=11)) $auxAdvg = 2
			if (($auxType=5) or ($auxType=8) or ($auxType=12)) $auxAdvg = 0.5
		end
		on type3advantage:
			$auxAdvg = 1
			if (($auxType=11) or ($auxType=17)) $auxAdvg = 2
			if (($auxType=3) or ($auxType=4) or ($auxType=5) or ($auxType=7)) $auxAdvg = 0.5
			if ($auxType=8) $auxAdvg = 0
		end
		on type4advantage:
			$auxAdvg = 1
			if (($auxType=3) or ($auxType=5) or ($auxType=8) or ($auxType=9) or ($auxType=12)) $auxAdvg = 2
			if (($auxType=6) or ($auxType=11)) $auxAdvg = 0.5
			if ($auxType=2) $auxAdvg = 0
		end
		on type5advantage:
			$auxAdvg = 1
			if (($auxType=2) or ($auxType=6) or ($auxType=9) or ($auxType=14)) $auxAdvg = 2
			if (($auxType=1) or ($auxType=4) or ($auxType=8)) $auxAdvg = 0.5
		end
		on type6advantage:
			$auxAdvg = 1
			if (($auxType=11) or ($auxType=13) or ($auxType=16)) $auxAdvg = 2
			if (($auxType=1) or ($auxType=2) or ($auxType=3) or ($auxType=7) or ($auxType=8) or ($auxType=9) or ($auxType=17)) $auxAdvg = 0.5
		end
		on type7advantage:
			$auxAdvg = 1
			if (($auxType=7) or ($auxType=13)) $auxAdvg = 2
			if ($auxType=16) $auxAdvg = 0.5
			if ($auxType=0) $auxAdvg = 0
		end
		on type8advantage:
			$auxAdvg = 1
			if (($auxType=5) or ($auxType=14) or ($auxType=17)) $auxAdvg = 2
			if (($auxType=8) or ($auxType=9) or ($auxType=10) or ($auxType=12)) $auxAdvg = 0.5
		end
		on type9advantage:
			$auxAdvg = 1
			if (($auxType=6) or ($auxType=8) or ($auxType=11) or ($auxType=14)) $auxAdvg = 2
			if (($auxType=5) or ($auxType=9) or ($auxType=10) or ($auxType=15)) $auxAdvg = 0.5
		end
		on type10advantage:
			$auxAdvg = 1
			if (($auxType=4) or ($auxType=5) or ($auxType=9)) $auxAdvg = 2
			if (($auxType=10) or ($auxType=11) or ($auxType=15)) $auxAdvg = 0.5
		end
		on type11advantage:
			$auxAdvg = 1
			if (($auxType=4) or ($auxType=5) or ($auxType=10)) $auxAdvg = 2
			if (($auxType=2) or ($auxType=3) or ($auxType=6) or ($auxType=8) or ($auxType=9) or ($auxType=11) or ($auxType=15)) $auxAdvg = 0.5
		end
		on type12advantage:
			$auxAdvg = 1
			if (($auxType=2) or ($auxType=10)) $auxAdvg = 2
			if (($auxType=11) or ($auxType=12) or ($auxType=15)) $auxAdvg = 0.5
			if ($auxType=4) $auxAdvg = 0
		end
		on type13advantage:
			$auxAdvg = 1
			if (($auxType=1) or ($auxType=3)) $auxAdvg = 2
			if (($auxType=8) or ($auxType=13)) $auxAdvg = 0.5
			if ($auxType=0) $auxAdvg = 0
		end
		on type14advantage:
			$auxAdvg = 1
			if (($auxType=2) or ($auxType=4) or ($auxType=11) or ($auxType=15)) $auxAdvg = 2
			if (($auxType=8) or ($auxType=9) or ($auxType=10) or ($auxType=14)) $auxAdvg = 0.5
		end
		on type15advantage:
			$auxAdvg = 1
			if ($auxType=15) $auxAdvg = 2
			if ($auxType=8) $auxAdvg = 0.5
			if ($auxType=17) $auxAdvg = 0
		end
		on type16advantage:
			$auxAdvg = 1
			if (($auxType=7) or ($auxType=13)) $auxAdvg = 2
			if (($auxType=1) or ($auxType=16) or ($auxType=17)) $auxAdvg = 0.5
		end
		on type17advantage:
			$auxAdvg = 1
			if (($auxType=1) or ($auxType=15) or ($auxType=16)) $auxAdvg = 2
			if (($auxType=3) or ($auxType=8) or ($auxType=9)) $auxAdvg = 0.5
		end
//		on type#advantage:
//			$auxAdvg = 1
//			if (($auxType=5) or ($auxType=8)) $auxAdvg = 2
