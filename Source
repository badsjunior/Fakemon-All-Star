	if ((enemyCurHP>0) and (enemyCurHP>0) and ($enemyMoveClass>=[%hit]) and ($numberOfHits>=[%hit]) and ($enemyAim = 1))
		do getname with tag:enemy
		log atacou pela [%hit]ª vez...
		$enemyCrit = 1
		$enemyRandomVar = frandom(0.85,1)
	end

	if ((enemyCurHP>0) and (enemyCurHP>0) and ($enemyMoveClass>=[%hit]) and ($numberOfHits>=[%hit]) and (chance($enemyMoveCrit)) and ($enemyAim = 1))
		$enemyCrit = 1.5
		log ... e realizou um golpe crítico!
	end

	if ((enemyCurHP>0) and (enemyCurHP>0) and ($enemyMoveClass>=[%hit]) and ($numberOfHits>=[%hit]) and ($enemyAim = 1))
		$enemyDamageMods = $enemyTypeAdvg*$enemyRandomVar*$enemySTAB*$enemyCrit*$enemyStatusMod
		$enemyEffectiveDamage = round(min(enemyCurHP,($enemyRawDamage*$enemyDamageMods)))
		lose $enemyEffectiveDamage enemyCurHP
		do getname with tag:enemy
		log tomou [$enemyEffectiveDamage] dano!
		log <#3366ff>-----------------------------------</#>
	end

*include defModByStage %stage %mod
	if (activeAttackStage=[%stage]) activeAttackMod = [%mod]
	if (activeDefenseStage=[%stage]) activeDefenseMod = [%mod]
	if (activeSpAttackStage=[%stage]) activeSpAttackMod = [%mod]
	if (activeSpDefenseStage=[%stage]) activeSpDefenseMod = [%mod]
	if (activeSpeedStage=[%stage])activeSpeedMod = [%mod]
	if (enemyAttackStage=[%stage]) enemyAttackMod = [%mod]
	if (enemyDefenseStage=[%stage]) enemyDefenseMod = [%mod]
	if (enemySpAttackStage=[%stage]) enemySpAttackMod = [%mod]
	if (enemySpDefenseStage=[%stage]) enemySpDefenseMod = [%mod]
	if (enemySpeedStage=[%stage]) enemySpeedMod = [%mod]

*include defAcEvModByStage %stage %mod
	if (activeAccuracyStage=[%stage]) activeAccuracyMod = [%mod]
	if (activeEvasionStage=(-[%stage])) activeEvasionMod = [%mod]
	if (enemyAccuracyStage=[%stage]) enemyAccuracyMod = [%mod]
	if (enemyEvasionStage=(-[%stage])) enemyEvasionMod = [%mod]

*include capturedNoIcon %name %number %region %variation %evoLvl %evoNumber
	*CPT[%number]from[%region]var[%variation]
		name:[%name]
		on click:
			if (([%evoLvl]>0) and (this>=[%evoLvl]) and (CPT[%evoNumber]from[%region]var[%variation]=0))
				toast <#00cc00><b>[n:this]</b></#> evoluiu para <#00cc00><b>[n:CPT[%evoNumber]from[%region]var[%variation]]</b></#>!
				gain FND[%evoNumber]from[%region]var[%variation]
				hide FND[%evoNumber]from[%region]var[%variation]
				CPT[%evoNumber]from[%region]var[%variation] = this
				this = 0
			end
			if ((this>0) and (no activePkm) and (switches>=1))
				switch = (switch-1)
				activeLvl = this
				gain ATV[%number]from[%region]var[%variation]
				toast <#00cc00><b>[n:this]</b></#>, vai!
			end
		end

*include placeNoIcon %id %region %name %desc %common1Lvl %common1Range %common1PkmNum %common1PkmVar %common2Lvl %common2Range %common2PkmNum %common2PkmVar %common3Lvl %common3Range %common3PkmNum %common3PkmVar %uncommonLvl %uncommonRange %uncommonPkmNum %uncommonPkmVar %rareLvl %rareRange %rarePkmNum %rarePkmVar %exoticLvl %exoticRange %exoticPkmNum %exoticPkmVar %legendLvl %legendRange %legendPkmNum %legendPkmVar
    *place[%id]from[%region]
		name:[%name]
		desc:[%desc]
		icon:exploration[0,0]
		on common1:
			enemyLvl = ([%common1Lvl] + random(-[%common1Range],[%common1Range]))
			gain ENM[%common1PkmNum]from[%region]var[%common1PkmVar]
		end
		on common2:
			enemyLvl = ([%common2Lvl] + random(-[%common2Range],[%common2Range]))
			gain ENM[%common2PkmNum]from[%region]var[%common2PkmVar]
		end
		on common3:
			enemyLvl = ([%common3Lvl] + random(-[%common3Range],[%common3Range]))
			gain ENM[%common3PkmNum]from[%region]var[%common3PkmVar]
		end
		on uncommon:
			enemyLvl = ([%uncommonLvl] + random(-[%uncommonRange],[%uncommonRange]))
			gain ENM[%uncommonPkmNum]from[%region]var[%uncommonPkmVar]
		end
		on rare:
			enemyLvl = ([%rareLvl] + random(-[%rareRange],[%rareRange]))
			gain ENM[%rarePkmNum]from[%region]var[%rarePkmVar]
		end
		on exotic:
			enemyLvl = ([%exoticLvl] + random(-[%exoticRange],[%exoticRange]))
			gain ENM[%exoticPkmNum]from[%region]var[%exoticPkmVar]
		end
		on legend:
			enemyLvl = ([%legendLvl] + random(-[%legendRange],[%legendRange]))
			gain ENM[%legendPkmNum]from[%region]var[%legendPkmVar]
		end

*include moveEffect %who %number
	if (((($[%who]MoveEffect=[%number]) and (chance($[%who]MoveFxChance))) or (($[%who]MoveEffect2=[%number]) and (chance($[%who]MoveFxChance2)))) and ($[%who]Aim = 1))

*include conditionalMoveEffect %who %number %condition
	if (((($[%who]MoveEffect=[%number]) and (chance($[%who]MoveFxChance))) or (($[%who]MoveEffect2=[%number]) and (chance($[%who]MoveFxChance2)))) and ($[%who]Aim = 1) and ([%condition]))

*include moveStatusEffect %caster %target %number %status %log
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Status = 0))
		[%target]Status = [%status]
		do getname with tag:[%target]
		log [%log]
		log <#3366ff>-----------------------------------</#>
	end

*include moveHealStatusEffect %caster %target %number %status %log
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1))
		[%target]Status = 0
		do getname with tag:[%target]
		log [%log]
		log <#3366ff>-----------------------------------</#>
	end

*include moveDebuffEffect %caster %target %number %status %amount %log
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1))
		[%target][%status]Stage = max((-6),([%target][%status]Stage-[%amount]))
		do getname with tag:[%target]
		log [%log]
		log <#3366ff>-----------------------------------</#>
	end

*include moveBuffEffect %caster %target %number %status %amount %log
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1))
		[%target][%status]Stage = min(6,([%target][%status]Stage+[%amount]))
		do getname with tag:[%target]
		log [%log]
		log <#3366ff>-----------------------------------</#>
	end

*include moveStageResetEffect %caster %target %number %log
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1))
		[%target]AttackStage = 0
		[%target]DefenseStage = 0
		[%target]SpAttackStage = 0
		[%target]SpDefenseStage = 0
		[%target]SpeedStage = 0
		[%target]AccuracyStage = 0
		[%target]EvasionStage = 0
		[%target]CriticalStage = 0
		do getname with tag:[%target]
		log [%log]
		log <#3366ff>-----------------------------------</#>
	end

*include moveDirectDamageEffect %caster %target %number
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1))
		$[%caster]EffectiveDamage = round(min([%target]CurHP,$[%caster]MovePower))
		lose $[%caster]EffectiveDamage [%target]CurHP
		do getname with tag:[%target]
		log tomou [$[%caster]EffectiveDamage] dano!
		log <#3366ff>-----------------------------------</#>
	end

*include moveEatEffect %caster %target %number
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Berry=1))
		 do [%caster]bite with BER1
	end
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Berry=2))
		 do [%caster]bite with BER2
	end
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Berry=3))
		 do [%caster]bite with BER3
	end
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Berry=4))
		 do [%caster]bite with BER4
	end
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Berry=5))
		 do [%caster]bite with BER5
	end
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Berry=6))
		 do [%caster]bite with BER6
	end
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Berry=7))
		 do [%caster]bite with BER7
	end
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Berry=8))
		 do [%caster]bite with BER8
	end
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Berry=9))
		 do [%caster]bite with BER9
	end
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Berry=10))
		 do [%caster]bite with BER10
	end
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Berry=11))
		 do [%caster]bite with BER11
	end
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Berry=12))
		 do [%caster]bite with BER12
	end
	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1) and ([%target]Berry=13))
		 do [%caster]bite with BER13
	end

*include moveTypeChangeEffect %caster %target %number
	if (($[%caster]MoveEffect=[%number]) or ($[%caster]MoveEffect2=[%number]) and ($[%caster]MovePower=(-1)))
		$[%caster]MovePower = [%caster]Ptype
	end
	if (($[%caster]MoveEffect=[%number]) or ($[%caster]MoveEffect2=[%number]) and ($[%caster]MovePower=(-2)))
		$[%caster]MovePower = [%target]Ptype
	end

	if (((($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance))) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)))) and ($[%caster]Aim = 1))
		[%target]Ptype = $[%caster]MovePower
		[%target]Stype = $[%caster]MovePower
		do getname with tag:[%target]
		log mudou de tipo, agora ele é [?[%target]Ptype=0|Normal|][?[%target]Ptype=1|Lutador|][?[%target]Ptype=2|Voador|][?[%target]Ptype=3|Venenoso|][?[%target]Ptype=4|Terrestre|][?[%target]Ptype=5|de Pedra|][?[%target]Ptype=6|Inseto|][?[%target]Ptype=7|Fantasma|][?[%target]Ptype=8|Metálico|][?[%target]Ptype=9|de Fogo|][?[%target]Ptype=10|Aquático|][?[%target]Ptype=11|Planta|][?[%target]Ptype=12|Elétrico|][?[%target]Ptype=13|Psíquico|][?[%target]Ptype=14|de Gelo|][?[%target]Ptype=15|Dragão|][?[%target]Ptype=16|Noturno|][?[%target]Ptype=17|Fada|]
		log <#3366ff>-----------------------------------</#>
	end

*include announce %who %log
		do getname with tag:[%who]
		log [%log]
		log <#3366ff>-----------------------------------</#>
	end

*include onMove %caster %target
	$[%caster]Aim = 1

	if ($[%caster]MoveAccuracy>=0)
		$[%caster]Aim = chance(($[%caster]MoveAccuracy*[%caster]AccuracyMod*[%target]EvasionMod)*100)
	end

	if ($[%caster]Aim = 0)
		do getname with tag:[%caster]
		log errou o golpe!
		[%caster]MovesInARow = 0
	end

	if (($[%caster]MoveID=[%caster]LastMove) and ($[%caster]Aim = 1))
		[%caster]MovesInARow = ([%caster]MovesInARow+1)
	end
	if ((!($[%caster]MoveID=[%caster]LastMove)) and ($[%caster]Aim = 1))
		[%caster]MovesInARow = 1
	end

	if (($[%caster]MoveClass>=0) and ($[%caster]Aim = 1))
		do getname with tag:[%caster]
		log atacou...
		$[%caster]STAB = 1
		$[%caster]Crit = 1
		$[%caster]StatusMod = 1
	end
	if (($[%caster]MoveClass=1) and ($[%caster]Aim = 1))
		$[%caster]MovePower = ($[%caster]MovePower*(min(5,[%caster]MovesInARow)))
		log ... com um poder acumulado de [[%caster]MovesInARow] golpes seguidos ...
	end
	if (($[%caster]MoveClass>=2) and ($[%caster]Aim = 1))
		$numberOfHits = random(2,$[%caster]MoveClass)
		log ... [$numberOfHits] vezes ...
	end
	if ((($[%caster]MoveClass>=0) or ($[%caster]MoveClass=(-3))) and ([%caster]Ptype=$[%caster]Movetype) and ($[%caster]Aim = 1))
		$[%caster]STAB = $[%caster]STAB + 0.5
		log ... o golpe foi fortalecido pelo seu tipo principal ...
	end
	if ((($[%caster]MoveClass>=0) or ($[%caster]MoveClass=(-3))) and ([%caster]Stype=$[%caster]Movetype) and ($[%caster]Aim = 1))
		$[%caster]STAB = $[%caster]STAB + 0.5
		log ... o golpe foi fortalecido pelo seu tipo secundário ...
	end
	if (($[%caster]MoveClass=(-3)) and ($[%caster]Aim = 1))
		$[%caster]Heal = round(min(([%caster]MaxHP-[%caster]CurHP),(([%caster]MaxHP/100)*$[%caster]MovePower*$[%caster]STAB)))
		gain $[%caster]Heal [%caster]CurHP
		log se curou em [$[%caster]Heal] pontos de vida!
	end
	if (($[%caster]MoveClass>=0) and (chance($[%caster]MoveCrit*pow(2,[%caster]CriticalStage))) and ($[%caster]Aim = 1))
		$[%caster]Crit = 1.5
		log ... e realizou um golpe crítico!
	end

	$auxMtype = $[%caster]MoveType
	$auxType = [%target]Ptype

*include effectivenessVerification %caster %target
	$[%caster]Sadvg = $auxAdvg
	$[%caster]TypeAdvg = $[%caster]Padvg*$[%caster]Sadvg

	if ([%target]Ptype=[%target]Stype)
		$[%caster]TypeAdvg = $[%caster]Padvg
	end

	if (($[%caster]MoveClass>=0) and ($[%caster]TypeAdvg=4) and ($[%caster]Aim = 1))
		log ... <#cc33ff><b>foi hiper efetivo!</b></#>
	end
	if (($[%caster]MoveClass>=0) and ($[%caster]TypeAdvg=2) and ($[%caster]Aim = 1))
		log ... <#4dffa6><b>foi super efetivo!</b></#>
	end
	if (($[%caster]MoveClass>=0) and ($[%caster]TypeAdvg=0.5) and ($[%caster]Aim = 1))
		log ... mas foi pouco efetivo!
	end
	if (($[%caster]MoveClass>=0) and ($[%caster]TypeAdvg=0.25) and ($[%caster]Aim = 1))
		log ... <#999999><b>mas não foi quase nada efetivo!</b></#>
	end
	if (($[%caster]MoveClass>=0) and ($[%caster]TypeAdvg=0) and ($[%caster]Aim = 1))
		log ... <#b30000><b>mas o golpe não afeta o adversário!</b></#>
	end

*include damageCalculation %caster %target
	$[%caster]RandomVar = frandom(0.85,1)

	if ($[%caster]MoveIsSpecial=0)
		$aA = [%caster]Attack
		$aD = [%target]Defense
	end
	if ($[%caster]MoveIsSpecial=1)
		$aA = [%caster]SpAttack
		$aD = [%target]SpDefense
	end
	if (($[%caster]MoveIsSpecial=0) and ([%caster]Status=3))
		$[%caster]StatusMod = 0.5
	end

	if (($[%caster]MoveClass>=0) and ($[%caster]Aim = 1))
		$[%caster]RawDamage = ((((0.4*[%caster]Lvl + 2)*$[%caster]MovePower*($aA/$aD))/50)+2)
		$[%caster]DamageMods = $[%caster]TypeAdvg*$[%caster]RandomVar*$[%caster]STAB*$[%caster]Crit*$[%caster]StatusMod
		$[%caster]EffectiveDamage = round(min([%target]CurHP,($[%caster]RawDamage*$[%caster]DamageMods)))
		lose $[%caster]EffectiveDamage [%target]CurHP
		do getname with tag:[%target]
		log tomou [$[%caster]EffectiveDamage] dano!
		log <#3366ff>-----------------------------------</#>
	end

*include randomBasedConditions
	log <#3366ff>-----------------------------------</#>
	$quarter=random(1,4)
	$paralysis=0
	$confusion=0
	if ($quarter=1)
		$paralysis=1
		$confusion=1
	end
	$equarter=random(1,4)
	$eparalysis=0
	$econfusion=0
	if ($equarter=1)
		$eparalysis=1
		$econfusion=1
	end

*include checkMoveFirst
	if ((activeSpeed+$activeMovePriority*10000)>=(enemySpeed+$enemyMovePriority*10000) and (activeStatus>=0)) do activemove with this

*include checkMoveFirstUnderStatus %statusID %statusName
	if ((activeSpeed+$activeMovePriority*10000)>=(enemySpeed+$enemyMovePriority*10000) and (activeStatus=(-[%statusID])) and ($[%statusName]=0)) do activemove with this
	if ((activeSpeed+$activeMovePriority*10000)>=(enemySpeed+$enemyMovePriority*10000) and (activeStatus=(-[%statusID])) and ($[%statusName]=1)) do active[%statusName] with this

*include checkEnemyMoveUnderStatus %statusID %statusName
	if ((enemyStatus=(-[%statusID])) and ($e[%statusName]=0)) do enemymove with this
	if ((enemyStatus=(-[%statusID])) and ($e[%statusName]=1)) do enemy[%statusName] with this

*include checkMoveSecond
	if ((enemySpeed+$enemyMovePriority*10000)>(activeSpeed+$activeMovePriority*10000) and (activeStatus>=0)) do activemove with this

*include checkMoveSecondUnderStatus %statusID %statusName
	if ((enemySpeed+$enemyMovePriority*10000)>(activeSpeed+$activeMovePriority*10000) and (activeStatus=(-[%statusID])) and ($[%statusName]=0)) do activemove with this
	if ((enemySpeed+$enemyMovePriority*10000)>(activeSpeed+$activeMovePriority*10000) and (activeStatus=(-[%statusID])) and ($[%statusName]=1)) do active[%statusName] with this

*include statusHealingBerry %name %status %log
		on activeeat:
			do getname with tag:active
			log consumiu uma <#e65c00>[%name]</#> ...
			if (activeStatus=([%status]))
				activeStatus=0
				log [%log]
				log <#3366ff>-----------------------------------</#>
			end
			activeBerry = 0
		end
		on activebite:
			do getname with tag:active
			log mordeu a <#e65c00>[%name]</#> do adversário ...
			if (activeStatus=([%status]))
				activeStatus=0
				log [%log]
				log <#3366ff>-----------------------------------</#>
			end
			enemyBerry = 0
		end
		on enemyeat:
			do getname with tag:enemy
			log consumiu uma <#e65c00>[%name]</#> ...
			if (enemyStatus=([%status]))
				enemyStatus=0
				log [%log]
				log <#3366ff>-----------------------------------</#>
			end
			enemyBerry = 0
		end
		on enemybite:
			do getname with tag:enemy
			log mordeu a <#e65c00>[%name]</#> do adversário ...
			if (enemyStatus=([%status]))
				enemyStatus=0
				log [%log]
				log <#3366ff>-----------------------------------</#>
			end
			activeBerry = 0
		end

*include hpHealingBerry %name %heal
		on activeeat:
			do getname with tag:active
			log consumiu uma <#e65c00>[%name]</#> ...
			$heal = min(round((activeMaxHP/100)*[%heal]),(activeMaxHP-activeCurHP))
			gain $heal activeCurHP
			log se curou em [$heal] pontos de vida!
			log <#3366ff>-----------------------------------</#>
			activeBerry = 0
		end
		on activebite:
			do getname with tag:active
			log mordeu a <#e65c00>[%name]</#> do adversário ...
			$heal = min(round((activeMaxHP/100)*[%heal]),(activeMaxHP-activeCurHP))
			gain $heal activeCurHP
			log se curou em [$heal] pontos de vida!
			log <#3366ff>-----------------------------------</#>
			enemyBerry = 0
		end
		on enemyeat:
			do getname with tag:enemy
			log consumiu uma <#e65c00>[%name]</#> ...
			$heal = min(round((enemyMaxHP/100)*[%heal]),(enemyMaxHP-enemyCurHP))
			gain $heal enemyCurHP
			log se curou em [$heal] pontos de vida!
			log <#3366ff>-----------------------------------</#>
			enemyBerry = 0
		end
		on enemybite:
			do getname with tag:enemy
			log mordeu a <#e65c00>[%name]</#> do adversário ...
			$heal = min(round((enemyMaxHP/100)*[%heal]),(enemyMaxHP-enemyCurHP))
			gain $heal enemyCurHP
			log se curou em [$heal] pontos de vida!
			log <#3366ff>-----------------------------------</#>
			activeBerry = 0
		end
