Layout
*wrapper
	contains:upperArea, lowerArea, leftArea, middleArea, rightArea
	*upperArea
		contains:statusDisplay
		*statusDisplay
			contains:tag:status
			class:fullWidth
	*leftArea
		contains:logDisplay
		*logDisplay
			contains:Log
			header:Log
	*middleArea
		contains:clickablesDisplay
		*clickablesDisplay
			contains:starterDisplay, activeDisplay, enemyDisplay, moveDisplay
			*starterDisplay
				contains:tag:starterChoice
			*activeDisplay
				contains:tag:active
			*enemyDisplay
				contains:tag:enemy
			*moveDisplay
				contains:tag:activeMove, tag:enemyMove
	*rightArea
		contains:exploreDisplay
		*exploreDisplay
			contains:tag:explore
			header:Exploração
			tooltip origin:left
	*lowerArea
		contains:mainMenu
			*mainMenu
				contains:navigationDisplay, contentDisplay
				class:fullWidth
				*navigationDisplay
					contains:tag:navigationButton
					costs:hide
					names:hide
				*contentDisplay
					contains:tag:foundTab, tag:capturedTab, tag:regionTab, tag:achievTab, tag:statisticTab
					costs:hide
					names:hide

//-----------------------------------------------UI Text------------------------------------------------------------------//

Upgrades
	*TEMPLATE
		class:UItext
		no tooltip
		owned	
	*foundText
		name:Pokémon Avistados
		tag:foundTab
	*capturedText
		name:Pokémon Capturados
		tag:capturedTab
	*regionText
		name:Regiões
		tag:regionTab
	*achievText
		name:Conquistas
		tag:achievTab
	*statisticText
		name:Estatísticas
		tag:statisticTab
	*TEMPLATE

//---------------------------------------------------GLOBAL---------------------------------------------------------------//

Resources
	*TEMPLATE
		tags:status hidden
	*global
		name:Global Controler and Initializer
		passive:
			gain 1 secondPlayed
			if (no inBattle)
				gain min((activeMaxHP/100),(activeMaxHP-activeCurHP)) activeCurHP
				gain 0.01 pokeball
				gain 0.025 switch
			end
		end
		on start:
			pokeball = 5
			switch = 1
			gain tag:starterFound
			show tag:foundTab
			hide tag:capturedTab
			hide tag:regionTab
			hide tag:achievTab
			hide tag:statisticTab
			log Bem vindo! Depois que <#ffcc00><b>escolher</b></#> um dos pokémon iniciais, veja seus pokémon capturados na segunda aba do <#ff33ff><b>menu</b></#> abaixo (ícone de pokébola). <#ffcc00><b>Clique</b></#> em um de seus pokémon capturados para chamá-lo para batalhar. Isto irá gastar um ponto de <u>troca</u>. <#ffcc00><b>Selecione</b></#> uma área no <#ff33ff><b>menu de exploração</b></#> à direita para encontrar pokémon selvagens. <#ffcc00><b>Clique</b></#> em um pokémon selvagem para tentar <#ffcc00><b>capturá-lo</b></#>. Isto irá gastar uma <u>pokébola</u>. Te dei 5 <u>pokébolas</u> de presente, não as desperdice! É muito mais fácil <#ffcc00><b>capturar</b></#> um pokémon quando ele está enfraquecido. Enquanto estiver fora de batalha, seu pokémon irá regenerar pontos de <u>vida</u> e você irá conseguir mais <u>pokébolas</u> e pontos de <u>troca</u> com o tempo.
		end

		on beginturn:
			$quarter=random(1,4)
			$paralysis=0
			$confusion=0
			$activeMoveAttempt = 0
			$enemyMoveAttempt = 0
			if ($quarter=1)
				$paralysis=1
				$confusion=1
			end
			$equarter=random(1,4)
			$eparalysis=0
			$econfusion=0
			if ($equarter=1)
				$eparalysis=1
				$econfusion=1
			end
			$protection = 0
			$reflection = 0

			echo Starting this turn with: \n $quarter=[$quarter]
			echo Move Data: $activeMoveID = [$activeMoveID]
			echo Move Data: $activeMoveType = [$activeMoveType]
			echo Move Data: $activeMoveCategory = [$activeMoveCategory]
			echo Move Data: $activeMoveClass = [$activeMoveClass]
			echo Move Data: $activeMoveShape = [$activeMoveShape]
			echo Move Data: $activeMovePriority = [$activeMovePriority]
			echo Move Data: $activeMoveCrit = [$activeMoveCrit]
			echo Move Data: $activeMovePower = [$activeMovePower]
			echo Move Data: $activeMoveFxChance0 = [$activeMoveFxChance0]
			echo Move Data: $activeMoveFxCondID0 = [$activeMoveFxCondID0]
			echo Move Data: $activeMoveFxCondNum0 = [$activeMoveFxCondNum0]
			echo Move Data: $activeMoveEffect1 = [$activeMoveEffect1]
			echo Move Data: $activeMoveFxPower1 = [$activeMoveFxPower1]
			echo Move Data: $activeMoveFxChance1 = [$activeMoveFxChance1]
			echo Move Data: $activeMoveFxCondID1 = [$activeMoveFxCondID1]
			echo Move Data: $activeMoveFxCondNum1 = [$activeMoveFxCondNum1]
			echo Move Data: $activeMoveEffect2 = [$activeMoveEffect2]
			echo Move Data: $activeMoveFxPower2 = [$activeMoveFxPower2]
			echo Move Data: $activeMoveFxChance2 = [$activeMoveFxChance2]
			echo Move Data: $activeMoveFxCondID2 = [$activeMoveFxCondID2]
			echo Move Data: $activeMoveFxCondNum2 = [$activeMoveFxCondNum2]
			echo Move Data: $activeMoveEffect3 = [$activeMoveEffect3]
			echo Move Data: $activeMoveFxPower3 = [$activeMoveFxPower3]
			echo Move Data: $activeMoveFxChance3 = [$activeMoveFxChance3]
			echo Move Data: $activeMoveFxCondID3 = [$activeMoveFxCondID3]
			echo Move Data: $activeMoveFxCondNum3 = [$activeMoveFxCondNum3]
			echo Move Data: $enemyMoveID = [$enemyMoveID]
			echo Move Data: $enemyMoveType = [$enemyMoveType]
			echo Move Data: $enemyMoveCategory = [$enemyMoveCategory]
			echo Move Data: $enemyMoveClass = [$enemyMoveClass]
			echo Move Data: $enemyMoveShape = [$enemyMoveShape]
			echo Move Data: $enemyMovePriority = [$enemyMovePriority]
			echo Move Data: $enemyMoveCrit = [$enemyMoveCrit]
			echo Move Data: $enemyMovePower = [$enemyMovePower]
			echo Move Data: $enemyMoveFxChance0 = [$enemyMoveFxChance0]
			echo Move Data: $enemyMoveFxCondID0 = [$enemyMoveFxCondID0]
			echo Move Data: $enemyMoveFxCondNum0 = [$enemyMoveFxCondNum0]
			echo Move Data: $enemyMoveEffect1 = [$enemyMoveEffect1]
			echo Move Data: $enemyMoveFxPower1 = [$enemyMoveFxPower1]
			echo Move Data: $enemyMoveFxChance1 = [$enemyMoveFxChance1]
			echo Move Data: $enemyMoveFxCondID1 = [$enemyMoveFxCondID1]
			echo Move Data: $enemyMoveFxCondNum1 = [$enemyMoveFxCondNum1]
			echo Move Data: $enemyMoveEffect2 = [$enemyMoveEffect2]
			echo Move Data: $enemyMoveFxPower2 = [$enemyMoveFxPower2]
			echo Move Data: $enemyMoveFxChance2 = [$enemyMoveFxChance2]
			echo Move Data: $enemyMoveFxCondID2 = [$enemyMoveFxCondID2]
			echo Move Data: $enemyMoveFxCondNum2 = [$enemyMoveFxCondNum2]
			echo Move Data: $enemyMoveEffect3 = [$enemyMoveEffect3]
			echo Move Data: $enemyMoveFxPower3 = [$enemyMoveFxPower3]
			echo Move Data: $enemyMoveFxChance3 = [$enemyMoveFxChance3]
			echo Move Data: $enemyMoveFxCondID3 = [$enemyMoveFxCondID3]
			echo Move Data: $enemyMoveFxCondNum3 = [$enemyMoveFxCondNum3]
			echo Checking First Move..........................................
			echo Active Speed = [activeSpeed] vs [enemySpeed] = Enemy Speed
			echo Active Move Priority = [$activeMovePriority] vs [$enemyMovePriority ] = Enemy Move Priority
			echo Active Status = [activeStatus] vs [enemyStatus] = Enemy Status

			log <#3366ff>-----------------------------------</#>
			[include checkMoveFirst]
			[include checkMoveFirstUnderStatus %statusID="2" %statusName="paralysis"]
			[include checkMoveFirstUnderStatus %statusID="7" %statusName="confusion"]

			if ((enemyStatus>=0) and ($reflection=0) and ($enemyMoveAttempt=0)) do enemymove with this
			if ((enemyStatus>=0) and ($reflection=1) and ($enemyMoveAttempt=0)) do enemyconfusion with this
			[include checkEnemyMoveUnderStatus %statusID="2" %statusName="paralysis"]
			[include checkEnemyMoveUnderStatus %statusID="7" %statusName="confusion"]

			[include checkMoveSecond]
			[include checkMoveSecondUnderStatus %statusID="2" %statusName="paralysis"]
			[include checkMoveSecondUnderStatus %statusID="7" %statusName="confusion"]

			do endturn with this
		end

		on activemove:
			echo Starting Active Move..........................................
			[include checkConditions %caster="active" %target="enemy" %number="0"]
			[include checkConditions %caster="active" %target="enemy" %number="1"]
			[include checkConditions %caster="active" %target="enemy" %number="2"]
			[include checkConditions %caster="active" %target="enemy" %number="3"]

			[include moveFxIf %caster="active" %number="31" %fxNum="1"]
				[include moveCoreCopy %caster="active" %target="enemy"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="1"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="2"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="3"]
			end
			[include moveFxIf %caster="active" %number="31" %fxNum="2"]
				[include moveCoreCopy %caster="active" %target="enemy"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="1"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="2"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="3"]
			end
			[include moveFxIf %caster="active" %number="31" %fxNum="3"]
				[include moveCoreCopy %caster="active" %target="enemy"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="1"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="2"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="3"]
			end
			[include moveFxIf %caster="active" %number="32" %fxNum="1"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="1"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="2"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="3"]
			end
			[include moveFxIf %caster="active" %number="32" %fxNum="2"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="1"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="2"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="3"]
			end
			[include moveFxIf %caster="active" %number="32" %fxNum="3"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="1"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="2"]
				[include moveFxCopy %caster="active" %target="enemy" %fxNum="3"]
			end
			[include moveFxIf %caster="active" %number="33" %fxNum="1"]
				$activeMovePower = $activeMovePower + $activeMoveFxPower1
			end
			[include moveFxIf %caster="active" %number="33" %fxNum="2"]
				$activeMovePower = $activeMovePower + $activeMoveFxPower2
			end
			[include moveFxIf %caster="active" %number="33" %fxNum="3"]
				$activeMovePower = $activeMovePower + $activeMoveFxPower3
			end

			[include moveFxIf %caster="active" %number="37" %fxNum="1"]
				$activeNoKill = $activeNoKill + $activeMoveFxPower1
			end
			[include moveFxIf %caster="active" %number="37" %fxNum="2"]
				$activeNoKill = $activeNoKill + $activeMoveFxPower2
			end
			[include moveFxIf %caster="active" %number="37" %fxNum="3"]
				$activeNoKill = $activeNoKill + $activeMoveFxPower3
			end

			[include onMove %caster="active" %target="enemy"]
			[include activeMoveTypeAdvantage %type="0"]
			[include activeMoveTypeAdvantage %type="1"]
			[include activeMoveTypeAdvantage %type="2"]
			[include activeMoveTypeAdvantage %type="3"]
			[include activeMoveTypeAdvantage %type="4"]
			[include activeMoveTypeAdvantage %type="5"]
			[include activeMoveTypeAdvantage %type="6"]
			[include activeMoveTypeAdvantage %type="7"]
			[include activeMoveTypeAdvantage %type="8"]
			[include activeMoveTypeAdvantage %type="9"]
			[include activeMoveTypeAdvantage %type="10"]
			[include activeMoveTypeAdvantage %type="11"]
			[include activeMoveTypeAdvantage %type="12"]
			[include activeMoveTypeAdvantage %type="13"]
			[include activeMoveTypeAdvantage %type="14"]
			[include activeMoveTypeAdvantage %type="15"]
			[include activeMoveTypeAdvantage %type="16"]
			[include activeMoveTypeAdvantage %type="17"]
			[include effectivenessVerification %caster="active" %target="enemy"]
			[include damageCalculation %caster="active" %target="enemy"]

			[include moveMetaEffects %caster="active" %fxNum="1"]
			[include moveMetaEffects %caster="active" %fxNum="2"]
			[include moveMetaEffects %caster="active" %fxNum="3"]

			[include moveDrainEffect %caster="active" %number="1" %fxNum="1"]
			[include moveDrainEffect %caster="active" %number="1" %fxNum="2"]
			[include moveDrainEffect %caster="active" %number="1" %fxNum="3"]
			[include moveRecoilEffect %caster="active" %number="2" %fxNum="1"]
			[include moveRecoilEffect %caster="active" %number="2" %fxNum="2"]
			[include moveRecoilEffect %caster="active" %number="2" %fxNum="3"]
			[include moveStatusEffect %caster="active" %target="enemy" %number="3" %fxNum="1"]
			[include moveStatusEffect %caster="active" %target="enemy" %number="3" %fxNum="2"]
			[include moveStatusEffect %caster="active" %target="enemy" %number="3" %fxNum="3"]
			[include moveHealStatusEffect %caster="active" %target="enemy" %number="4" %fxNum="1"]
			[include moveHealStatusEffect %caster="active" %target="enemy" %number="4" %fxNum="2"]
			[include moveHealStatusEffect %caster="active" %target="enemy" %number="4" %fxNum="3"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="5" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu" %fxNum="1"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="5" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu" %fxNum="2"]
