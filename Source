			end
			if (($enemyMoveEffect=36) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyDefenseStage = min(6,(enemyDefenseStage+3))
				do getname with tag:enemy
				log teve sua Defesa drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=37) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemySpAttackStage = min(6,(enemySpAttackStage+3))
				do getname with tag:enemy
				log teve seu Ataque Especial drasticamente aumentado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=38) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemySpDefenseStage = min(6,(enemySpDefenseStage+3))
				do getname with tag:enemy
				log teve sua Defesa Especial drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=39) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemySpeedStage = min(6,(enemySpeedStage+3))
				do getname with tag:enemy
				log teve sua Velocidade drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=40) and (chance($enemyMoveFxChance)) and (enemyStatus=0) and ($enemyAim = 1))
				enemyStatus = -5
				do getname with tag:enemy
				log ficou hesitante!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=41) and (chance($enemyMoveFxChance)) and (enemyStatus=0) and ($enemyAim = 1))
				enemyStatus = -6
				do getname with tag:enemy
				log foi forçado a fugir!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=42) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyAttackStage = min(6,(enemyAttackStage+1))
				enemyDefenseStage = min(6,(enemyDefenseStage+1))
				enemySpAttackStage = min(6,(enemySpAttackStage+1))
				enemySpDefenseStage = min(6,(enemySpDefenseStage+1))
				enemySpeedStage = min(6,(enemySpeedStage+1))
				do getname with tag:enemy
				log teve todos os seus status ligeiramente aumentados!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=43) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyAccuracyStage = min(6,(enemyAccuracyStage+1))
				do getname with tag:enemy
				log teve sua precisão ligeiramente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=44) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyAccuracyStage = min(6,(enemyAccuracyStage+2))
				do getname with tag:enemy
				log teve sua precisão bruscamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=45) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyAccuracyStage = min(6,(enemyAccuracyStage+3))
				do getname with tag:enemy
				log teve sua precisão drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=46) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyEvasionStage = max((-6),(enemyEvasionStage-1))
				do getname with tag:enemy
				log teve sua evasão ligeiramente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=47) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyEvasionStage = max((-6),(enemyEvasionStage-2))
				do getname with tag:enemy
				log teve sua evasão duramente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=48) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyEvasionStage = max((-6),(enemyEvasionStage-3))
				do getname with tag:enemy
				log teve sua evasão severamente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=49) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyAccuracyStage = max((-6),(enemyAccuracyStage-1))%
				do getname with tag:enemy
				log teve sua precisão ligeiramente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=50) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyAccuracyStage = max((-6),(enemyAccuracyStage-2))
				do getname with tag:enemy
				log teve sua precisão bruscamente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=51) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyAccuracyStage = max((-6),(enemyAccuracyStage-3))
				do getname with tag:enemy
				log teve sua precisão drasticamente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=52) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyEvasionStage = min(6,(enemyEvasionStage+1))
				do getname with tag:enemy
				log teve sua evasão ligeiramente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=53) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyEvasionStage = min(6,(enemyEvasionStage+2))
				do getname with tag:enemy
				log teve sua evasão duramente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=54) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyEvasionStage = min(6,(enemyEvasionStage+3))
				do getname with tag:enemy
				log teve sua evasão severamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=55) and (chance($enemyMoveFxChance)) and (enemyStatus=0) and ($enemyAim = 1))
				enemyStatus = (-7)
				do getname with tag:enemy
				log ficou confuso!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=56) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyCriticalStage = min(6,(enemyCriticalStage+1))
				do getname with tag:enemy
				log teve sua chance de crítico ligeiramente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=57) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyCriticalStage = min(6,(enemyCriticalStage+2))
				do getname with tag:enemy
				log teve sua chance de crítico bruscamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($enemyMoveEffect=58) and (chance($enemyMoveFxChance)) and ($enemyAim = 1))
				enemyCriticalStage = min(6,(enemyCriticalStage+3))
				do getname with tag:enemy
				log teve sua chance de crítico drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end

			[include enemyConfusedMultHit %hit="2"]
			[include enemyConfusedMultHit %hit="3"]
			[include enemyConfusedMultHit %hit="4"]
			[include enemyConfusedMultHit %hit="5"]

			if (0>=enemyCurHP)
				enemyStatus=-1
			end

			do getname with tag:enemy
			log não está mais confuso.

			enemyLastMove = $enemyMoveID
		end

//-------------------------------------------------------------------------------------------------//

		on activefaint:
			do getname with tag:active
			log está fora de batalha!
			do return with tag:active
		end
		on enemyfaint:
			do getname with tag:enemy
			log está fora de batalha!
			do faint with tag:enemy
		end

		on activeflee:
			do getname with tag:active
			log deixou a batalha!
			do return with tag:active
		end
		on enemyflee:
			do getname with tag:enemy
			log deixou a batalha!
			lose tag:enemy
		end

		on activeflinch:
			do getname with tag:active
			log hesitou em atacar!
			activeStatus = 0
		end
		on enemyflinch:
			do getname with tag:enemy
			log hesitou em atacar!
			activeStatus = 0
		end

		on activethaw:
			do getname with tag:active
			log descongelou!
			activeStatus = 0
		end
		on enemythaw:
			do getname with tag:enemy
			log descongelou!
			activeStatus = 0
		end

		on activefreeze:
			do getname with tag:active
			log nem se moveu, pois ainda está dentro de uma pedra de gelo!
			activeSleep = (activeSleep-1)
		end
		on enemyfreeze:
			do getname with tag:enemy
			log nem se moveu, pois ainda está dentro de uma pedra de gelo!
			activeSleep = (activeSleep-1)
		end

		on activewake:
			do getname with tag:active
			log acordou!
			activeStatus = 0
		end
		on enemywake:
			do getname with tag:enemy
			log acordou!
			activeStatus = 0
		end

		on activesleep:
			do getname with tag:active
			log nem se moveu, pois ainda está dormindo!
			activeSleep = (activeSleep-1)
		end
		on enemysleep:
			do getname with tag:enemy
			log nem se moveu, pois ainda está dormindo!
			activeSleep = (activeSleep-1)
		end

		on activeparalysis:
			do getname with tag:active
			log não conseguiu se mover por causa da paralisia!
		end
		on enemyparalysis:
			do getname with tag:enemy
			log não conseguiu se mover por causa da paralisia!
		end

		on activepoison:
			$activePoisonDamage = round(min((activeMaxHP/8),activeCurHP))
			lose $activePoisonDamage activeCurHP
			do getname with tag:active
			log tomou [$activePoisonDamage] dano por envenenamento!
		end
		on enemypoison:
			$enemyPoisonDamage = round(min((enemyMaxHP/8),enemyCurHP))
			lose $enemyPoisonDamage enemyCurHP
			do getname with tag:enemy
			log tomou [$enemyPoisonDamage] dano por envenenamento!
		end

		on activebadpoison:
			$activePoisonDamage = round(min((activeBadPoison*activeMaxHP/16),activeCurHP))
			lose $activePoisonDamage activeCurHP
			do getname with tag:active
			log tomou [$activePoisonDamage] dano por intoxicação!
		end
		on enemybadpoison:
			$enemyPoisonDamage = round(min((enemyBadPoison*enemyMaxHP/8),enemyCurHP))
			lose $enemyPoisonDamage enemyCurHP
			do getname with tag:enemy
			log tomou [$enemyPoisonDamage] dano por intoxicação!
		end

		on activeburn:
			$activePoisonDamage = round(min((activeMaxHP/16),activeCurHP))
			lose $activePoisonDamage activeCurHP
			do getname with tag:active
			log tomou [$activePoisonDamage] dano por queimadura!
		end
		on enemyburn:
			$enemyPoisonDamage = round(min((enemyMaxHP/16),enemyCurHP))
			lose $enemyPoisonDamage enemyCurHP
			do getname with tag:enemy
			log tomou [$enemyPoisonDamage] dano por queimadura!
		end

		on endturn:
			if (activeStatus=1) do activepoison with this
			if (enemyStatus=1) do enemypoison with this
			if (activeStatus=2) do activebadpoison with this
			if (enemyStatus=2) do enemybadpoison with this
			if (activeStatus=3) do activeburn with this
			if (enemyStatus=3) do enemyburn with this
			if ((activeStatus=(-3)) and (activeSleep>0)) do activesleep with this
			if ((enemyStatus=(-3)) and (enemySleep>0)) do enemysleep with this
			if ((activeStatus=(-3)) and (0>=activeSleep)) do activewake with this
			if ((enemyStatus=(-3)) and (0>=enemySleep)) do enemywake with this
			$fifth = random(1,5)
			$thaw = 0
			if ($fifth=1) $thaw=1
			if ((activeStatus=(-4)) and ($thaw=0)) do activefreeze with this
			if ((enemyStatus=(-4)) and ($thaw=0)) do enemyfreeze with this
			if ((activeStatus=(-4)) and ($thaw=1)) do activethaw with this
			if ((enemyStatus=(-4)) and ($thaw=1)) do enemythaw with this
			if (activeStatus=(-5)) do activeflinch with this
			if (enemyStatus=(-5)) do enemyflinch with this
			if (activeStatus=(-6)) do activeflee with this
			if (enemyStatus=(-6)) do enemyflee with this

			[include defModByStage %stage="(-6)" %mod="0.25"]
			[include defModByStage %stage="(-5)" %mod="0.2857"]
			[include defModByStage %stage="(-4)" %mod="0.3333"]
			[include defModByStage %stage="(-3)" %mod="0.4"]
			[include defModByStage %stage="(-2)" %mod="0.5"]
			[include defModByStage %stage="(-1)" %mod="0.6666"]
			[include defModByStage %stage="0" %mod="1"]
			[include defModByStage %stage="1" %mod="1.5"]
			[include defModByStage %stage="2" %mod="2"]
			[include defModByStage %stage="3" %mod="2.5"]
			[include defModByStage %stage="4" %mod="3"]
			[include defModByStage %stage="5" %mod="3.5"]
			[include defModByStage %stage="6" %mod="4"]

			[include defAcEvModByStage %stage="(-6)" %mod="0.3333"]
			[include defAcEvModByStage %stage="(-5)" %mod="0.375"]
			[include defAcEvModByStage %stage="(-4)" %mod="0.4286"]
			[include defAcEvModByStage %stage="(-3)" %mod="0.5"]
			[include defAcEvModByStage %stage="(-2)" %mod="0.6"]
			[include defAcEvModByStage %stage="(-1)" %mod="0.75"]
			[include defAcEvModByStage %stage="0" %mod="1"]
			[include defAcEvModByStage %stage="1" %mod="1.3333"]
			[include defAcEvModByStage %stage="2" %mod="1.6666"]
			[include defAcEvModByStage %stage="3" %mod="2"]
			[include defAcEvModByStage %stage="4" %mod="2.3333"]
			[include defAcEvModByStage %stage="5" %mod="2.6666"]
			[include defAcEvModByStage %stage="6" %mod="3"]

			if ((activeStatus=(-1)) or (0>=round(activeCurHP))) do activefaint with this
			if ((enemyStatus=(-1)) or (0>=round(enemyCurHP))) do enemyfaint with this
		end

		on type0advantage:
			$auxAdvg = 1
			if (($auxType=5) or ($auxType=8)) $auxAdvg = 0.5
			if ($auxType=7) $auxAdvg = 0
		end
		on type1advantage:
			$auxAdvg = 1
			if (($auxType=0) or ($auxType=5) or ($auxType=8) or ($auxType=14) or ($auxType=16)) $auxAdvg = 2
			if (($auxType=2) or ($auxType=3) or ($auxType=6) or ($auxType=13) or ($auxType=17)) $auxAdvg = 0.5
			if ($auxType=7) $auxAdvg = 0
		end
		on type2advantage:
			$auxAdvg = 1
			if (($auxType=1) or ($auxType=6) or ($auxType=11)) $auxAdvg = 2
			if (($auxType=5) or ($auxType=8) or ($auxType=12)) $auxAdvg = 0.5
		end
		on type3advantage:
			$auxAdvg = 1
			if (($auxType=11) or ($auxType=17)) $auxAdvg = 2
			if (($auxType=3) or ($auxType=4) or ($auxType=5) or ($auxType=7)) $auxAdvg = 0.5
			if ($auxType=8) $auxAdvg = 0
		end
		on type4advantage:
			$auxAdvg = 1
			if (($auxType=3) or ($auxType=5) or ($auxType=8) or ($auxType=9) or ($auxType=12)) $auxAdvg = 2
			if (($auxType=6) or ($auxType=11)) $auxAdvg = 0.5
			if ($auxType=2) $auxAdvg = 0
		end
		on type5advantage:
			$auxAdvg = 1
			if (($auxType=2) or ($auxType=6) or ($auxType=9) or ($auxType=14)) $auxAdvg = 2
			if (($auxType=1) or ($auxType=4) or ($auxType=8)) $auxAdvg = 0.5
		end
		on type6advantage:
			$auxAdvg = 1
			if (($auxType=11) or ($auxType=13) or ($auxType=16)) $auxAdvg = 2
			if (($auxType=1) or ($auxType=2) or ($auxType=3) or ($auxType=7) or ($auxType=8) or ($auxType=9) or ($auxType=17)) $auxAdvg = 0.5
		end
		on type7advantage:
			$auxAdvg = 1
			if (($auxType=7) or ($auxType=13)) $auxAdvg = 2
			if ($auxType=16) $auxAdvg = 0.5
			if ($auxType=0) $auxAdvg = 0
		end
		on type8advantage:
			$auxAdvg = 1
			if (($auxType=5) or ($auxType=14) or ($auxType=17)) $auxAdvg = 2
			if (($auxType=8) or ($auxType=9) or ($auxType=10) or ($auxType=12)) $auxAdvg = 0.5
		end
		on type9advantage:
			$auxAdvg = 1
			if (($auxType=6) or ($auxType=8) or ($auxType=11) or ($auxType=14)) $auxAdvg = 2
			if (($auxType=5) or ($auxType=9) or ($auxType=10) or ($auxType=15)) $auxAdvg = 0.5
		end
		on type10advantage:
			$auxAdvg = 1
			if (($auxType=4) or ($auxType=5) or ($auxType=9)) $auxAdvg = 2
			if (($auxType=10) or ($auxType=11) or ($auxType=15)) $auxAdvg = 0.5
		end
		on type11advantage:
			$auxAdvg = 1
			if (($auxType=4) or ($auxType=5) or ($auxType=10)) $auxAdvg = 2
			if (($auxType=2) or ($auxType=3) or ($auxType=6) or ($auxType=8) or ($auxType=9) or ($auxType=11) or ($auxType=15)) $auxAdvg = 0.5
		end
		on type12advantage:
			$auxAdvg = 1
			if (($auxType=2) or ($auxType=10)) $auxAdvg = 2
			if (($auxType=11) or ($auxType=12) or ($auxType=15)) $auxAdvg = 0.5
			if ($auxType=4) $auxAdvg = 0
		end
		on type13advantage:
			$auxAdvg = 1
			if (($auxType=1) or ($auxType=3)) $auxAdvg = 2
			if (($auxType=8) or ($auxType=13)) $auxAdvg = 0.5
			if ($auxType=0) $auxAdvg = 0
		end
		on type14advantage:
			$auxAdvg = 1
			if (($auxType=2) or ($auxType=4) or ($auxType=11) or ($auxType=15)) $auxAdvg = 2
			if (($auxType=8) or ($auxType=9) or ($auxType=10) or ($auxType=14)) $auxAdvg = 0.5
		end
		on type15advantage:
			$auxAdvg = 1
			if ($auxType=15) $auxAdvg = 2
			if ($auxType=8) $auxAdvg = 0.5
			if ($auxType=17) $auxAdvg = 0
		end
		on type16advantage:
			$auxAdvg = 1
			if (($auxType=7) or ($auxType=13)) $auxAdvg = 2
			if (($auxType=1) or ($auxType=16) or ($auxType=17)) $auxAdvg = 0.5
		end
		on type17advantage:
			$auxAdvg = 1
			if (($auxType=1) or ($auxType=15) or ($auxType=16)) $auxAdvg = 2
			if (($auxType=3) or ($auxType=8) or ($auxType=9)) $auxAdvg = 0.5
		end
//		on type#advantage:
//			$auxAdvg = 1
//			if (($auxType=5) or ($auxType=8)) $auxAdvg = 2
//			if (($auxType=5) or ($auxType=8)) $auxAdvg = 0.5
//			if ($auxType=8) $auxAdvg = 0
//		end
			

//------------------------------------------------Basic------------------------------------------------------------------//

	*TEMPLATE
		tags:status hidden
		*activeLvl
			desc: Level do seu pokémon
		*activePtype
			desc: Tipo primário do seu pokémon
		*activeStype
			desc: Tipo secundário do seu pokémon
		*enemyLvl
			desc: Level do pokémon inimigo
		*enemyPtype
			desc: Tipo primário do pokémon inimigo
		*enemyStype
			desc: Tipo secundário do pokémon inimigo
		*enemyCurHP
			desc: HP atual do pokémon inimigo
		*enemyMaxHP
			desc: HP máximo do pokémon inimigo
		*enemyAttack
			name:Ataque
			desc:Potencial de golpes físicos do pokémon inimigo.
			is always: (enemyBaseAttack*enemyAttackMod)
		*enemyDefense
			name:Defesa
			desc:Resistência do pokémon inimigo aos golpes físicos.
			is always: (enemyBaseDefense*enemyDefenseMod)
		*enemySpAttack
			name:Ataque Especial
			desc:Potencial de golpes especiais do pokémon inimigo.
			is always: (enemyBaseSpAttack*enemySpAttackMod)
		*enemySpDefense
			name:Defesa Especial
			desc:Resistência do pokémon inimigo aos golpes especiais.
			is always: (enemyBaseSpDefense*enemySpDefenseMod)
		*enemySpeed
			name:Velocidade
			desc:A rapidez com que o pokémon inimigo ataca, define quem ataca primeiro.
			is always: (enemyBaseSpeed*enemySpeedMod)
	*TEMPLATE
		tags:status base hidden
		on reset:
			this = 0
		end
		*activeBaseAttack
			name:Ataque (Base)
		*activeBaseDefense
			name:Defesa (Base)
		*activeBaseSpAttack
			name:Ataque Especial (Base)
		*activeBaseSpDefense
			name:Defesa Especial (Base)
		*activeBaseSpeed
			name:Velocidade (Base)
	*TEMPLATE
		tags:status baseEnemy hidden
		on reset:
			this = 0
		end
		*enemyBaseAttack
			name:Ataque (Base) Inimigo
		*enemyBaseDefense
			name:Defesa (Base) Inimigo
		*enemyBaseSpAttack
			name:Ataque Especial (Base) Inimigo
		*enemyBaseSpDefense
			name:Defesa Especial (Base) Inimigo
		*enemyBaseSpeed
			name:Velocidade (Base) Inimigo
	*TEMPLATE
		tags:status modifier hidden
		on reset:
			this = 1
		end
		*activeAttackMod
			name:Ataque (Modificador)
		*activeDefenseMod
			name:Defesa (Modificador)
		*activeSpAttackMod
			name:Ataque Especial (Modificador)
		*activeSpDefenseMod
			name:Defesa Especial (Modificador)
		*activeSpeedMod
			name:Velocidade (Modificador)
		*activeAccuracyMod
			name:Precisão (Modificador)
		*activeEvasionMod
			name:Evasão (Modificador)
	*TEMPLATE
		tags:status modifier hidden
		can be negative
		on reset:
			this = 0
		end
		*activeStatus
			desc:Condição do seu pokémon
		*activeLastMove
			desc:A última habilidade usada pelo seu pokémon
		*activeMovesInARow
			desc:A quantidade de vezes que seu pokémon usou a mesma habilidade consecutivamente
		*activeBadPoison
			desc:O número de turnos que faz que seu pokémon foi intoxicado
