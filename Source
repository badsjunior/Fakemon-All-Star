Settings
	background:http://i.imgur.com/Dnd9gLk.png
	spritesheet:pType, 128 by 128, https://i.imgur.com/1rvvX5Q.png
	spritesheet:sType, 128 by 128, https://i.imgur.com/gRRkLwO.png
	spritesheet:filter, 128 by 128, https://i.imgur.com/WUzoOZB.png
	spritesheet:exploration, 128 by 128, https://i.imgur.com/5TDMKUS.png
	spritesheet:pindohDex, 128 by 128, https://i.imgur.com/F5He5Ae.png
	stylesheet:https://pastebin.com/zcLE68K1

Layout
*wrapper
	contains:upperArea, lowerArea, leftArea, middleArea, rightArea
	*upperArea
		contains:statusDisplay
		*statusDisplay
			contains:tag:status
			class:fullWidth
	*leftArea
		contains:logDisplay
		*logDisplay
			contains:Log
			header:Log
	*middleArea
		contains:clickablesDisplay
		*clickablesDisplay
			contains:starterDisplay, activeDisplay, enemyDisplay, moveDisplay
			*starterDisplay
				contains:tag:starterChoice
			*activeDisplay
				contains:tag:active
			*enemyDisplay
				contains:tag:enemy
			*moveDisplay
				contains:tag:activeMove, tag:enemyMove
	*rightArea
		contains:exploreDisplay
		*exploreDisplay
			contains:tag:explore
			header:Exploração
			tooltip origin:left
	*lowerArea
		contains:mainMenu
			*mainMenu
				contains:navigationDisplay, contentDisplay
				class:fullWidth
				*navigationDisplay
					contains:tag:navigationButton
					costs:hide
					names:hide
				*contentDisplay
					contains:tag:foundTab, tag:capturedTab, tag:regionTab, tag:achievTab, tag:statisticTab
					costs:hide
					names:hide

//-----------------------------------------------UI Text------------------------------------------------------------------//

Upgrades
	*TEMPLATE
		class:UItext
		no tooltip
		owned	
	*foundText
		name:Pokémon Avistados
		tag:foundTab
	*capturedText
		name:Pokémon Capturados
		tag:capturedTab
	*regionText
		name:Regiões
		tag:regionTab
	*achievText
		name:Conquistas
		tag:achievTab
	*statisticText
		name:Estatísticas
		tag:statisticTab
	*TEMPLATE

//---------------------------------------------------GLOBAL---------------------------------------------------------------//

Resources
	*TEMPLATE
		tags:status hidden
	*global
		name:Global Controler and Initializer
		passive:
			gain 1 secondPlayed
			if (no inBattle)
				gain min((activeMaxHP/100),(activeMaxHP-activeCurHP)) activeCurHP
				gain 0.01 pokeball
				gain 0.025 switch
			end
		end
		on start:
			pokeball = 5
			switch = 1
			gain tag:starterFound
			show tag:foundTab
			hide tag:capturedTab
			hide tag:regionTab
			hide tag:achievTab
			hide tag:statisticTab
			log Bem vindo! Depois que <#ffcc00><b>escolher</b></#> um dos pokémon iniciais, veja seus pokémon capturados na segunda aba do <#ff33ff><b>menu</b></#> abaixo (ícone de pokébola). <#ffcc00><b>Clique</b></#> em um de seus pokémon capturados para chamá-lo para batalhar. Isto irá gastar um ponto de <u>troca</u>. <#ffcc00><b>Selecione</b></#> uma área no <#ff33ff><b>menu de exploração</b></#> à direita para encontrar pokémon selvagens. <#ffcc00><b>Clique</b></#> em um pokémon selvagem para tentar <#ffcc00><b>capturá-lo</b></#>. Isto irá gastar uma <u>pokébola</u>. Te dei 5 <u>pokébolas</u> de presente, não as desperdice! É muito mais fácil <#ffcc00><b>capturar</b></#> um pokémon quando ele está enfraquecido. Enquanto estiver fora de batalha, seu pokémon irá regenerar pontos de <u>vida</u> e você irá conseguir mais <u>pokébolas</u> e pontos de <u>troca</u> com o tempo.
		end

		on beginturn:
			log <#3366ff>-----------------------------------</#>
			$quarter=random(1,4)
			$paralysis=0
			$confusion=0
			if ($quarter=1)
				$paralysis=1
				$confusion=1
			end
			$equarter=random(1,4)
			$eparalysis=0
			$econfusion=0
			if ($equarter=1)
				$eparalysis=1
				$econfusion=1
			end

			if ((activeSpeed+$activeMovePriority*10000)>=(enemySpeed+$enemyMovePriority*10000) and (activeStatus>=0)) do activemove with this
			if ((activeSpeed+$activeMovePriority*10000)>=(enemySpeed+$enemyMovePriority*10000) and (activeStatus=(-2)) and ($paralysis=0)) do activemove with this
			if ((activeSpeed+$activeMovePriority*10000)>=(enemySpeed+$enemyMovePriority*10000) and (activeStatus=(-2)) and ($paralysis=1)) do activeparalysis with this
			if ((activeSpeed+$activeMovePriority*10000)>=(enemySpeed+$enemyMovePriority*10000) and (activeStatus=(-7)) and ($confusion=0)) do activemove with this
			if ((activeSpeed+$activeMovePriority*10000)>=(enemySpeed+$enemyMovePriority*10000) and (activeStatus=(-7)) and ($confusion=1)) do activeconfusion with this

			if (enemyStatus>=0) do enemymove with this
			if ((enemyStatus=(-2)) and ($eparalysis=0)) do enemymove with this
			if ((enemyStatus=(-2)) and ($eparalysis=1)) do enemyparalysis with this
			if ((enemyStatus=(-7)) and ($econfusion=0)) do enemymove with this
			if ((enemyStatus=(-7)) and ($econfusion=1)) do enemyconfusion with this

			if ((enemySpeed+$enemyMovePriority*10000)>(activeSpeed+$activeMovePriority*10000) and (activeStatus>=0)) do activemove with this
			if ((enemySpeed+$enemyMovePriority*10000)>(activeSpeed+$activeMovePriority*10000) and (activeStatus=(-2)) and ($paralysis=0)) do activemove with this
			if ((enemySpeed+$enemyMovePriority*10000)>(activeSpeed+$activeMovePriority*10000) and (activeStatus=(-2)) and ($paralysis=1)) do activeparalysis with this
			if ((enemySpeed+$enemyMovePriority*10000)>(activeSpeed+$activeMovePriority*10000) and (activeStatus=(-7)) and ($confusion=0)) do activemove with this
			if ((enemySpeed+$enemyMovePriority*10000)>(activeSpeed+$activeMovePriority*10000) and (activeStatus=(-7)) and ($confusion=1)) do activeconfusion with this

			do endturn with this
		end

		on activemove:
			$activeAim = 1

			if ($activeMoveAccuracy>=0)
				$activeAim = chance(($activeMoveAccuracy*activeAccuracyMod*enemyEvasionMod)*100)
			end

			if ($activeAim = 0)
				do getname with tag:active
				log errou o golpe!
				activeMovesInARow = 0
			end

			if (($activeMoveID=activeLastMove) and ($activeAim = 1))
				activeMovesInARow = (activeMovesInARow+1)
			end
			if ((!($activeMoveID=activeLastMove)) and ($activeAim = 1))
				activeMovesInARow = 1
			end

			if (($activeMoveClass>=0) and ($activeAim = 1))
				do getname with tag:active
				log atacou...
				$activeSTAB = 1
				$activeCrit = 1
				$activeStatusMod = 1
			end
			if (($activeMoveClass=1) and ($activeAim = 1))
				$activeMovePower = ($activeMovePower*(min(5,activeMovesInARow)))
				log ... com um poder acumulado de [activeMovesInARow] golpes seguidos ...
			end
			if (($activeMoveClass>=2) and ($activeAim = 1))
				$numberOfHits = random(2,$activeMoveClass)
				log ... [$numberOfHits] vezes ...
			end

			if (($activeMoveClass>=0) and (activePtype=$activeMovetype) and ($activeAim = 1))
				$activeSTAB = $activeSTAB + 0.5
				log ... o golpe foi fortalecido pelo seu tipo principal ...
			end
			if (($activeMoveClass>=0) and (activeStype=$activeMovetype) and ($activeAim = 1))
				$activeSTAB = $activeSTAB + 0.5
				log ... o golpe foi fortalecido pelo seu tipo secundário ...
			end
			if (($activeMoveClass>=0) and (chance($activeMoveCrit*pow(2,activeCriticalStage))) and ($activeAim = 1))
				$activeCrit = 1.5
				log ... e realizou um golpe crítico!
			end

			$auxMtype = $activeMoveType
			$auxType = enemyPtype

			[include activeMoveTypeAdvantage %type="0"]
			[include activeMoveTypeAdvantage %type="1"]
			[include activeMoveTypeAdvantage %type="2"]
			[include activeMoveTypeAdvantage %type="3"]
			[include activeMoveTypeAdvantage %type="4"]
			[include activeMoveTypeAdvantage %type="5"]
			[include activeMoveTypeAdvantage %type="6"]
			[include activeMoveTypeAdvantage %type="7"]
			[include activeMoveTypeAdvantage %type="8"]
			[include activeMoveTypeAdvantage %type="9"]
			[include activeMoveTypeAdvantage %type="10"]
			[include activeMoveTypeAdvantage %type="11"]
			[include activeMoveTypeAdvantage %type="12"]
			[include activeMoveTypeAdvantage %type="13"]
			[include activeMoveTypeAdvantage %type="14"]
			[include activeMoveTypeAdvantage %type="15"]
			[include activeMoveTypeAdvantage %type="16"]
			[include activeMoveTypeAdvantage %type="17"]

			$activeSadvg = $auxAdvg
			$activeTypeAdvg = $activePadvg*$activeSadvg

			if (enemyPtype=enemyStype)
				$activeTypeAdvg = $activePadvg
			end

			if (($activeMoveClass>=0) and ($activeTypeAdvg=4) and ($activeAim = 1))
				log ... <#cc33ff><b>foi hiper efetivo!</b></#>
			end
			if (($activeMoveClass>=0) and ($activeTypeAdvg=2) and ($activeAim = 1))
				log ... <#4dffa6><b>foi super efetivo!</b></#>
			end
			if (($activeMoveClass>=0) and ($activeTypeAdvg=0.5) and ($activeAim = 1))
				log ... mas foi pouco efetivo!
			end
			if (($activeMoveClass>=0) and ($activeTypeAdvg=0.25) and ($activeAim = 1))
				log ... <#999999><b>mas não foi quase nada efetivo!</b></#>
			end
			if (($activeMoveClass>=0) and ($activeTypeAdvg=0) and ($activeAim = 1))
				log ... <#b30000><b>mas o golpe não afeta o adversário!</b></#>
			end

			$activeRandomVar = frandom(0.85,1)

			if ($activeMoveIsSpecial=0)
				$aA = activeAttack
				$aD = enemyDefense
			end
			if ($activeMoveIsSpecial=1)
				$aA = activeSpAttack
				$aD = enemySpDefense
			end
			if (($activeMoveIsSpecial=0) and (activeStatus=3))
				$activeStatusMod = 0.5
			end

			if (($activeMoveClass>=0) and ($activeAim = 1))
				$activeRawDamage = ((((0.4*activeLvl + 2)*$activeMovePower*($aA/$aD))/50)+2)
				$activeDamageMods = $activeTypeAdvg*$activeRandomVar*$activeSTAB*$activeCrit*$activeStatusMod
				$activeEffectiveDamage = round(min(enemyCurHP,($activeRawDamage*$activeDamageMods)))
				lose $activeEffectiveDamage enemyCurHP
				do getname with tag:enemy
				log tomou [$activeEffectiveDamage] dano!
				log <#3366ff>-----------------------------------</#>
			end

			if ($activeMoveEffect=59)
				$activeMoveEffect = 25 + random(0,4)
			end
			if ($activeMoveEffect=60)
				$activeMoveEffect = 30 + random(0,4)
			end
			if ($activeMoveEffect=61)
				$activeMoveEffect = 35 + random(0,4)
			end

			if ((($activeMoveEffect=1) or ($activeMoveEffect2=1)) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				$activeEffectiveHeal = ceil(min(($activeEffectiveDamage/2),(activeMaxHP-activeCurHP)))
				gain $activeEffectiveHeal activeCurHP
				do getname with tag:active
				log drenou [$activeEffectiveHeal] pontos de vida!
				log <#3366ff>-----------------------------------</#>
			end

			if ((($activeMoveEffect=2) or ($activeMoveEffect2=2)) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				$activeEffectiveRecoil = ceil(min(($activeEffectiveDamage/4),activeCurHP))
				lose $activeEffectiveRecoil activeCurHP
				do getname with tag:active
				log também se feriu, tomando [$activeEffectiveRecoil] dano!
				log <#3366ff>-----------------------------------</#>
			end
			if ((($activeMoveEffect=3) or ($activeMoveEffect2=3)) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				$activeEffectiveRecoil = ceil(min(($activeEffectiveDamage/3),activeCurHP))
				lose $activeEffectiveRecoil activeCurHP
				do getname with tag:active
				log também se feriu, tomando [$activeEffectiveRecoil] dano!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=4) and (chance($activeMoveFxChance)) and (enemyStatus=0) and ($activeAim = 1))
				enemyStatus = 1
				do getname with tag:enemy
				log foi envenenado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=5) and (chance($activeMoveFxChance)) and (enemyStatus=0) and ($activeAim = 1))
				enemyStatus = 2
				do getname with tag:enemy
				log foi intoxicado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=6) and (chance($activeMoveFxChance)) and (enemyStatus=0) and ($activeAim = 1))
				enemyStatus = 3
				do getname with tag:enemy
				log está em chamas!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=7) and (chance($activeMoveFxChance)) and (enemyStatus=0) and ($activeAim = 1))
				enemyStatus = -2
				do getname with tag:enemy
				log foi paralisado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=8) and (chance($activeMoveFxChance)) and (enemyStatus=0) and ($activeAim = 1))
				enemyStatus = -3
				do getname with tag:enemy
				enemySleep = random(1,3)
				log caiu no sono!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=9) and (chance($activeMoveFxChance)) and (enemyStatus=0) and ($activeAim = 1))
				enemyStatus = -4
				do getname with tag:enemy
				log foi congelado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=10) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemyAttackStage = max((-6),(enemyAttackStage-1))
				do getname with tag:enemy
				log teve seu Ataque ligeiramente reduzido!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=11) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemyDefenseStage = max((-6),(enemyDefenseStage-1))
				do getname with tag:enemy
				log teve sua Defesa ligeiramente reduzida!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=12) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemySpAttackStage = max((-6),(enemySpAttackStage-1))
				do getname with tag:enemy
				log teve seu Ataque Especial ligeiramente reduzido!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=13) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemySpDefenseStage = max((-6),(enemySpDefenseStage-1))
				do getname with tag:enemy
				log teve sua Defesa Especial ligeiramente reduzida!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=14) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemySpeedStage = max((-6),(enemySpeedStage-1))
				do getname with tag:enemy
				log teve sua Velocidade ligeiramente reduzida!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=15) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemyAttackStage = max((-6),(enemyAttackStage-2))
				do getname with tag:enemy
				log teve seu Ataque duramente reduzido!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=16) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemyDefenseStage = max((-6),(enemyDefenseStage-2))
				do getname with tag:enemy
				log teve sua Defesa duramente reduzida!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=17) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemySpAttackStage = max((-6),(enemySpAttackStage-2))
				do getname with tag:enemy
				log teve seu Ataque Especial duramente reduzido!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=18) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemySpDefenseStage = max((-6),(enemySpDefenseStage-2))
				do getname with tag:enemy
				log teve sua Defesa Especial duramente reduzida!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=19) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemySpeedStage = max((-6),(enemySpeedStage-2))
				do getname with tag:enemy
				log teve sua Velocidade duramente reduzida!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=20) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemyAttackStage = max((-6),(enemyAttackStage-3))
				do getname with tag:enemy
				log teve seu Ataque severamente reduzido!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=21) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemyDefenseStage = max((-6),(enemyDefenseStage-3))
				do getname with tag:enemy
				log teve sua Defesa severamente reduzida!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=22) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemySpAttackStage = max((-6),(enemySpAttackStage-3))
				do getname with tag:enemy
				log teve seu Ataque Especial severamente reduzido!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=23) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemySpDefenseStage = max((-6),(enemySpDefenseStage-3))
				do getname with tag:enemy
				log teve sua Defesa Especial severamente reduzida!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=24) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemySpeedStage = max((-6),(enemySpeedStage-3))
				do getname with tag:enemy
				log teve sua Velocidade severamente reduzida!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=25) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeAttackStage = min(6,(activeAttackStage+1))
				do getname with tag:active
				log teve seu Ataque ligeiramente aumentado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=26) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeDefenseStage = min(6,(activeDefenseStage+1))
				do getname with tag:active
				log teve sua Defesa ligeiramente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=27) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeSpAttackStage = min(6,(activeSpAttackStage+1))
				do getname with tag:active
				log teve seu Ataque Especial ligeiramente aumentado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=28) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeSpDefenseStage = min(6,(activeSpDefenseStage+1))
				do getname with tag:active
				log teve sua Defesa Especial ligeiramente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=29) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeSpeedStage = min(6,(activeSpeedStage+1))
				do getname with tag:active
				log teve sua Velocidade ligeiramente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=30) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeAttackStage = min(6,(activeAttackStage+2))
				do getname with tag:active
				log teve seu Ataque bruscamente aumentado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=31) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeDefenseStage = min(6,(activeDefenseStage+2))
				do getname with tag:active
				log teve sua Defesa bruscamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=32) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeSpAttackStage = min(6,(activeSpAttackStage+2))
				do getname with tag:active
				log teve seu Ataque Especial bruscamente aumentado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=33) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeSpDefenseStage = min(6,(activeSpDefenseStage+2))
				do getname with tag:active
				log teve sua Defesa Especial bruscamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=34) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeSpeedStage = min(6,(activeSpeedStage+2))
				do getname with tag:active
				log teve sua Velocidade bruscamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=35) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeAttackStage = min(6,(activeAttackStage+3))
				do getname with tag:active
				log teve seu Ataque drasticamente aumentado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=36) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeDefenseStage = min(6,(activeDefenseStage+3))
				do getname with tag:active
				log teve sua Defesa drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=37) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeSpAttackStage = min(6,(activeSpAttackStage+3))
				do getname with tag:active
				log teve seu Ataque Especial drasticamente aumentado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=38) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeSpDefenseStage = min(6,(activeSpDefenseStage+3))
				do getname with tag:active
				log teve sua Defesa Especial drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=39) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeSpeedStage = min(6,(activeSpeedStage+3))
				do getname with tag:active
				log teve sua Velocidade drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=40) and (chance($activeMoveFxChance)) and (enemyStatus=0) and ($activeAim = 1))
				enemyStatus = -5
				do getname with tag:enemy
				log ficou hesitante!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=41) and (chance($activeMoveFxChance)) and (enemyStatus=0) and ($activeAim = 1))
				enemyStatus = -6
				do getname with tag:enemy
				log foi forçado a fugir!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=42) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeAttackStage = min(6,(activeAttackStage+1))
				activeDefenseStage = min(6,(activeDefenseStage+1))
				activeSpAttackStage = min(6,(activeSpAttackStage+1))
				activeSpDefenseStage = min(6,(activeSpDefenseStage+1))
				activeSpeedStage = min(6,(activeSpeedStage+1))
				do getname with tag:active
				log teve todos os seus status ligeiramente aumentados!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=43) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeAccuracyStage = min(6,(activeAccuracyStage+1))
				do getname with tag:active
				log teve sua precisão ligeiramente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=44) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeAccuracyStage = min(6,(activeAccuracyStage+2))
				do getname with tag:active
				log teve sua precisão bruscamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=45) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeAccuracyStage = min(6,(activeAccuracyStage+3))
				do getname with tag:active
				log teve sua precisão drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=46) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemyEvasionStage = max((-6),(enemyEvasionStage-1))
				do getname with tag:enemy
				log teve sua evasão ligeiramente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=47) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemyEvasionStage = max((-6),(enemyEvasionStage-2))
				do getname with tag:enemy
				log teve sua evasão duramente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=48) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemyEvasionStage = max((-6),(enemyEvasionStage-3))
				do getname with tag:enemy
				log teve sua evasão severamente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=49) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemyAccuracyStage = max((-6),(enemyAccuracyStage-1))%
				do getname with tag:enemy
				log teve sua precisão ligeiramente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=50) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemyAccuracyStage = max((-6),(enemyAccuracyStage-2))
				do getname with tag:enemy
				log teve sua precisão duramente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=51) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				enemyAccuracyStage = max((-6),(enemyAccuracyStage-3))
