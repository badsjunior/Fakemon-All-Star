//-----------------------------------------------Exploração---------------------------------------------------------------//

    *TEMPLATE
		tag:explore
		on click:
			log Um pokémon selvagem apareceu na sua frente enquanto explorava [n:this]!
			lose tag:enemy
			if (have activePkm) gain inBattle
			$rand = random(2)
			if (chance(0.1%))
				toast <#ff9900><b>LENDÁRIO!</b></#>
				do legend with this
			else if (chance(1%))
				toast <#cc33ff><b>EXÓTICO!</b></#>
				do exotic with this
			else if (chance(10%))
				toast <#00cccc><b>RARO!</b></#>
				do rare with this
			else if (chance(40%))
				toast <#4dffa6><b>INCOMUM!</b></#>
				do uncommon with this
			else if ($rand=0) 
				toast <#999999><b>COMUM!</b></#>
				do common1 with this
			else if ($rand=1) 
				toast <#999999><b>COMUM!</b></#>
				do common2 with this
			else if ($rand=2) 
				toast <#999999><b>COMUM!</b></#>
				do common3 with this
			end
		end

	[include placeNoIcon %id="0" %region="0" %name="Rota 1" %desc="Rota Inicial de Pindoh!" %common1Lvl="3" %common1Range="1" %common1PkmNum="10" %common1PkmVar="0" %common2Lvl="4" %common2Range="1" %common2PkmNum="10" %common2PkmVar="0" %common3Lvl="2" %common3Range="0" %common3PkmNum="10" %common3PkmVar="0" %uncommonLvl="5" %uncommonRange="1" %uncommonPkmNum="10" %uncommonPkmVar="0" %rareLvl="7" %rareRange="2" %rarePkmNum="10" %rarePkmVar="0" %exoticLvl="8" %exoticRange="3" %exoticPkmNum="10" %exoticPkmVar="0" %legendLvl="20" %legendRange="5" %legendPkmNum="10" %legendPkmVar="0"]
    *TEMPLATE

//-----------------------------------------------Golpes-----------------------------------------------------------------//
Upgrades
    *TEMPLATE
		text:<t>[n:this]</t>
		hidden when 0
		on click:
			if (no inBattle)
				log Você não pode usar <#ffcc00><b>[n:this]</b></#> fora de batalha!
			else
				clear log
				do select with this
				do iaplay with tag:enemy
				do beginturn with global
			end
		end

