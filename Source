			if (($activeMoveEffect=54) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeEvasionStage = min(6,(activeEvasionStage+3))
				do getname with tag:active
				log teve sua evasão drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=55) and (chance($activeMoveFxChance)) and (enemyStatus=0) and ($activeAim = 1))
				enemyStatus = (-7)
				do getname with tag:enemy
				log ficou confuso!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=56) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeCriticalStage = min(6,(activeCriticalStage+1))
				do getname with tag:active
				log teve sua chance de crítico ligeiramente aumentada!
