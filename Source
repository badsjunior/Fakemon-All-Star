		end

//-------------------------------------------------------------------------------------------------//

		on activeconfusion:
			activeStatus = 0
			$activeAim = 1

			if ($activeMoveAccuracy>=0)
				$activeAim = chance(($activeMoveAccuracy*activeAccuracyMod*activeEvasionMod)*100)
			end

			if ($activeAim = 0)
				do getname with tag:active
				log errou o golpe contra si!
				activeMovesInARow = 0
			end

			if (($activeMoveID=activeLastMove) and ($activeAim = 1))
				activeMovesInARow = (activeMovesInARow+1)
			end
			if ((!($activeMoveID=activeLastMove)) and ($activeAim = 1))
				activeMovesInARow = 1
			end

			if (($activeMoveClass>=0) and ($activeAim = 1))
				do getname with tag:active
				log se atacou...
				$activeSTAB = 1
				$activeCrit = 1
				$activeStatusMod = 1
			end
			if (($activeMoveClass=1) and ($activeAim = 1))
				$activeMovePower = ($activeMovePower*(min(5,activeMovesInARow)))
				log ... com um poder acumulado de [activeMovesInARow] golpes seguidos ...
			end
			if (($activeMoveClass>=2) and ($activeAim = 1))
				$numberOfHits = random(2,$activeMoveClass)
				log ... [$numberOfHits] vezes ...
			end

			if (($activeMoveClass>=0) and (activePtype=$activeMovetype) and ($activeAim = 1))
				$activeSTAB = $activeSTAB + 0.5
				log ... o golpe foi fortalecido pelo seu tipo principal ...
			end
			if (($activeMoveClass>=0) and (activeStype=$activeMovetype) and ($activeAim = 1))
				$activeSTAB = $activeSTAB + 0.5
				log ... o golpe foi fortalecido pelo seu tipo secundário ...
			end
			if (($activeMoveClass>=0) and (chance($activeMoveCrit*pow(2,activeCriticalStage))) and ($activeAim = 1))
				$activeCrit = 1.5
				log ... e realizou um golpe crítico!
			end

			$auxMtype = $activeMoveType
			$auxType = activePtype

			[include activeMoveTypeAdvantage %type="0"]
			[include activeMoveTypeAdvantage %type="1"]
			[include activeMoveTypeAdvantage %type="2"]
			[include activeMoveTypeAdvantage %type="3"]
			[include activeMoveTypeAdvantage %type="4"]
			[include activeMoveTypeAdvantage %type="5"]
			[include activeMoveTypeAdvantage %type="6"]
			[include activeMoveTypeAdvantage %type="7"]
			[include activeMoveTypeAdvantage %type="8"]
			[include activeMoveTypeAdvantage %type="9"]
			[include activeMoveTypeAdvantage %type="10"]
			[include activeMoveTypeAdvantage %type="11"]
			[include activeMoveTypeAdvantage %type="12"]
			[include activeMoveTypeAdvantage %type="13"]
			[include activeMoveTypeAdvantage %type="14"]
			[include activeMoveTypeAdvantage %type="15"]
			[include activeMoveTypeAdvantage %type="16"]
			[include activeMoveTypeAdvantage %type="17"]

			$activeSadvg = $auxAdvg
			$activeTypeAdvg = $activePadvg*$activeSadvg

			if (activePtype=activeStype)
				$activeTypeAdvg = $activePadvg
			end

			if (($activeMoveClass>=0) and ($activeTypeAdvg=4) and ($activeAim = 1))
				log ... <#cc33ff><b>foi hiper efetivo!</b></#>
			end
			if (($activeMoveClass>=0) and ($activeTypeAdvg=2) and ($activeAim = 1))
				log ... <#4dffa6><b>foi super efetivo!</b></#>
			end
			if (($activeMoveClass>=0) and ($activeTypeAdvg=0.5) and ($activeAim = 1))
				log ... mas foi pouco efetivo!
			end
			if (($activeMoveClass>=0) and ($activeTypeAdvg=0.25) and ($activeAim = 1))
				log ... <#999999><b>mas não foi quase nada efetivo!</b></#>
			end
			if (($activeMoveClass>=0) and ($activeTypeAdvg=0) and ($activeAim = 1))
				log ... <#b30000><b>mas o golpe não afeta o adversário!</b></#>
			end

			$activeRandomVar = frandom(0.85,1)

			if ($activeMoveIsSpecial=0)
				$aA = activeAttack
				$aD = activeDefense
			end
			if ($activeMoveIsSpecial=1)
				$aA = activeSpAttack
				$aD = activeSpDefense
			end
			if (($activeMoveIsSpecial=0) and (activeStatus=3))
				$activeStatusMod = 0.5
			end

			if (($activeMoveClass>=0) and ($activeAim = 1))
				$activeRawDamage = ((((0.4*activeLvl + 2)*$activeMovePower*($aA/$aD))/50)+2)
				$activeDamageMods = $activeTypeAdvg*$activeRandomVar*$activeSTAB*$activeCrit*$activeStatusMod
				$activeEffectiveDamage = round(min(activeCurHP,($activeRawDamage*$activeDamageMods)))
				lose $activeEffectiveDamage activeCurHP
				do getname with tag:active
				log tomou [$activeEffectiveDamage] dano!
				log <#3366ff>-----------------------------------</#>
			end

			if ($activeMoveEffect=59)
				$activeMoveEffect = 25 + random(0,4)
			end
			if ($activeMoveEffect=60)
				$activeMoveEffect = 30 + random(0,4)
			end
			if ($activeMoveEffect=61)
				$activeMoveEffect = 35 + random(0,4)
			end

			if (($activeMoveEffect=1) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				$activeEffectiveHeal = ceil(min(($activeEffectiveDamage/2),(activeMaxHP-activeCurHP)))
				gain $activeEffectiveHeal activeCurHP
				do getname with tag:active
				log drenou [$activeEffectiveHeal] pontos de vida!
