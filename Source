			if (0>=enemyCurHP)
				enemyStatus=-1
				echo Attribute Update >> enemyStatus = [enemyStatus]
			end

			[include moveFxIf %caster="enemy" %number="35" %fxNum="1"]
				$reflection = 1
				echo Variable Update >> $reflection = [$reflection]
			end
			[include moveFxIf %caster="enemy" %number="35" %fxNum="2"]
				$reflection = 1
				echo Variable Update >> $reflection = [$reflection]
			end
			[include moveFxIf %caster="enemy" %number="35" %fxNum="3"]
				$reflection = 1
				echo Variable Update >> $reflection = [$reflection]
			end

			[include moveFxIf %caster="enemy" %number="36" %fxNum="1"]
				$protection = 1
				echo Variable Update >> $protection = [$protection]
			end
			[include moveFxIf %caster="enemy" %number="36" %fxNum="2"]
				$protection = 1
				echo Variable Update >> $protection = [$protection]
			end
			[include moveFxIf %caster="enemy" %number="36" %fxNum="3"]
				$protection = 1
				echo Variable Update >> $protection = [$protection]
			end

			[include moveFxIf %caster="enemy" %number="38" %fxNum="1"]
				enemyStatus=-1
				echo Attribute Update >> enemyStatus = [enemyStatus]
				do getname with tag:enemy
				log foi atingido por um golpe de 1HKO!
			end
			[include moveFxIf %caster="enemy" %number="38" %fxNum="2"]
				enemyStatus=-1
				echo Attribute Update >> enemyStatus = [enemyStatus]
				do getname with tag:enemy
				log foi atingido por um golpe de 1HKO!
			end
			[include moveFxIf %caster="enemy" %number="38" %fxNum="3"]
				enemyStatus=-1
				echo Attribute Update >> enemyStatus = [enemyStatus]
				do getname with tag:enemy
				log foi atingido por um golpe de 1HKO!
			end

			enemyLastMoveID = $enemyMoveID
			enemyLastMoveShape = $enemyMoveShape
			$enemyMoveAttempt = 1
			echo Attribute Update >> enemyLastMoveID = [enemyLastMoveID]; enemyLastMoveShape = [enemyLastMoveShape]; $enemyMoveAttempt = [$enemyMoveAttempt]

		end

//-------------------------------------------------------------------------------------------------//

		on activefaint:
			do getname with tag:active
			log está fora de batalha!
			do return with tag:active
		end
		on enemyfaint:
			do getname with tag:enemy
			log está fora de batalha!
			do faint with tag:enemy
		end

		on activeready:
			do getname with tag:active
			log usou este turno para se recuperar do esforço e agora está pronto para voltar para o combate!
			activeStatus = 0
			echo Attribute Update >> activeStatus = [activeStatus]
		end
		on enemyready:
			do getname with tag:enemy
			log usou este turno para se recuperar do esforço e agora está pronto para voltar para o combate!
			enemyStatus = 0
			echo Attribute Update >> enemyStatus = [enemyStatus]
		end

		on activefatigue:
			do getname with tag:active
			log está se recuperando do esforço que fez no último golpe!
			activeStatusTimer = (activeStatusTimer-1)
			echo Attribute Update >> activeStatusTimer = [activeStatusTimer]
		end
		on enemyfatigue:
			do getname with tag:enemy
			log está se recuperando do esforço que fez no último golpe!
			enemyStatusTimer = (enemyStatusTimer-1)
			echo Attribute Update >> enemyStatusTimer = [enemyStatusTimer]
		end

		on activeflee:
			do getname with tag:active
			log deixou a batalha!
			do return with tag:active
		end
		on enemyflee:
			do getname with tag:enemy
			log deixou a batalha!
			lose tag:enemy
		end

		on activeflinch:
			do getname with tag:active
			log hesitou em atacar!
			activeStatus = 0
			echo Attribute Update >> activeStatus = [activeStatus]
		end
		on enemyflinch:
			do getname with tag:enemy
			log hesitou em atacar!
			enemyStatus = 0
			echo Attribute Update >> enemyStatus = [enemyStatus]
		end

		on activethaw:
			do getname with tag:active
			log descongelou!
			activeStatus = 0
			echo Attribute Update >> activeStatus = [activeStatus]
		end
		on enemythaw:
			do getname with tag:enemy
			log descongelou!
			enemyStatus = 0
			echo Attribute Update >> enemyStatus = [enemyStatus]
		end

		on activefreeze:
			do getname with tag:active
			log está dentro de uma pedra de gelo!
		end
		on enemyfreeze:
			do getname with tag:enemy
			log está dentro de uma pedra de gelo!
		end

		on activewake:
			do getname with tag:active
			log acordou!
			activeStatus = 0
			echo Attribute Update >> activeStatus = [activeStatus]
		end
		on enemywake:
			do getname with tag:enemy
			log acordou!
			enemyStatus = 0
			echo Attribute Update >> enemyStatus = [enemyStatus]
		end

		on activesleep:
			do getname with tag:active
			log está dormindo!
			activeStatusTimer = (activeStatusTimer-1)
			echo Attribute Update >> activeStatusTimer = [activeStatusTimer]
		end
		on enemysleep:
			do getname with tag:enemy
			log está dormindo!
			enemyStatusTimer = (enemyStatusTimer-1)
			echo Attribute Update >> enemyStatusTimer = [enemyStatusTimer]
		end

		on activeparalysis:
			do getname with tag:active
			log não conseguiu se mover por causa da paralisia!
		end
		on enemyparalysis:
			do getname with tag:enemy
			log não conseguiu se mover por causa da paralisia!
		end

		on activepoison:
			$activePoisonDamage = round(min((activeMaxHP/8),activeCurHP))
			lose $activePoisonDamage activeCurHP
			echo Attribute Update >> activeCurHP = [activeCurHP] (It was damaged by the poison that deals [activeMaxHP/8], the activeMaxHP/8, capped at [activeCurHP], the activeCurHP)
			do getname with tag:active
			log tomou [$activePoisonDamage] dano por envenenamento!
		end
		on enemypoison:
			$enemyPoisonDamage = round(min((enemyMaxHP/8),enemyCurHP))
			lose $enemyPoisonDamage enemyCurHP
			echo Attribute Update >> enemyCurHP = [enemyCurHP] (It was damaged by the poison that deals [enemyMaxHP/8], the enemyMaxHP/8, capped at [enemyCurHP], the enemyCurHP)
			do getname with tag:enemy
			log tomou [$enemyPoisonDamage] dano por envenenamento!
		end

		on activebadpoison:
			$activePoisonDamage = round(min((activeBadPoison*activeMaxHP/16),activeCurHP))
			lose $activePoisonDamage activeCurHP
			echo Attribute Update >> activeCurHP = [activeCurHP] (It was damaged by the poison that deals [activeMaxHP/16], the activeMaxHP/16, times [activeBadPoison], the activeBadPoison, capped at [activeCurHP], the activeCurHP)
			do getname with tag:active
			log tomou [$activePoisonDamage] dano por intoxicação!
		end
		on enemybadpoison:
			$enemyPoisonDamage = round(min((enemyBadPoison*enemyMaxHP/8),enemyCurHP))
			lose $enemyPoisonDamage enemyCurHP
			echo Attribute Update >> enemyCurHP = [enemyCurHP] (It was damaged by the poison that deals [enemyMaxHP/16], the enemyMaxHP/16, times [enemyBadPoison], the enemyBadPoison, capped at [enemyCurHP], the enemyCurHP)
			do getname with tag:enemy
			log tomou [$enemyPoisonDamage] dano por intoxicação!
		end

		on activeburn:
			$activePoisonDamage = round(min((activeMaxHP/16),activeCurHP))
			lose $activePoisonDamage activeCurHP
			echo Attribute Update >> activeCurHP = [activeCurHP] (It was damaged by its burn that deals [activeMaxHP/16], the activeMaxHP/16, capped at [activeCurHP], the activeCurHP)
			do getname with tag:active
			log tomou [$activePoisonDamage] dano por queimadura!
		end
		on enemyburn:
			$enemyPoisonDamage = round(min((enemyMaxHP/16),enemyCurHP))
			lose $enemyPoisonDamage enemyCurHP
			echo Attribute Update >> enemyCurHP = [enemyCurHP] (It was damaged by its burn that deals [enemyMaxHP/16], the enemyMaxHP/16, capped at [enemyCurHP], the enemyCurHP)
			do getname with tag:enemy
			log tomou [$enemyPoisonDamage] dano por queimadura!
		end

		on endturn:
			if (activeStatus=1) do activepoison with this
			if (enemyStatus=1) do enemypoison with this
			if (activeStatus=2) do activebadpoison with this
			if (enemyStatus=2) do enemybadpoison with this
			if (activeStatus=3) do activeburn with this
			if (enemyStatus=3) do enemyburn with this
			if ((activeStatus=(-3)) and (0>=activeStatusTimer)) do activewake with this
			if ((enemyStatus=(-3)) and (0>=enemyStatusTimer)) do enemywake with this
			if ((activeStatus=(-3)) and (activeStatusTimer>0)) do activesleep with this
			if ((enemyStatus=(-3)) and (enemyStatusTimer>0)) do enemysleep with this
			if ((activeStatus=(-8)) and (0>=activeStatusTimer)) do activeready with this
			if ((enemyStatus=(-8)) and (0>=enemyStatusTimer)) do enemyready with this
			if ((activeStatus=(-8)) and (activeStatusTimer>0)) do activefatigue with this
			if ((enemyStatus=(-8)) and (enemyStatusTimer>0)) do enemyfatigue with this
			$fifth = random(1,5)
			$thaw = 0
			if ($fifth=1) $thaw=1
			if ((activeStatus=(-4)) and ($thaw=0)) do activefreeze with this
			if ((enemyStatus=(-4)) and ($thaw=0)) do enemyfreeze with this
			if ((activeStatus=(-4)) and ($thaw=1)) do activethaw with this
			if ((enemyStatus=(-4)) and ($thaw=1)) do enemythaw with this
			if (activeStatus=(-5)) do activeflinch with this
			if (enemyStatus=(-5)) do enemyflinch with this
			if (activeStatus=(-6)) do activeflee with this
			if (enemyStatus=(-6)) do enemyflee with this

			[include triggerPeriodicAbilities %caster="active" %target="enemy"]
			[include triggerPeriodicAbilities %caster="enemy" %target="active"]

			[include defModByStage %stage="(-6)" %mod="0.25"]
			[include defModByStage %stage="(-5)" %mod="0.2857"]
			[include defModByStage %stage="(-4)" %mod="0.3333"]
			[include defModByStage %stage="(-3)" %mod="0.4"]
			[include defModByStage %stage="(-2)" %mod="0.5"]
			[include defModByStage %stage="(-1)" %mod="0.6666"]
			[include defModByStage %stage="0" %mod="1"]
			[include defModByStage %stage="1" %mod="1.5"]
			[include defModByStage %stage="2" %mod="2"]
			[include defModByStage %stage="3" %mod="2.5"]
			[include defModByStage %stage="4" %mod="3"]
			[include defModByStage %stage="5" %mod="3.5"]
			[include defModByStage %stage="6" %mod="4"]

			[include defAcEvModByStage %stage="(-6)" %mod="0.3333"]
			[include defAcEvModByStage %stage="(-5)" %mod="0.375"]
			[include defAcEvModByStage %stage="(-4)" %mod="0.4286"]
			[include defAcEvModByStage %stage="(-3)" %mod="0.5"]
			[include defAcEvModByStage %stage="(-2)" %mod="0.6"]
			[include defAcEvModByStage %stage="(-1)" %mod="0.75"]
			[include defAcEvModByStage %stage="0" %mod="1"]
			[include defAcEvModByStage %stage="1" %mod="1.3333"]
			[include defAcEvModByStage %stage="2" %mod="1.6666"]
			[include defAcEvModByStage %stage="3" %mod="2"]
			[include defAcEvModByStage %stage="4" %mod="2.3333"]
			[include defAcEvModByStage %stage="5" %mod="2.6666"]
			[include defAcEvModByStage %stage="6" %mod="3"]

			if ((enemyBerry=1) and (enemyStatus=(-2))) do enemyeat with BER1
			if ((enemyBerry=2) and (enemyStatus=(-3))) do enemyeat with BER2
			if ((enemyBerry=3) and (enemyStatus=1)) do enemyeat with BER3
			if ((enemyBerry=4) and (enemyStatus=3)) do enemyeat with BER4
			if ((enemyBerry=5) and (enemyStatus=(-4))) do enemyeat with BER5
			if ((enemyBerry=6) and (enemyStatus=(-7))) do enemyeat with BER6
			if ((enemyBerry=7) and (!((enemyStatus=(-1)) or (enemyStatus=0)))) do enemyeat with BER7
			if ((enemyBerry=8) and (!(enemyStatus=(-1))) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER8
			if ((enemyBerry=9) and (!(enemyStatus=(-1))) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER9
			if ((enemyBerry=10) and (!(enemyStatus=(-1))) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER10
			if ((enemyBerry=11) and (!(enemyStatus=(-1))) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER11
			if ((enemyBerry=12) and (!(enemyStatus=(-1))) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER12
			if ((enemyBerry=13) and (!(enemyStatus=(-1))) and ((enemyMaxHP/4)>=enemyCurHP)) do enemyeat with BER13

			if ((activeStatus=(-1)) or (0>=round(activeCurHP))) do activefaint with this
			if ((enemyStatus=(-1)) or (0>=round(enemyCurHP))) do enemyfaint with this
		end

		on type0advantage:
			$auxAdvg = 1
			if (($auxType=5) or ($auxType=8)) $auxAdvg = 0.5
			if ($auxType=7) $auxAdvg = 0
		end
		on type1advantage:
			$auxAdvg = 1
			if (($auxType=0) or ($auxType=5) or ($auxType=8) or ($auxType=14) or ($auxType=16)) $auxAdvg = 2
			if (($auxType=2) or ($auxType=3) or ($auxType=6) or ($auxType=13) or ($auxType=17)) $auxAdvg = 0.5
			if ($auxType=7) $auxAdvg = 0
		end
		on type2advantage:
			$auxAdvg = 1
			if (($auxType=1) or ($auxType=6) or ($auxType=11)) $auxAdvg = 2
			if (($auxType=5) or ($auxType=8) or ($auxType=12)) $auxAdvg = 0.5
		end
		on type3advantage:
			$auxAdvg = 1
			if (($auxType=11) or ($auxType=17)) $auxAdvg = 2
			if (($auxType=3) or ($auxType=4) or ($auxType=5) or ($auxType=7)) $auxAdvg = 0.5
			if ($auxType=8) $auxAdvg = 0
		end
		on type4advantage:
			$auxAdvg = 1
			if (($auxType=3) or ($auxType=5) or ($auxType=8) or ($auxType=9) or ($auxType=12)) $auxAdvg = 2
			if (($auxType=6) or ($auxType=11)) $auxAdvg = 0.5
			if ($auxType=2) $auxAdvg = 0
		end
		on type5advantage:
			$auxAdvg = 1
			if (($auxType=2) or ($auxType=6) or ($auxType=9) or ($auxType=14)) $auxAdvg = 2
			if (($auxType=1) or ($auxType=4) or ($auxType=8)) $auxAdvg = 0.5
		end
		on type6advantage:
			$auxAdvg = 1
			if (($auxType=11) or ($auxType=13) or ($auxType=16)) $auxAdvg = 2
			if (($auxType=1) or ($auxType=2) or ($auxType=3) or ($auxType=7) or ($auxType=8) or ($auxType=9) or ($auxType=17)) $auxAdvg = 0.5
		end
		on type7advantage:
			$auxAdvg = 1
			if (($auxType=7) or ($auxType=13)) $auxAdvg = 2
			if ($auxType=16) $auxAdvg = 0.5
			if ($auxType=0) $auxAdvg = 0
		end
		on type8advantage:
			$auxAdvg = 1
			if (($auxType=5) or ($auxType=14) or ($auxType=17)) $auxAdvg = 2
			if (($auxType=8) or ($auxType=9) or ($auxType=10) or ($auxType=12)) $auxAdvg = 0.5
		end
		on type9advantage:
			$auxAdvg = 1
			if (($auxType=6) or ($auxType=8) or ($auxType=11) or ($auxType=14)) $auxAdvg = 2
			if (($auxType=5) or ($auxType=9) or ($auxType=10) or ($auxType=15)) $auxAdvg = 0.5
		end
		on type10advantage:
			$auxAdvg = 1
			if (($auxType=4) or ($auxType=5) or ($auxType=9)) $auxAdvg = 2
			if (($auxType=10) or ($auxType=11) or ($auxType=15)) $auxAdvg = 0.5
		end
		on type11advantage:
			$auxAdvg = 1
			if (($auxType=4) or ($auxType=5) or ($auxType=10)) $auxAdvg = 2
			if (($auxType=2) or ($auxType=3) or ($auxType=6) or ($auxType=8) or ($auxType=9) or ($auxType=11) or ($auxType=15)) $auxAdvg = 0.5
		end
		on type12advantage:
			$auxAdvg = 1
			if (($auxType=2) or ($auxType=10)) $auxAdvg = 2
			if (($auxType=11) or ($auxType=12) or ($auxType=15)) $auxAdvg = 0.5
			if ($auxType=4) $auxAdvg = 0
		end
		on type13advantage:
			$auxAdvg = 1
			if (($auxType=1) or ($auxType=3)) $auxAdvg = 2
			if (($auxType=8) or ($auxType=13)) $auxAdvg = 0.5
			if ($auxType=0) $auxAdvg = 0
		end
		on type14advantage:
			$auxAdvg = 1
			if (($auxType=2) or ($auxType=4) or ($auxType=11) or ($auxType=15)) $auxAdvg = 2
			if (($auxType=8) or ($auxType=9) or ($auxType=10) or ($auxType=14)) $auxAdvg = 0.5
		end
		on type15advantage:
			$auxAdvg = 1
			if ($auxType=15) $auxAdvg = 2
			if ($auxType=8) $auxAdvg = 0.5
			if ($auxType=17) $auxAdvg = 0
		end
		on type16advantage:
			$auxAdvg = 1
			if (($auxType=7) or ($auxType=13)) $auxAdvg = 2
			if (($auxType=1) or ($auxType=16) or ($auxType=17)) $auxAdvg = 0.5
		end
		on type17advantage:
			$auxAdvg = 1
			if (($auxType=1) or ($auxType=15) or ($auxType=16)) $auxAdvg = 2
			if (($auxType=3) or ($auxType=8) or ($auxType=9)) $auxAdvg = 0.5
		end
//		on type#advantage:
//			$auxAdvg = 1
//			if (($auxType=5) or ($auxType=8)) $auxAdvg = 2
//			if (($auxType=5) or ($auxType=8)) $auxAdvg = 0.5
//			if ($auxType=8) $auxAdvg = 0
//		end
			

//------------------------------------------------Basic------------------------------------------------------------------//

	*TEMPLATE
		tags:status hidden
		*activeLvl
			desc: Level do seu pokémon
		*enemyLvl
			desc: Level do pokémon inimigo
		*enemyPtype
			desc: Tipo primário do pokémon inimigo
		*enemyStype
			desc: Tipo secundário do pokémon inimigo
		*enemyCurHP
			desc: HP atual do pokémon inimigo
		*enemyMaxHP
			desc: HP máximo do pokémon inimigo
		*enemyAttack
			name:Ataque
			desc:Potencial de golpes físicos do pokémon inimigo.
			is always: (enemyBaseAttack*enemyAttackMod)
		*enemyDefense
			name:Defesa
			desc:Resistência do pokémon inimigo aos golpes físicos.
			is always: (enemyBaseDefense*enemyDefenseMod)
		*enemySpAttack
			name:Ataque Especial
			desc:Potencial de golpes especiais do pokémon inimigo.
			is always: (enemyBaseSpAttack*enemySpAttackMod)
		*enemySpDefense
			name:Defesa Especial
			desc:Resistência do pokémon inimigo aos golpes especiais.
			is always: (enemyBaseSpDefense*enemySpDefenseMod)
		*enemySpeed
			name:Velocidade
			desc:A rapidez com que o pokémon inimigo ataca, define quem ataca primeiro.
			is always: (enemyBaseSpeed*enemySpeedMod)
	*TEMPLATE
		tags:status base hidden
		on reset:
			this = 0
		end
		*activeBaseAttack
			name:Ataque (Base)
		*activeBaseDefense
			name:Defesa (Base)
		*activeBaseSpAttack
			name:Ataque Especial (Base)
		*activeBaseSpDefense
			name:Defesa Especial (Base)
		*activeBaseSpeed
			name:Velocidade (Base)
	*TEMPLATE
		tags:status baseEnemy hidden
		on reset:
			this = 0
		end
		*enemyBaseAttack
			name:Ataque (Base) Inimigo
		*enemyBaseDefense
			name:Defesa (Base) Inimigo
		*enemyBaseSpAttack
			name:Ataque Especial (Base) Inimigo
		*enemyBaseSpDefense
			name:Defesa Especial (Base) Inimigo
		*enemyBaseSpeed
			name:Velocidade (Base) Inimigo
	*TEMPLATE
		tags:status modifier hidden
		on reset:
			this = 1
		end
		*activeAttackMod
			name:Ataque (Modificador)
		*activeDefenseMod
			name:Defesa (Modificador)
		*activeSpAttackMod
			name:Ataque Especial (Modificador)
		*activeSpDefenseMod
			name:Defesa Especial (Modificador)
		*activeSpeedMod
			name:Velocidade (Modificador)
		*activeAccuracyMod
			name:Precisão (Modificador)
		*activeEvasionMod
			name:Evasão (Modificador)
	*TEMPLATE
		tags:status modifier hidden
		can be negative
		on reset:
			this = 0
		end
		*activeStatus
			desc:Condição do seu pokémon
		*activeLastMoveID
			desc:O último golpe usado pelo seu pokémon
		*activeLastMoveShape
			desc:A forma do último golpe usado pelo seu pokémon
		*activeMovesInARow
			desc:A quantidade de vezes que seu pokémon usou o mesmo golpe consecutivamente
		*activeBadPoison
			desc:O número de turnos que faz que seu pokémon foi intoxicado
		*activeStatusTimer
			desc:O número de turnos que falta para seu pokémon se livrar do status atual
		*activeAttackStage
			name:Ataque (Estágio)
		*activeDefenseStage
			name:Defesa (Estágio)
		*activeSpAttackStage
			name:Ataque Especial (Estágio)
		*activeSpDefenseStage
			name:Defesa Especial (Estágio)
		*activeSpeedStage
			name:Velocidade (Estágio)
		*activeAccuracyStage
			name:Precisão (Estágio)
		*activeEvasionStage
			name:Evasão (Modificador)
		*activeCriticalStage
			name:Crítico (Modificador)
		*activeStance
			name:Postura
		*activePosition
			name:Posição
	*TEMPLATE
		tags:status modifierEnemy hidden
		on reset:
			this = 1
		end
		*enemyAttackMod
			name:Ataque (Modificador) Inimigo
		*enemyDefenseMod
			name:Defesa (Modificador) Inimigo
		*enemySpAttackMod
			name:Ataque Especial (Modificador) Inimigo
		*enemySpDefenseMod
			name:Defesa Especial (Modificador) Inimigo
		*enemySpeedMod
			name:Velocidade (Modificador) Inimigo
		*enemyAccuracyMod
			name:Precisão (Modificador) Inimigo
		*enemyEvasionMod
			name:Evasão (Modificador) Inimigo
	*TEMPLATE
		tags:status modifierEnemy hidden
		can be negative
		on reset:
			this = 0
		end
		*enemyStatus
			desc:Condição do pokémon inimigo
		*enemyLastMoveID
			desc:O último golpe usado pelo pokémon inimigo
		*enemyLastMoveShape
			desc:A forma do último golpe usado pelo pokémon inimigo
		*enemyMovesInARow
			desc:A quantidade de vezes que o pokémon inimigo usou o mesmo golpe consecutivamente
		*enemyBadPoison
			desc:O número de turnos que faz que o pokémon inimigo foi intoxicado
		*enemyStatusTimer
			desc:O número de turnos que falta para o pokémon inimigo se livrar do status atual
		*enemyAttackStage
			name:Ataque (Estágio) Inimigo
		*enemyDefenseStage
			name:Defesa (Estágio) Inimigo
		*enemySpAttackStage
			name:Ataque Especial (Estágio) Inimigo
		*enemySpDefenseStage
			name:Defesa Especial (Estágio) Inimigo
		*enemySpeedStage
			name:Velocidade (Estágio) Inimigo
		*enemyAccuracyStage
			name:Precisão (Estágio) Inimigo
		*enemyEvasionStage
			name:Evasão (Estágio) Inimigo
		*enemyCriticalStage
			name:Crítico (Estágio) Inimigo
		*enemyStance
			name:Postura do Inimigo
		*enemyPosition
			name:Posição do Inimigo
		*enemyBerry
			name:Fruto do inimigo
		*enemyAbility
			name:Habilidade do inimigo
	*TEMPLATE
		icon class:tinyIcon
		tag:status
		*activeCurHP
			name:Pontos de Vida (HP)
			text:<t>HP: [this]/[activeMaxHP] ([((this/activeMaxHP)*100)]%)</t></#></>[?(0.15>=(this/activeMaxHP))|<#cc0000>|<#e6b800>][?((this/activeMaxHP)>=0.4)|<#009900>|][?((this/activeMaxHP)=1)|▓|░][?((this/activeMaxHP)>=0.95)|▓|░][?((this/activeMaxHP)>=0.9)|▓|░][?((this/activeMaxHP)>=0.85)|▓|░][?((this/activeMaxHP)>=0.8)|▓|░][?((this/activeMaxHP)>=0.75)|▓|░][?((this/activeMaxHP)>=0.7)|▓|░][?((this/activeMaxHP)>=0.65)|▓|░][?((this/activeMaxHP)>=0.6)|▓|░][?((this/activeMaxHP)>=0.55)|▓|░][?((this/activeMaxHP)>=0.5)|▓|░][?((this/activeMaxHP)>=0.45)|▓|░][?((this/activeMaxHP)>=0.4)|▓|░][?((this/activeMaxHP)>=0.35)|▓|░][?((this/activeMaxHP)>=0.3)|▓|░][?((this/activeMaxHP)>=0.25)|▓|░][?((this/activeMaxHP)>=0.2)|▓|░][?((this/activeMaxHP)>=0.15)|▓|░][?((this/activeMaxHP)>=0.1)|▓|░][?((this/activeMaxHP)>=0.05)|▓|░]</#><//><#ffffff>
			tags:status trickyBar
			desc:Saúde atual do seu pokémon.
			icon:https://i.imgur.com/rz2we1C.png
		*activeMaxHP
			name:Vida Máxima (max HP)
			desc:Vitalidade do seu pokémon.
			hidden
		*activeAttack
			name:Ataque
			text:[?activeAttackStage>0|<#00cc00>|][?0>activeAttackStage|<#ff3300>|]A[?(activeAttackStage=1 or activeAttackStage=(-1))|</#>|]t[?(activeAttackStage=2 or activeAttackStage=(-2))|</#>|]a[?(activeAttackStage=3 or activeAttackStage=(-3))|</#>|]q[?(activeAttackStage=4 or activeAttackStage=(-4))|</#>|]u[?(activeAttackStage=5 or activeAttackStage=(-5))|</#>|]e[?(activeAttackStage=6 or activeAttackStage=(-6))|</#>|]<t>[this]</t>
			desc:Potencial de golpes físicos do seu pokémon.
			icon:https://i.imgur.com/5hyjXAR.png
			is always: (activeBaseAttack*activeAttackMod)
		*activeDefense
			name:Defesa
			text:[?activeDefenseStage>0|<#00cc00>|][?0>activeDefenseStage|<#ff3300>|]D[?(activeDefenseStage=1 or activeDefenseStage=(-1))|</#>|]e[?(activeDefenseStage=2 or activeDefenseStage=(-2))|</#>|]f[?(activeDefenseStage=3 or activeDefenseStage=(-3))|</#>|]e[?(activeDefenseStage=4 or activeDefenseStage=(-4))|</#>|]s[?(activeDefenseStage=5 or activeDefenseStage=(-5))|</#>|]a[?(activeDefenseStage=6 or activeDefenseStage=(-6))|</#>|]<t>[this]</t>
			desc:Resistência do seu pokémon aos golpes físicos.
			icon:https://i.imgur.com/armNyK4.png
			is always: (activeBaseDefense*activeDefenseMod)
		*activeSpAttack
			name:Ataque Especial
			text:[?activeSpAttackStage>0|<#00cc00>|][?0>activeSpAttackStage|<#ff3300>|]At[?(activeSpAttackStage=1 or activeSpAttackStage=(-1))|</#>|]aq[?(activeSpAttackStage=2 or activeSpAttackStage=(-2))|</#>|]ue [?(activeSpAttackStage=3 or activeSpAttackStage=(-3))|</#>|]Es[?(activeSpAttackStage=4 or activeSpAttackStage=(-4))|</#>|]pec[?(activeSpAttackStage=5 or activeSpAttackStage=(-5))|</#>|]ial[?(activeSpAttackStage=6 or activeSpAttackStage=(-6))|</#>|]<t>[this]</t>
			desc:Potencial de golpes especiais do seu pokémon.
			icon:https://i.imgur.com/yVS3ERm.png
