			[include moveDebuffEffect %caster="enemy" %target="active" %number="10" %statusKey="Accuracy" %statusName="Precisão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="11" %statusKey="Evasion" %statusName="Evasão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="12" %statusKey="Critical" %statusName="Chance de Crítico" %gender="a" %gender2="sua"]
			[include moveAllDebuffEffect %caster="enemy" %target="active" %number="14"]
			[include moveBuffEffect %caster="enemy" %target="active" %number="15" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
