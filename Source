*include moveStageResetEffect %caster %target %number %fxNum
	if (($[%caster]MovePower[%fxNum]=1) or ($[%caster]MovePower[%fxNum]=0))
		$atkReset = 1
	end
	if (($[%caster]MovePower[%fxNum]=2) or ($[%caster]MovePower[%fxNum]=0))
		$dfsReset = 1
	end
	if (($[%caster]MovePower[%fxNum]=3) or ($[%caster]MovePower[%fxNum]=0))
		$spatkReset = 1
	end
	if (($[%caster]MovePower[%fxNum]=4) or ($[%caster]MovePower[%fxNum]=0))
		$spdfsReset = 1
	end
	if (($[%caster]MovePower[%fxNum]=5) or ($[%caster]MovePower[%fxNum]=0))
		$speedReset = 1
	end
	if (($[%caster]MovePower[%fxNum]=6) or ($[%caster]MovePower[%fxNum]=0))
		$acuReset = 1
	end
	if (($[%caster]MovePower[%fxNum]=7) or ($[%caster]MovePower[%fxNum]=0))
		$evsReset = 1
	end
	if (($[%caster]MovePower[%fxNum]=8) or ($[%caster]MovePower[%fxNum]=0))
		$crtReset = 1
	end
	if (($[%caster]MoveEffect[%fxNum]=[%number]) and (chance($[%caster]MoveFxChance[%fxNum])) and ($[%caster]FxCondSuccess[%fxNum]=1))
		[%target]AttackStage = [%target]AttackStage*(1-$atkReset)
		[%target]DefenseStage = [%target]DefenseStage*(1-$dfsReset)
		[%target]SpAttackStage = [%target]SpAttackStage*(1-$spatkReset)
		[%target]SpDefenseStage = [%target]SpDefenseStage*(1-$spdfsReset)
		[%target]SpeedStage = [%target]SpeedStage*(1-$speedReset)
		[%target]AccuracyStage = [%target]AccuracyStage*(1-$acuReset)
		[%target]EvasionStage = [%target]EvasionStage*(1-$evsReset)
		[%target]CriticalStage = [%target]CriticalStage*(1-$crtReset)
		echo Triggering $[%caster]MoveEffect[%fxNum] (#[%number]) with [$[%caster]MovePower[%fxNum]] power, ([$[%caster]MoveFxChance[%fxNum]]% chance)✔️ and condition #[$[%caster]MoveFxCondID[%fxNum]] satisfied ✔️. Attribute Update >> [%target]AttackStage = [[%target]AttackStage]; [%target]DefenseStage = [[%target]DefenseStage]; [%target]SpAttackStage = [[%target]SpAttackStage]; [%target]SpDefenseStage = [[%target]SpDefenseStage]; [%target]SpeedStage = [[%target]SpeedStage]; [%target]AccuracyStage = [[%target]AccuracyStage]; [%target]EvasionStage = [[%target]EvasionStage]; [%target]CriticalStage = [[%target]CriticalStage]
		do getname with tag:[%target]
		log teve o modificador de [?($[%caster]MovePower[%fxNum]=0)|todos os atributos|][?($[%caster]MovePower[%fxNum]=1)|Ataque|][?($[%caster]MovePower[%fxNum]=2)|Defesa|][?($[%caster]MovePower[%fxNum]=3)|Ataque Especial|][?($[%caster]MovePower[%fxNum]=4)|Defesa Especial|][?($[%caster]MovePower[%fxNum]=5)|Velocidade|][?($[%caster]MovePower[%fxNum]=6)|Precisão|][?($[%caster]MovePower[%fxNum]=7)|Evasão|][?($[%caster]MovePower[%fxNum]=8)|Chance de Crítico|] zerado! 
		log <#3366ff>-----------------------------------</#>
	end
*include moveDirectDamageEffect %caster %target %number %fxNum
	if (($[%caster]MoveEffect[%fxNum]=[%number]) and (chance($[%caster]MoveFxChance[%fxNum])) and ($[%caster]Aim = 1) and ($[%caster]FxCondSuccess[%fxNum]=1))
		$[%caster]EffectiveFxDamage = round(min([%target]CurHP,$[%caster]MoveFxPower[%fxNum]))
		echo Triggering $[%caster]MoveEffect[%fxNum] (#[%number]) with [$[%caster]MovePower[%fxNum]] power, ([$[%caster]MoveFxChance[%fxNum]]% chance)✔️, condition #[$[%caster]MoveFxCondID[%fxNum]] satisfied ✔️ and $[%caster]Aim hit ✔️. Attribute Update >> [%target]CurHP = [[%target]CurHP] ([$[%caster]MoveFxPower[%fxNum]] direct damage, capped at [[%target]CurHP], the [%target]CurHP)
		lose $[%caster]EffectiveFxDamage [%target]CurHP
		do getname with tag:[%target]
		log tomou [$[%caster]EffectiveFxDamage] dano!
		log <#3366ff>-----------------------------------</#>
	end
*include moveLoseBerryEffect %caster %target %number
	if (((($[%caster]MoveEffect1=[%number]) and (chance($[%caster]MoveFxChance1)) and ($[%caster]FxCondSuccess1=1)) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)) and ($[%caster]FxCondSuccess2=1)) or (($[%caster]MoveEffect3=[%number]) and (chance($[%caster]MoveFxChance3)) and ($[%caster]FxCondSuccess3=1))) and ($[%caster]Aim = 1))
		[%target]Berry = 0
		do getname with tag:[%target]
		log perdeu seu fruto!
		log <#3366ff>-----------------------------------</#>
	end
*include moveEatEffect %caster %target %number %berry
	if (((($[%caster]MoveEffect1=[%number]) and (chance($[%caster]MoveFxChance1)) and ($[%caster]FxCondSuccess1=1)) or (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)) and ($[%caster]FxCondSuccess2=1)) or (($[%caster]MoveEffect3=[%number]) and (chance($[%caster]MoveFxChance3)) and ($[%caster]FxCondSuccess3=1))) and ($[%caster]Aim = 1) and ([%target]Berry=[%berry]))
		 do [%caster]eat[%target] with BER[%berry]
	end
*include moveTypeChangeEffect %caster %target %number %fxNum
	if (($[%caster]MoveEffect[%fxNum]=[%number]) and ($[%caster]MoveFxPower[%fxNum]=(-1)) and ($[%caster]FxCondSuccess[%fxNum]=1))
		$[%caster]MoveFxPower = [%caster]Ptype
	end
	if (($[%caster]MoveEffect[%fxNum]=[%number]) and ($[%caster]MoveFxPower[%fxNum]=(-2)) and ($[%caster]FxCondSuccess[%fxNum]=1))
		$[%caster]MoveFxPower = [%target]Ptype
	end
	if (($[%caster]MoveEffect[%fxNum]=[%number]) and (chance($[%caster]MoveFxChance[%fxNum])) and ($[%caster]Aim = 1) and ($[%caster]FxCondSuccess[%fxNum]=1))
		[%target]Ptype = $[%caster]MoveFxPower[%fxNum]
		[%target]Stype = $[%caster]MoveFxPower[%fxNum]
		echo Triggering $[%caster]MoveEffect[%fxNum] (#[%number]) with [$[%caster]MovePower[%fxNum]] power, ([$[%caster]MoveFxChance[%fxNum]]% chance)✔️ and condition #[$[%caster]MoveFxCondID[%fxNum]] satisfied ✔️. Attribute Update >> [%target]Ptype = [[%target]Ptype]; [%target]Stype = [[%target]Stype]
		do getname with tag:[%target]
		log mudou de tipo, agora ele é [?[%target]Ptype=0|Normal|][?[%target]Ptype=1|Lutador|][?[%target]Ptype=2|Voador|][?[%target]Ptype=3|Venenoso|][?[%target]Ptype=4|Terrestre|][?[%target]Ptype=5|de Pedra|][?[%target]Ptype=6|Inseto|][?[%target]Ptype=7|Fantasma|][?[%target]Ptype=8|Metálico|][?[%target]Ptype=9|de Fogo|][?[%target]Ptype=10|Aquático|][?[%target]Ptype=11|Planta|][?[%target]Ptype=12|Elétrico|][?[%target]Ptype=13|Psíquico|][?[%target]Ptype=14|de Gelo|][?[%target]Ptype=15|Dragão|][?[%target]Ptype=16|Noturno|][?[%target]Ptype=17|Fada|]
		log <#3366ff>-----------------------------------</#>
