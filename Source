Settings
	background:http://i.imgur.com/Dnd9gLk.png
    spritesheet:pType, 128 by 128, https://i.imgur.com/1rvvX5Q.png
    spritesheet:sType, 128 by 128, https://i.imgur.com/gRRkLwO.png
    spritesheet:filter, 128 by 128, https://i.imgur.com/WUzoOZB.png
    spritesheet:exploration, 128 by 128, https://i.imgur.com/5TDMKUS.png
    spritesheet:pindohDex, 128 by 128, https://i.imgur.com/F5He5Ae.png
	stylesheet:https://pastebin.com/zcLE68K1

Layout
*wrapper
	contains:upperArea, lowerArea, leftArea, middleArea, rightArea
	*upperArea
		contains:statusDisplay
		*statusDisplay
			contains:tag:status
			class:fullWidth
	*leftArea
		contains:logDisplay
		*logDisplay
			contains:Log
			header:Log
	*middleArea
		contains:clickablesDisplay
		*clickablesDisplay
			contains:starterDisplay, activeDisplay, enemyDisplay, moveDisplay
			*starterDisplay
				contains:tag:starterChoice
			*activeDisplay
				contains:tag:active
			*enemyDisplay
				contains:tag:enemy
			*moveDisplay
				contains:tag:activeMove, tag:enemyMove
	*rightArea
		contains:exploreDisplay
		*exploreDisplay
			contains:tag:explore
			header:Exploração
			tooltip origin:left
	*lowerArea
		contains:mainMenu
			*mainMenu
				contains:navigationDisplay, contentDisplay
				class:fullWidth
				*navigationDisplay
					contains:tag:navigationButton
					costs:hide
					names:hide
				*contentDisplay
					contains:tag:foundTab, tag:capturedTab, tag:regionTab, tag:achievTab, tag:statisticTab
					costs:hide
					names:hide

//-----------------------------------------------UI Text------------------------------------------------------------------//

Upgrades
	*TEMPLATE
		class:UItext
		no tooltip
		owned	
	*foundText
		name:Pokémon Avistados
		tag:foundTab
	*capturedText
		name:Pokémon Capturados
		tag:capturedTab
	*regionText
		name:Regiões
		tag:regionTab
	*achievText
		name:Conquistas
		tag:achievTab
	*statisticText
		name:Estatísticas
		tag:statisticTab
	*TEMPLATE

//---------------------------------------------------GLOBAL---------------------------------------------------------------//

Resources
	*TEMPLATE
		tags:status hidden
	*global
		name:Global Controler and Initializer
		passive:
			gain 1 secondPlayed
			if (no inBattle)
				gain min((activeMaxHP/100),(activeMaxHP-activeCurHP)) activeCurHP
				gain 0.01 pokeball
				gain 0.025 switch
			end
		end
		on start:
			pokeball = 5
			switch = 1
			gain tag:starterFound
			show tag:foundTab
			hide tag:capturedTab
			hide tag:regionTab
			hide tag:achievTab
			hide tag:statisticTab
			log Bem vindo! Depois que <#ffcc00><b>escolher</b></#> um dos pokémon iniciais, veja seus pokémon capturados na segunda aba do <#ff33ff><b>menu</b></#> abaixo (ícone de pokébola). <#ffcc00><b>Clique</b></#> em um de seus pokémon capturados para chamá-lo para batalhar. Isto irá gastar um ponto de <u>troca</u>. <#ffcc00><b>Selecione</b></#> uma área no <#ff33ff><b>menu de exploração</b></#> à direita para encontrar pokémon selvagens. <#ffcc00><b>Clique</b></#> em um pokémon selvagem para tentar <#ffcc00><b>capturá-lo</b></#>. Isto irá gastar uma <u>pokébola</u>. Te dei 5 <u>pokébolas</u> de presente, não as desperdice! É muito mais fácil <#ffcc00><b>capturar</b></#> um pokémon quando ele está enfraquecido. Enquanto estiver fora de batalha, seu pokémon irá regenerar pontos de <u>vida</u> e você irá conseguir mais <u>pokébolas</u> e pontos de <u>troca</u> com o tempo.
		end

		on beginturn:
			$quarter=random(1,4)
			$paralysis=0
			$confusion=0
			if ($quarter=1)
				$paralysis=1
				$confusion=1
			end
			$equarter=random(1,4)
			$eparalysis=0
			$econfusion=0
			if ($equarter=1)
				$eparalysis=1
				$econfusion=1
			end
			log <#3366ff>-----------------------------------</#>
			[include checkMoveFirst]
			[include checkMoveFirstUnderStatus %statusID="2" %statusName="paralysis"]
			[include checkMoveFirstUnderStatus %statusID="7" %statusName="confusion"]

			if (enemyStatus>=0) do enemymove with this
			[include checkEnemyMoveUnderStatus %statusID="2" %statusName="paralysis"]
			[include checkEnemyMoveUnderStatus %statusID="7" %statusName="confusion"]

			[include checkMoveSecond]
			[include checkMoveSecondUnderStatus %statusID="2" %statusName="paralysis"]
			[include checkMoveSecondUnderStatus %statusID="7" %statusName="confusion"]

			do endturn with this
		end

		on activemove:
			[include checkFxConditions %caster="active" %target="enemy"]
			[include moveFullCopyEffect %caster="active" %target="enemy" %number="31"]
			[include moveFxCopyEffect %caster="active" %target="enemy" %number="32"]
			[include onMove %caster="active" %target="enemy"]
			[include activeMoveTypeAdvantage %type="0"]
			[include activeMoveTypeAdvantage %type="1"]
			[include activeMoveTypeAdvantage %type="2"]
			[include activeMoveTypeAdvantage %type="3"]
			[include activeMoveTypeAdvantage %type="4"]
			[include activeMoveTypeAdvantage %type="5"]
			[include activeMoveTypeAdvantage %type="6"]
			[include activeMoveTypeAdvantage %type="7"]
			[include activeMoveTypeAdvantage %type="8"]
			[include activeMoveTypeAdvantage %type="9"]
			[include activeMoveTypeAdvantage %type="10"]
			[include activeMoveTypeAdvantage %type="11"]
			[include activeMoveTypeAdvantage %type="12"]
			[include activeMoveTypeAdvantage %type="13"]
			[include activeMoveTypeAdvantage %type="14"]
			[include activeMoveTypeAdvantage %type="15"]
			[include activeMoveTypeAdvantage %type="16"]
			[include activeMoveTypeAdvantage %type="17"]
			[include effectivenessVerification %caster="active" %target="enemy"]
			[include damageCalculation %caster="active" %target="enemy"]
			if (($activeMoveEffect=13) or ($activeMoveEffect=23))
				$activeMoveEffect = $activeMoveEffect - random(1,8)
			end
			if (($activeMoveEffect=-13) or ($activeMoveEffect=-23))
				$activeMoveEffect = $activeMoveEffect + random(1,8)
			end
			[include moveDrainEffect %caster="active" %number="1"]
			[include moveRecoilEffect %caster="active" %number="2"]
			[include moveStatusEffect %caster="active" %target="enemy" %number="3"]
			[include moveHealStatusEffect %caster="active" %target="enemy" %number="4"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="5" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="6" %statusKey="Defense" %statusName="Defesa" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="7" %statusKey="SpAttack" %statusName="Ataque Especial" %gender="o" %gender2="seu"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="8" %statusKey="SpDefense" %statusName="Defesa Especial" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="9" %statusKey="Speed" %statusName="Velocidade" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="10" %statusKey="Accuracy" %statusName="Precisão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="11" %statusKey="Evasion" %statusName="Evasão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="enemy" %number="12" %statusKey="Critical" %statusName="Chance de Crítico" %gender="a" %gender2="sua"]
			[include moveAllDebuffEffect %caster="active" %target="enemy" %number="14"]
			[include moveBuffEffect %caster="active" %target="enemy" %number="15" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
			[include moveBuffEffect %caster="active" %target="enemy" %number="16" %statusKey="Defense" %statusName="Defesa" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="enemy" %number="17" %statusKey="SpAttack" %statusName="Ataque Especial" %gender="o" %gender2="seu"]
			[include moveBuffEffect %caster="active" %target="enemy" %number="18" %statusKey="SpDefense" %statusName="Defesa Especial" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="enemy" %number="19" %statusKey="Speed" %statusName="Velocidade" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="enemy" %number="20" %statusKey="Accuracy" %statusName="Precisão" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="enemy" %number="21" %statusKey="Evasion" %statusName="Evasão" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="enemy" %number="22" %statusKey="Critical" %statusName="Chance de Crítico" %gender="a" %gender2="sua"]
			[include moveAllBuffEffect %caster="active" %target="enemy" %number="24"]
			[include moveStageResetEffect %caster="active" %target="enemy" %number="25"]
			[include moveLoseBerryEffect %caster="active" %target="enemy" %number="26"]
			[include moveEatEffect %caster="active" %target="enemy" %number="27"]
			[include moveTypeChangeEffect %caster="active" %target="enemy" %number="28"]
			[include moveAbilityChangeEffect %caster="active" %target="enemy" %number="29"]
			[include moveDirectDamageEffect %caster="active" %target="enemy" %number="30"]
			[include moveHealStatusEffect %caster="active" %target="active" %number="-4"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-5" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-6" %statusKey="Defense" %statusName="Defesa" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-7" %statusKey="SpAttack" %statusName="Ataque Especial" %gender="o" %gender2="seu"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-8" %statusKey="SpDefense" %statusName="Defesa Especial" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-9" %statusKey="Speed" %statusName="Velocidade" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-10" %statusKey="Accuracy" %statusName="Precisão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-11" %statusKey="Evasion" %statusName="Evasão" %gender="a" %gender2="sua"]
			[include moveDebuffEffect %caster="active" %target="active" %number="-12" %statusKey="Critical" %statusName="Chance de Crítico" %gender="a" %gender2="sua"]
			[include moveAllDebuffEffect %caster="active" %target="active" %number="-14"]
			[include moveBuffEffect %caster="active" %target="active" %number="-15" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
			[include moveBuffEffect %caster="active" %target="active" %number="-16" %statusKey="Defense" %statusName="Defesa" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="-17" %statusKey="SpAttack" %statusName="Ataque Especial" %gender="o" %gender2="seu"]
			[include moveBuffEffect %caster="active" %target="active" %number="-18" %statusKey="SpDefense" %statusName="Defesa Especial" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="-19" %statusKey="Speed" %statusName="Velocidade" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="-20" %statusKey="Accuracy" %statusName="Precisão" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="-21" %statusKey="Evasion" %statusName="Evasão" %gender="a" %gender2="sua"]
			[include moveBuffEffect %caster="active" %target="active" %number="-22" %statusKey="Critical" %statusName="Chance de Crítico" %gender="a" %gender2="sua"]
			[include moveAllBuffEffect %caster="active" %target="active" %number="-24"]
			[include moveStageResetEffect %caster="active" %target="active" %number="-25"]
			[include moveLoseBerryEffect %caster="active" %target="active" %number="-26"]
			[include moveEatEffect %caster="active" %target="active" %number="-27"]
			[include moveTypeChangeEffect %caster="active" %target="active" %number="-28"]
			[include moveAbilityChangeEffect %caster="active" %target="active" %number="-29"]
			[include moveDirectDamageEffect %caster="active" %target="active" %number="-30"]

			[include multipleHit %caster="active" %target="enemy" %hit="2"]
			[include multipleHit %caster="active" %target="enemy" %hit="3"]
			[include multipleHit %caster="active" %target="enemy" %hit="4"]
			[include multipleHit %caster="active" %target="enemy" %hit="5"]

			if (0>=enemyCurHP)
				enemyStatus=-1
			end

			activeLastMove = $activeMoveID
		end

//-------------------------------------------------------------------------------------------------//

		on enemymove:
			[include checkFxConditions %caster="enemy" %target="active"]
			[include moveFullCopyEffect %caster="enemy" %target="active" %number="31"]
			[include moveFxCopyEffect %caster="enemy" %target="active" %number="32"]
			[include onMove %caster="enemy" %target="active"]
			[include enemyMoveTypeAdvantage %type="0"]
			[include enemyMoveTypeAdvantage %type="1"]
			[include enemyMoveTypeAdvantage %type="2"]
			[include enemyMoveTypeAdvantage %type="3"]
			[include enemyMoveTypeAdvantage %type="4"]
			[include enemyMoveTypeAdvantage %type="5"]
			[include enemyMoveTypeAdvantage %type="6"]
			[include enemyMoveTypeAdvantage %type="7"]
			[include enemyMoveTypeAdvantage %type="8"]
			[include enemyMoveTypeAdvantage %type="9"]
			[include enemyMoveTypeAdvantage %type="10"]
			[include enemyMoveTypeAdvantage %type="11"]
			[include enemyMoveTypeAdvantage %type="12"]
			[include enemyMoveTypeAdvantage %type="13"]
			[include enemyMoveTypeAdvantage %type="14"]
			[include enemyMoveTypeAdvantage %type="15"]
			[include enemyMoveTypeAdvantage %type="16"]
			[include enemyMoveTypeAdvantage %type="17"]
			[include effectivenessVerification %caster="enemy" %target="active"]
			[include damageCalculation %caster="enemy" %target="active"]
			if (($enemyMoveEffect=13) or ($enemyMoveEffect=23))
				$enemyMoveEffect = $enemyMoveEffect - random(1,8)
			end
			if (($enemyMoveEffect=-13) or ($enemyMoveEffect=-23))
				$enemyMoveEffect = $enemyMoveEffect + random(1,8)
			end
			[include moveDrainEffect %caster="enemy" %number="1"]
			[include moveRecoilEffect %caster="enemy" %number="2"]
			[include moveStatusEffect %caster="enemy" %target="active" %number="3"]
			[include moveHealStatusEffect %caster="enemy" %target="active" %number="4"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="5" %statusKey="Attack" %statusName="Ataque" %gender="o" %gender2="seu"]
			[include moveDebuffEffect %caster="enemy" %target="active" %number="6" %statusKey="Defense" %statusName="Defesa" %gender="a" %gender2="sua"]
