		name:Balldillo
		tags:foundTab starterFound
		desc:Inicial de <b>Pindoh</b>.
		icon:pindohDex[4,0]
	*FND7from0var0
		name:Gossipavy
		tags:foundTab starterFound
		desc:Inicial de <b>Pindoh</b>.
		icon:pindohDex[7,0]
