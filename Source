*include moveStageResetEffect %caster %target %number
	if (($[%caster]MovePower=1) or ($[%caster]MovePower=0))
		$atkReset = 1
	end
	if (($[%caster]MovePower=2) or ($[%caster]MovePower=0))
		$dfsReset = 1
	end
	if (($[%caster]MovePower=3) or ($[%caster]MovePower=0))
		$spatkReset = 1
	end
	if (($[%caster]MovePower=4) or ($[%caster]MovePower=0))
		$spdfsReset = 1
	end
	if (($[%caster]MovePower=5) or ($[%caster]MovePower=0))
		$speedReset = 1
	end
	if (($[%caster]MovePower=6) or ($[%caster]MovePower=0))
		$acuReset = 1
	end
	if (($[%caster]MovePower=7) or ($[%caster]MovePower=0))
		$evsReset = 1
	end
	if (($[%caster]MovePower=8) or ($[%caster]MovePower=0))
		$crtReset = 1
	end
	if (($[%caster]MoveEffect=[%number]) and (chance($[%caster]MoveFxChance)) and ($[%caster]Aim = 1))
		[%target]AttackStage = [%target]AttackStage*(1-$atkReset)
		[%target]DefenseStage = [%target]DefenseStage*(1-$dfsReset)
		[%target]SpAttackStage = [%target]SpAttackStage*(1-$spatkReset)
		[%target]SpDefenseStage = [%target]SpDefenseStage*(1-$spdfsReset)
		[%target]SpeedStage = [%target]SpeedStage*(1-$speedReset)
		[%target]AccuracyStage = [%target]AccuracyStage*(1-$acuReset)
		[%target]EvasionStage = [%target]EvasionStage*(1-$evsReset)
		[%target]CriticalStage = [%target]CriticalStage*(1-$crtReset)
		do getname with tag:[%target]
		log teve o modificador de [?($[%caster]MovePower=0)|todos os atributos|][?($[%caster]MovePower=1)|Ataque|][?($[%caster]MovePower=2)|Defesa|][?($[%caster]MovePower=3)|Ataque Especial|][?($[%caster]MovePower=4)|Defesa Especial|][?($[%caster]MovePower=5)|Velocidade|][?($[%caster]MovePower=6)|Precisão|][?($[%caster]MovePower=7)|Evasão|][?($[%caster]MovePower=8)|Chance de Crítico|] zerado! 
		log <#3366ff>-----------------------------------</#>
	end
	if (($[%caster]MovePower2=1) or ($[%caster]MovePower2=0))
		$atkReset2 = 1
	end
	if (($[%caster]MovePower2=2) or ($[%caster]MovePower2=0))
		$dfsReset2 = 1
	end
	if (($[%caster]MovePower2=3) or ($[%caster]MovePower2=0))
		$spatkReset2 = 1
	end
	if (($[%caster]MovePower2=4) or ($[%caster]MovePower2=0))
		$spdfsReset2 = 1
	end
	if (($[%caster]MovePower2=5) or ($[%caster]MovePower2=0))
		$speedReset2 = 1
	end
	if (($[%caster]MovePower2=6) or ($[%caster]MovePower2=0))
		$acuReset2 = 1
	end
	if (($[%caster]MovePower2=7) or ($[%caster]MovePower2=0))
		$evsReset2 = 1
	end
	if (($[%caster]MovePower2=8) or ($[%caster]MovePower2=0))
		$crtReset2 = 1
	end
	if (($[%caster]MoveEffect2=[%number]) and (chance($[%caster]MoveFxChance2)) and ($[%caster]Aim = 1))
		[%target]AttackStage = [%target]AttackStage*(1-$atkReset2)
		[%target]DefenseStage = [%target]DefenseStage*(1-$dfsReset2)
		[%target]SpAttackStage = [%target]SpAttackStage*(1-$spatkReset2)
		[%target]SpDefenseStage = [%target]SpDefenseStage*(1-$spdfsReset2)
		[%target]SpeedStage = [%target]SpeedStage*(1-$speedReset2)
		[%target]AccuracyStage = [%target]AccuracyStage*(1-$acuReset2)
		[%target]EvasionStage = [%target]EvasionStage*(1-$evsReset2)
