				do getname with tag:active
				log teve seu Ataque Especial drasticamente aumentado!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=38) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeSpDefenseStage = min(6,(activeSpDefenseStage+3))
				do getname with tag:active
				log teve sua Defesa Especial drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=39) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeSpeedStage = min(6,(activeSpeedStage+3))
				do getname with tag:active
				log teve sua Velocidade drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=40) and (chance($activeMoveFxChance)) and (activeStatus=0) and ($activeAim = 1))
				activeStatus = -5
				do getname with tag:active
				log ficou hesitante!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=41) and (chance($activeMoveFxChance)) and (activeStatus=0) and ($activeAim = 1))
				activeStatus = -6
				do getname with tag:active
				log foi forçado a fugir!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=42) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeAttackStage = min(6,(activeAttackStage+1))
				activeDefenseStage = min(6,(activeDefenseStage+1))
				activeSpAttackStage = min(6,(activeSpAttackStage+1))
				activeSpDefenseStage = min(6,(activeSpDefenseStage+1))
				activeSpeedStage = min(6,(activeSpeedStage+1))
				do getname with tag:active
				log teve todos os seus status ligeiramente aumentados!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=43) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeAccuracyStage = min(6,(activeAccuracyStage+1))
				do getname with tag:active
				log teve sua precisão ligeiramente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=44) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeAccuracyStage = min(6,(activeAccuracyStage+2))
				do getname with tag:active
				log teve sua precisão bruscamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=45) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeAccuracyStage = min(6,(activeAccuracyStage+3))
				do getname with tag:active
				log teve sua precisão drasticamente aumentada!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=46) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeEvasionStage = max((-6),(activeEvasionStage-1))
				do getname with tag:active
				log teve sua evasão ligeiramente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=47) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeEvasionStage = max((-6),(activeEvasionStage-2))
				do getname with tag:active
				log teve sua evasão duramente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=48) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeEvasionStage = max((-6),(activeEvasionStage-3))
				do getname with tag:active
				log teve sua evasão severamente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=49) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeAccuracyStage = max((-6),(activeAccuracyStage-1))%
				do getname with tag:active
				log teve sua precisão ligeiramente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=50) and (chance($activeMoveFxChance)) and ($activeAim = 1))
				activeAccuracyStage = max((-6),(activeAccuracyStage-2))
				do getname with tag:active
				log teve sua precisão bruscamente diminuída!
				log <#3366ff>-----------------------------------</#>
			end
			if (($activeMoveEffect=51) and (chance($activeMoveFxChance)) and ($activeAim = 1))
